{
  "livro": "Sofonias",
  "capítulo": 3,
  "versículos": {
    "1": {
      "título": "Ameaças contra Jerusalém",
      "texto": "Ai da cidade opressora,"
    },
    "2": {
      "título": "",
      "texto": "Não atende ninguém,"
    },
    "3": {
      "título": "",
      "texto": "Os seus príncipes"
    },
    "4": {
      "título": "",
      "texto": "Os seus profetas"
    },
    "5": {
      "título": "",
      "texto": "O Senhor é justo,"
    },
    "6": {
      "título": "",
      "texto": "“Exterminei nações."
    },
    "7": {
      "título": "",
      "texto": "Eu dizia:"
    },
    "8": {
      "título": "A salvação da filha de Jerusalém",
      "texto": "“Portanto, esperem por mim”,"
    },
    "9": {
      "título": "",
      "texto": "“Então darei lábios puros"
    },
    "10": {
      "título": "",
      "texto": "Dalém dos rios da Etiópia,"
    },
    "11": {
      "título": "",
      "texto": "Naquele dia, você não terá"
    },
    "12": {
      "título": "",
      "texto": "Mas deixarei no meio de você"
    },
    "13": {
      "título": "",
      "texto": "O remanescente de Israel"
    },
    "14": {
      "título": "",
      "texto": "Cante, ó filha de Sião!"
    },
    "15": {
      "título": "",
      "texto": "O Senhor retirou as sentenças"
    },
    "16": {
      "título": "",
      "texto": "Naquele dia, se dirá a Jerusalém:"
    },
    "17": {
      "título": "",
      "texto": "O Senhor , seu Deus,"
    },
    "18": {
      "título": "",
      "texto": "“Congregarei"
    },
    "19": {
      "título": "",
      "texto": "Eis que, naquele tempo,"
    },
    "20": {
      "título": "",
      "texto": "Naquele tempo,"
    }
  }
}