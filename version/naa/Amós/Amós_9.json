{
  "livro": "Amós",
  "capítulo": 9,
  "versículos": {
    "1": {
      "título": "Os juízos de Deus são inevitáveis",
      "texto": "Vi o Senhor, que estava em pé junto ao altar. Ele me disse:"
    },
    "2": {
      "título": "",
      "texto": "Ainda que cavem para chegar"
    },
    "3": {
      "título": "",
      "texto": "Se eles se esconderem"
    },
    "4": {
      "título": "",
      "texto": "Se forem levados para o cativeiro"
    },
    "5": {
      "título": "",
      "texto": "Porque o Senhor,"
    },
    "6": {
      "título": "",
      "texto": "Deus é o que edifica"
    },
    "7": {
      "título": "",
      "texto": "“Filhos de Israel,"
    },
    "8": {
      "título": "",
      "texto": "Eis que os olhos do Senhor Deus"
    },
    "9": {
      "título": "",
      "texto": "“Porque eis que darei ordens"
    },
    "10": {
      "título": "",
      "texto": "Todos os pecadores"
    },
    "11": {
      "título": "Restauração do Israel espiritual",
      "texto": "“Naquele dia, levantarei"
    },
    "12": {
      "título": "",
      "texto": "para que o meu povo tome posse"
    },
    "13": {
      "título": "",
      "texto": "“Eis que vêm dias”,"
    },
    "14": {
      "título": "",
      "texto": "Mudarei a sorte"
    },
    "15": {
      "título": "",
      "texto": "Eu os plantarei na sua terra,"
    }
  }
}