{
  "livro": "Habacuque",
  "capítulo": 3,
  "versículos": {
    "1": {
      "título": "A oração de Habacuque",
      "texto": "Oração do profeta Habacuque sob a forma de canto."
    },
    "2": {
      "título": "",
      "texto": "Senhor , tenho ouvido a tua fama,"
    },
    "3": {
      "título": "",
      "texto": "Deus vem de Temã,"
    },
    "4": {
      "título": "",
      "texto": "O seu resplendor é como a luz,"
    },
    "5": {
      "título": "",
      "texto": "Adiante dele vai a peste,"
    },
    "6": {
      "título": "",
      "texto": "Ele para e faz a terra tremer;"
    },
    "7": {
      "título": "",
      "texto": "Vejo as tendas de Cusã"
    },
    "8": {
      "título": "",
      "texto": "Acaso é contra os rios, Senhor ,"
    },
    "9": {
      "título": "",
      "texto": "Preparas o teu arco;"
    },
    "10": {
      "título": "",
      "texto": "Os montes te veem"
    },
    "11": {
      "título": "",
      "texto": "O sol e a lua param"
    },
    "12": {
      "título": "",
      "texto": "Na tua indignação,"
    },
    "13": {
      "título": "",
      "texto": "Tu sais para salvar o teu povo,"
    },
    "14": {
      "título": "",
      "texto": "Traspassas a cabeça"
    },
    "15": {
      "título": "",
      "texto": "Marchas com os teus cavalos"
    },
    "16": {
      "título": "",
      "texto": "Ouvi isso, e o meu íntimo"
    },
    "17": {
      "título": "",
      "texto": "Ainda que a figueira"
    },
    "18": {
      "título": "",
      "texto": "mesmo assim"
    },
    "19": {
      "título": "",
      "texto": "Deus , o Senhor,"
    }
  }
}