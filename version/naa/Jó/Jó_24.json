{
  "livro": "Jó",
  "capítulo": 24,
  "versículos": {
    "1": {
      "título": "Os maus roubam",
      "texto": "“Por que o Todo-Poderoso"
    },
    "2": {
      "título": "",
      "texto": "Há os que removem"
    },
    "3": {
      "título": "",
      "texto": "Levam o jumento"
    },
    "4": {
      "título": "",
      "texto": "Desviam do caminho"
    },
    "5": {
      "título": "Os pobres são explorados pelos maus",
      "texto": "“Como jumentos selvagens"
    },
    "6": {
      "título": "",
      "texto": "Cortam o seu pasto no campo,"
    },
    "7": {
      "título": "",
      "texto": "Passam a noite nus"
    },
    "8": {
      "título": "",
      "texto": "São encharcados"
    },
    "9": {
      "título": "",
      "texto": "Orfãozinhos são arrancados"
    },
    "10": {
      "título": "",
      "texto": "Os pobres andam nus,"
    },
    "11": {
      "título": "",
      "texto": "Entre os muros desses perversos"
    },
    "12": {
      "título": "",
      "texto": "Desde as cidades gemem"
    },
    "13": {
      "título": "Perversos, assassinos, adúlteros, ladrões",
      "texto": "“Os perversos são"
    },
    "14": {
      "título": "",
      "texto": "O assassino se levanta"
    },
    "15": {
      "título": "",
      "texto": "O olho do adúltero"
    },
    "16": {
      "título": "",
      "texto": "Nas trevas,"
    },
    "17": {
      "título": "",
      "texto": "Pois a manhã é para todos eles"
    },
    "18": {
      "título": "Deus atenta ao perverso",
      "texto": "“Os perversos são levados"
    },
    "19": {
      "título": "",
      "texto": "A seca e o calor"
    },
    "20": {
      "título": "",
      "texto": "A mãe se esquecerá deles,"
    },
    "21": {
      "título": "",
      "texto": "Maltratam as estéreis,"
    },
    "22": {
      "título": "",
      "texto": "Mas Deus, por sua força,"
    },
    "23": {
      "título": "",
      "texto": "Ele lhes dá descanso,"
    },
    "24": {
      "título": "",
      "texto": "São exaltados por breve tempo;"
    },
    "25": {
      "título": "",
      "texto": "Se não é assim,"
    }
  }
