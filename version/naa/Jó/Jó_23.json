{
  "livro": "Jó",
  "capítulo": 23,
  "versículos": {
    "1": {
      "título": "Quem dera eu soubesse onde encontrar Deus!",
      "texto": "Então Jó respondeu:"
    },
    "2": {
      "título": "",
      "texto": "“Ainda hoje a minha queixa"
    },
    "3": {
      "título": "",
      "texto": "Quem dera eu soubesse"
    },
    "4": {
      "título": "",
      "texto": "Exporia diante dele"
    },
    "5": {
      "título": "",
      "texto": "Saberia com que palavras"
    },
    "6": {
      "título": "",
      "texto": "Será que ele discutiria comigo,"
    },
    "7": {
      "título": "",
      "texto": "Ali, o homem reto apresentaria"
    },
    "8": {
      "título": "Deus sabe o meu caminho",
      "texto": "“Se me adianto, Deus não está ali;"
    },
    "9": {
      "título": "",
      "texto": "Se ele age à minha esquerda,"
    },
    "10": {
      "título": "",
      "texto": "Mas ele sabe o meu caminho;"
    },
    "11": {
      "título": "",
      "texto": "Os meus pés seguiram"
    },
    "12": {
      "título": "",
      "texto": "Do mandamento dos seus lábios"
    },
    "13": {
      "título": "Deus cumprirá o que está ordenado a meu respeito",
      "texto": "“Mas, se Deus resolveu"
    },
    "14": {
      "título": "",
      "texto": "Pois ele cumprirá"
    },
    "15": {
      "título": "",
      "texto": "Por isso, fico apavorado"
    },
    "16": {
      "título": "",
      "texto": "Deus é quem fez"
    },
    "17": {
      "título": "",
      "texto": "Porque não estou desfalecido"
    }
  }
}