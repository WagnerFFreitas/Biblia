{
  "livro": "Jó",
  "capítulo": 36,
  "versículos": {
    "1": {
      "título": "Caps.",
      "texto": "Eliú seguiu falando e disse:"
    },
    "2": {
      "título": "",
      "texto": "“Mais um pouco de paciência,"
    },
    "3": {
      "título": "",
      "texto": "Trarei o meu conhecimento"
    },
    "4": {
      "título": "",
      "texto": "Porque, na verdade,"
    },
    "5": {
      "título": "Deus é grande e não despreza ninguém",
      "texto": "“Eis que Deus é grande"
    },
    "6": {
      "título": "",
      "texto": "Não poupa a vida do ímpio,"
    },
    "7": {
      "título": "",
      "texto": "Deus não tira os seus olhos"
    },
    "8": {
      "título": "",
      "texto": "Se estão presos com correntes"
    },
    "9": {
      "título": "",
      "texto": "ele lhes faz ver as suas obras,"
    },
    "10": {
      "título": "",
      "texto": "Abre-lhes também os ouvidos"
    },
    "11": {
      "título": "",
      "texto": "Se o ouvirem e o servirem,"
    },
    "12": {
      "título": "",
      "texto": "Porém, se não o ouvirem,"
    },
    "13": {
      "título": "",
      "texto": "“Os ímpios de coração"
    },
    "14": {
      "título": "",
      "texto": "Perdem a vida na sua mocidade"
    },
    "15": {
      "título": "",
      "texto": "Deus livra o aflito"
    },
    "16": {
      "título": "Não se incline para a iniquidade",
      "texto": "“Assim também a você"
    },
    "17": {
      "título": "",
      "texto": "Mas você se enche"
    },
    "18": {
      "título": "",
      "texto": "Tenha cuidado para que"
    },
    "19": {
      "título": "",
      "texto": "Será que ele levaria em conta"
    },
    "20": {
      "título": "",
      "texto": "Não suspire pela noite,"
    },
    "21": {
      "título": "",
      "texto": "Cuidado! Não se incline"
    },
    "22": {
      "título": "Deus se mostra grande em seu poder!",
      "texto": "“Eis que Deus se mostra grande"
    },
    "23": {
      "título": "",
      "texto": "Quem lhe prescreveu"
    },
    "24": {
      "título": "",
      "texto": "Lembre-se de exaltar"
    },
    "25": {
      "título": "",
      "texto": "Toda a humanidade"
    },
    "26": {
      "título": "",
      "texto": "Eis que Deus é grande,"
    },
    "27": {
      "título": "O poder e a majestade de Deus",
      "texto": "“Ele atrai para si"
    },
    "28": {
      "título": "",
      "texto": "a qual as nuvens derramam"
    },
    "29": {
      "título": "",
      "texto": "Pode alguém entender"
    },
    "30": {
      "título": "",
      "texto": "Eis que ele espalha sobre elas"
    },
    "31": {
      "título": "",
      "texto": "Pois por estas coisas"
    },
    "32": {
      "título": "",
      "texto": "Enche as mãos de relâmpagos"
    },
    "33": {
      "título": "",
      "texto": "O fragor da tempestade"
    }
  }
}