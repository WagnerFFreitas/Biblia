{
  "livro": "Jó",
  "capítulo": 10,
  "versículos": {
    "1": {
      "título": "Darei livre curso à minha queixa",
      "texto": "“Estou cansado de viver."
    },
    "2": {
      "título": "",
      "texto": "Pedirei a Deus:"
    },
    "3": {
      "título": "",
      "texto": "Será que tens prazer"
    },
    "4": {
      "título": "",
      "texto": "Por acaso, tens olhos de gente?"
    },
    "5": {
      "título": "",
      "texto": "São os teus dias"
    },
    "6": {
      "título": "",
      "texto": "para te informares"
    },
    "7": {
      "título": "",
      "texto": "Bem sabes"
    },
    "8": {
      "título": "Vais reduzir-me a pó?",
      "texto": "“As tuas mãos me plasmaram"
    },
    "9": {
      "título": "",
      "texto": "Lembra-te de que me formaste"
    },
    "10": {
      "título": "",
      "texto": "Por acaso, não me derramaste"
    },
    "11": {
      "título": "",
      "texto": "De pele e carne me vestiste"
    },
    "12": {
      "título": "",
      "texto": "Tu me deste vida e bondade,"
    },
    "13": {
      "título": "",
      "texto": "Mas ocultaste estas coisas"
    },
    "14": {
      "título": "",
      "texto": "Se eu pecar, tu me observas;"
    },
    "15": {
      "título": "",
      "texto": "Se for iníquo, ai de mim!"
    },
    "16": {
      "título": "",
      "texto": "Porque, se levanto a cabeça,"
    },
    "17": {
      "título": "",
      "texto": "Renovas contra mim"
    },
    "18": {
      "título": "Ó Deus, deixa-me em paz!",
      "texto": "“Por que me tiraste"
    },
    "19": {
      "título": "",
      "texto": "Teria sido como alguém"
    },
    "20": {
      "título": "",
      "texto": "Não são poucos os meus dias?"
    },
    "21": {
      "título": "",
      "texto": "antes que eu vá para o lugar"
    },
    "22": {
      "título": "",
      "texto": "terra de escuridão,"
    }
  }
}