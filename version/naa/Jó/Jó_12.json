{
  "livro": "Jó",
  "capítulo": 12,
  "versículos": {
    "1": {
      "título": "Eu também tenho entendimento",
      "texto": "Então Jó respondeu:"
    },
    "2": {
      "título": "",
      "texto": "“Na verdade, vocês são o povo,"
    },
    "3": {
      "título": "",
      "texto": "Mas eu também tenho"
    },
    "4": {
      "título": "",
      "texto": "Eu sou motivo de riso"
    },
    "5": {
      "título": "",
      "texto": "No pensamento"
    },
    "6": {
      "título": "",
      "texto": "Os opressores têm paz"
    },
    "7": {
      "título": "Os animais o ensinarão",
      "texto": "“Mas pergunte agora aos animais,"
    },
    "8": {
      "título": "",
      "texto": "Ou fale com a terra,"
    },
    "9": {
      "título": "",
      "texto": "De todos estes, quem não sabe"
    },
    "10": {
      "título": "",
      "texto": "Na sua mão está a vida"
    },
    "11": {
      "título": "",
      "texto": "Por acaso, o ouvido"
    },
    "12": {
      "título": "Com Deus estão a sabedoria e a força",
      "texto": "“Está a sabedoria com os idosos?"
    },
    "13": {
      "título": "",
      "texto": "Com Deus estão a sabedoria"
    },
    "14": {
      "título": "",
      "texto": "O que ele derruba"
    },
    "15": {
      "título": "",
      "texto": "Se ele retém as águas,"
    },
    "16": {
      "título": "",
      "texto": "“Com ele estão a força"
    },
    "17": {
      "título": "",
      "texto": "Ele leva os conselheiros embora,"
    },
    "18": {
      "título": "",
      "texto": "Solta os laços"
    },
    "19": {
      "título": "",
      "texto": "Ele leva os sacerdotes embora,"
    },
    "20": {
      "título": "",
      "texto": "Deixa os conselheiros"
    },
    "21": {
      "título": "",
      "texto": "Lança desprezo"
    },
    "22": {
      "título": "",
      "texto": "Das trevas revela"
    },
    "23": {
      "título": "",
      "texto": "Deus engrandece as nações"
    },
    "24": {
      "título": "",
      "texto": "Tira o entendimento"
    },
    "25": {
      "título": "",
      "texto": "Nas trevas andam tateando,"
    }
  }
}