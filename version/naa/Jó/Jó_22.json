{
  "livro": "Jó",
  "capítulo": 22,
  "versículos": {
    "1": {
      "título": "Você cometeu muitos pecados",
      "texto": "Então Elifaz, o temanita, tomou a palavra e disse:"
    },
    "2": {
      "título": "",
      "texto": "“Pode o homem ser"
    },
    "3": {
      "título": "",
      "texto": "Será que o Todo-Poderoso"
    },
    "4": {
      "título": "",
      "texto": "Ou será que é por causa"
    },
    "5": {
      "título": "",
      "texto": "Não é fato que é grande"
    },
    "6": {
      "título": "",
      "texto": "Porque sem motivo você exigiu"
    },
    "7": {
      "título": "",
      "texto": "Você não deu água ao cansado"
    },
    "8": {
      "título": "",
      "texto": "A terra pertencia"
    },
    "9": {
      "título": "",
      "texto": "Você despediu as viúvas"
    },
    "10": {
      "título": "",
      "texto": "Por isso, você está cercado"
    },
    "11": {
      "título": "",
      "texto": "Está submerso por trevas,"
    },
    "12": {
      "título": "Você quer seguir os iníquos?",
      "texto": "“Não está Deus"
    },
    "13": {
      "título": "",
      "texto": "E você diz:"
    },
    "14": {
      "título": "",
      "texto": "Grossas nuvens o encobrem,"
    },
    "15": {
      "título": "",
      "texto": "“Você quer seguir a rota antiga,"
    },
    "16": {
      "título": "",
      "texto": "Estes foram levados"
    },
    "17": {
      "título": "",
      "texto": "Diziam a Deus:"
    },
    "18": {
      "título": "",
      "texto": "Contudo, foi Deus"
    },
    "19": {
      "título": "",
      "texto": "Os justos veem"
    },
    "20": {
      "título": "",
      "texto": "dizendo: ‘Na verdade,"
    },
    "21": {
      "título": "Reconcilie-se com Deus",
      "texto": "“Portanto, reconcilie-se"
    },
    "22": {
      "título": "",
      "texto": "Aceite a instrução"
    },
    "23": {
      "título": "",
      "texto": "Se você se converter"
    },
    "24": {
      "título": "",
      "texto": "e lançar ao pó o seu ouro —"
    },
    "25": {
      "título": "",
      "texto": "então o Todo-Poderoso"
    },
    "26": {
      "título": "",
      "texto": "Então você encontrará prazer"
    },
    "27": {
      "título": "",
      "texto": "Você fará oração,"
    },
    "28": {
      "título": "",
      "texto": "Se você projetar alguma coisa,"
    },
    "29": {
      "título": "",
      "texto": "Se forem humilhados,"
    },
    "30": {
      "título": "",
      "texto": "Livrará até o que não é inocente;"
    }
  }
}