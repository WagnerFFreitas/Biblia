{
  "livro": "Jó",
  "capítulo": 39,
  "versículos": {
    "1": {
      "título": "Quem fez cada animal com o seu jeito de ser?",
      "texto": "“Você sabe o tempo em que"
    },
    "2": {
      "título": "",
      "texto": "Pode contar os meses"
    },
    "3": {
      "título": "",
      "texto": "Elas se encurvam"
    },
    "4": {
      "título": "",
      "texto": "Seus filhos se tornam robustos,"
    },
    "5": {
      "título": "",
      "texto": "Quem pôs em liberdade"
    },
    "6": {
      "título": "",
      "texto": "Eu lhe dei o deserto por casa"
    },
    "7": {
      "título": "",
      "texto": "Ele se ri do tumulto da cidade,"
    },
    "8": {
      "título": "",
      "texto": "Os montes são o lugar"
    },
    "9": {
      "título": "",
      "texto": "Será que o boi selvagem"
    },
    "10": {
      "título": "",
      "texto": "Por acaso você consegue"
    },
    "11": {
      "título": "",
      "texto": "Você vai confiar nele,"
    },
    "12": {
      "título": "",
      "texto": "Você acredita que ele trará"
    },
    "13": {
      "título": "",
      "texto": "“A avestruz bate alegre as asas,"
    },
    "14": {
      "título": "",
      "texto": "Ela põe os seus ovos no chão"
    },
    "15": {
      "título": "",
      "texto": "e se esquece de que algum pé"
    },
    "16": {
      "título": "",
      "texto": "Trata com dureza os seus filhos,"
    },
    "17": {
      "título": "",
      "texto": "porque Deus lhe negou"
    },
    "18": {
      "título": "",
      "texto": "Mas, quando de um salto"
    },
    "19": {
      "título": "",
      "texto": "“Por acaso foi você quem deu"
    },
    "20": {
      "título": "",
      "texto": "É você quem o faz pular"
    },
    "21": {
      "título": "",
      "texto": "Escarva no vale,"
    },
    "22": {
      "título": "",
      "texto": "Zomba do medo"
    },
    "23": {
      "título": "",
      "texto": "Sobre ele balança a aljava,"
    },
    "24": {
      "título": "",
      "texto": "Com ímpeto e fúria"
    },
    "25": {
      "título": "",
      "texto": "A cada toque do clarim ele diz:"
    },
    "26": {
      "título": "",
      "texto": "“Será que é pela inteligência"
    },
    "27": {
      "título": "",
      "texto": "Ou é por uma ordem sua"
    },
    "28": {
      "título": "",
      "texto": "Ela mora no penhasco"
    },
    "29": {
      "título": "",
      "texto": "Dali, descobre a presa;"
    },
    "30": {
      "título": "",
      "texto": "Seus filhotes chupam sangue;"
    }
  }
}