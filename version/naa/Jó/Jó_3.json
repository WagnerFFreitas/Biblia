{
  "livro": "Jó",
  "capítulo": 3,
  "versículos": {
    "1": {
      "título": "Jó amaldiçoa o seu nascimento",
      "texto": "Depois disto, Jó passou a falar e amaldiçoou o dia do seu nascimento."
    },
    "2": {
      "título": "",
      "texto": "Jó disse:"
    },
    "3": {
      "título": "",
      "texto": "“Pereça o dia em que nasci"
    },
    "4": {
      "título": "",
      "texto": "Que aquele dia"
    },
    "5": {
      "título": "",
      "texto": "Que as trevas"
    },
    "6": {
      "título": "",
      "texto": "Aquela noite,"
    },
    "7": {
      "título": "",
      "texto": "Sim, que seja estéril aquela noite,"
    },
    "8": {
      "título": "",
      "texto": "Amaldiçoem-na"
    },
    "9": {
      "título": "",
      "texto": "Escureçam-se as estrelas"
    },
    "10": {
      "título": "",
      "texto": "pois não fechou as portas"
    },
    "11": {
      "título": "Por que não morri ao nascer?",
      "texto": "“Por que não morri ao nascer?"
    },
    "12": {
      "título": "",
      "texto": "Por que houve um colo"
    },
    "13": {
      "título": "",
      "texto": "Porque agora"
    },
    "14": {
      "título": "",
      "texto": "com os reis e conselheiros"
    },
    "15": {
      "título": "",
      "texto": "ou com os príncipes"
    },
    "16": {
      "título": "",
      "texto": "ou, como aborto oculto,"
    },
    "17": {
      "título": "",
      "texto": "Ali os maus cessam de perturbar,"
    },
    "18": {
      "título": "",
      "texto": "Ali os presos juntamente"
    },
    "19": {
      "título": "",
      "texto": "Ali está tanto o pequeno"
    },
    "20": {
      "título": "Por que o miserável continua vivendo?",
      "texto": "“Por que se concede"
    },
    "21": {
      "título": "",
      "texto": "que esperam a morte,"
    },
    "22": {
      "título": "",
      "texto": "que se alegrariam"
    },
    "23": {
      "título": "",
      "texto": "Por que se concede"
    },
    "24": {
      "título": "",
      "texto": "“Porque em vez do meu pão"
    },
    "25": {
      "título": "",
      "texto": "Aquilo que temo me sobrevém,"
    },
    "26": {
      "título": "",
      "texto": "Não tenho descanso,"
    }
  }
}