{
  "livro": "Juízes",
  "capítulo": 5,
  "versículos": {
    "1": {
      "título": "O cântico de Débora",
      "texto": "Naquele dia Débora e Baraque, filho de Abinoão, cantaram assim:"
    },
    "2": {
      "título": "",
      "texto": "“Porque os chefes se puseram"
    },
    "3": {
      "título": "",
      "texto": "Escutem, ó reis!"
    },
    "4": {
      "título": "",
      "texto": "“Quando tu, ó Senhor ,"
    },
    "5": {
      "título": "",
      "texto": "Os montes tremeram"
    },
    "6": {
      "título": "",
      "texto": "“Nos dias de Sangar,"
    },
    "7": {
      "título": "",
      "texto": "Ficaram desertas"
    },
    "8": {
      "título": "",
      "texto": "Escolheram-se deuses novos;"
    },
    "9": {
      "título": "",
      "texto": "Meu coração está"
    },
    "10": {
      "título": "",
      "texto": "Vocês que cavalgam"
    },
    "11": {
      "título": "",
      "texto": "À música dos distribuidores"
    },
    "12": {
      "título": "",
      "texto": "“Desperte, Débora, desperte!"
    },
    "13": {
      "título": "",
      "texto": "“Então desceu"
    },
    "14": {
      "título": "",
      "texto": "De Efraim, cujas raízes estão"
    },
    "15": {
      "título": "",
      "texto": "Também os príncipes de Issacar"
    },
    "16": {
      "título": "",
      "texto": "Por que vocês ficaram"
    },
    "17": {
      "título": "",
      "texto": "Gileade ficou"
    },
    "18": {
      "título": "",
      "texto": "Zebulom é povo"
    },
    "19": {
      "título": "",
      "texto": "“Vieram reis e lutaram."
    },
    "20": {
      "título": "",
      "texto": "Lá do céu as estrelas lutaram;"
    },
    "21": {
      "título": "",
      "texto": "O ribeiro de Quisom os arrastou,"
    },
    "22": {
      "título": "",
      "texto": "Então os cascos dos cavalos"
    },
    "23": {
      "título": "",
      "texto": "“Amaldiçoem Meroz,"
    },
    "24": {
      "título": "",
      "texto": "“Que a mais bendita"
    },
    "25": {
      "título": "",
      "texto": "Sísera pediu água,"
    },
    "26": {
      "título": "",
      "texto": "Ela estendeu uma das mãos"
    },
    "27": {
      "título": "",
      "texto": "Aos pés dela ele se encurvou,"
    },
    "28": {
      "título": "",
      "texto": "“A mãe de Sísera"
    },
    "29": {
      "título": "",
      "texto": "As mais sábias das suas damas"
    },
    "30": {
      "título": "",
      "texto": "‘Não é verdade que acharam"
    },
    "31": {
      "título": "",
      "texto": "“Assim, ó Senhor , pereçam"
    }
  }
}