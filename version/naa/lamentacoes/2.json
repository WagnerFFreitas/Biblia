{
    "livro": "Lamentações de Jeremias",
    "capitulo": 2,
    "titulos": {
        "1": "As tristezas de Sião provêm de Deus"
    },
    "versiculos": {
        "1": "Como o Senhor, na sua ira,",
        "2": "O Senhor devorou",
        "3": "No furor da sua ira,",
        "4": "Entesou o seu arco,",
        "5": "O Senhor se tornou",
        "6": "Demoliu com violência",
        "7": "O Senhor rejeitou",
        "8": "O Senhor resolveu",
        "9": "Os seus portões",
        "10": "Os anciãos da filha de Sião",
        "11": "Com lágrimas",
        "12": "Perguntam",
        "13": "O que posso lhe dizer?",
        "14": "As visões que",
        "15": "Todos os que passam",
        "16": "Todos os seus inimigos",
        "17": "O Senhor fez",
        "18": "O coração do povo",
        "19": "Levante-se",
        "20": "Vê, ó Senhor , e considera",
        "21": "Os jovens e os velhos",
        "22": "Convocaste de toda parte"
    }
