{
  "livro": "Lamentações de Jeremias",
  "capítulo": 5,
  "versículos": {
    "1": {
      "título": "Oração pedindo misericórdia",
      "texto": "Lembra-te, Senhor ,"
    },
    "2": {
      "título": "",
      "texto": "A nossa herança foi entregue"
    },
    "3": {
      "título": "",
      "texto": "Somos órfãos,"
    },
    "4": {
      "título": "",
      "texto": "Temos de comprar"
    },
    "5": {
      "título": "",
      "texto": "Os nossos perseguidores"
    },
    "6": {
      "título": "",
      "texto": "Submetemo-nos aos egípcios"
    },
    "7": {
      "título": "",
      "texto": "Nossos pais pecaram"
    },
    "8": {
      "título": "",
      "texto": "Escravos dominam sobre nós;"
    },
    "9": {
      "título": "",
      "texto": "Arriscamos a vida"
    },
    "10": {
      "título": "",
      "texto": "Nossa pele queima"
    },
    "11": {
      "título": "",
      "texto": "As mulheres foram"
    },
    "12": {
      "título": "",
      "texto": "Enforcaram os príncipes,"
    },
    "13": {
      "título": "",
      "texto": "Os jovens são obrigados"
    },
    "14": {
      "título": "",
      "texto": "Os anciãos já não se reúnem"
    },
    "15": {
      "título": "",
      "texto": "Cessou a alegria"
    },
    "16": {
      "título": "",
      "texto": "Caiu a coroa da nossa cabeça."
    },
    "17": {
      "título": "",
      "texto": "Por causa disso,"
    },
    "18": {
      "título": "",
      "texto": "Pelo monte Sião,"
    },
    "19": {
      "título": "",
      "texto": "Tu, Senhor ,"
    },
    "20": {
      "título": "",
      "texto": "Por que te esquecerias"
    },
    "21": {
      "título": "",
      "texto": "Converte-nos a ti, Senhor ,"
    },
    "22": {
      "título": "",
      "texto": "Por que nos rejeitarias de vez?"
    }
  }
}