{
  "livro": "Lamentações de Jeremias",
  "capítulo": 4,
  "versículos": {
    "1": {
      "título": "Os sofrimentos do cerco",
      "texto": "Como se escureceu o ouro!"
    },
    "2": {
      "título": "",
      "texto": "Os nobres filhos de Sião,"
    },
    "3": {
      "título": "",
      "texto": "Até os chacais"
    },
    "4": {
      "título": "",
      "texto": "A língua do bebê"
    },
    "5": {
      "título": "",
      "texto": "Os que se alimentavam"
    },
    "6": {
      "título": "",
      "texto": "Porque a maldade"
    },
    "7": {
      "título": "",
      "texto": "Os seus príncipes"
    },
    "8": {
      "título": "",
      "texto": "Mas agora o aspecto deles"
    },
    "9": {
      "título": "",
      "texto": "Mais felizes foram"
    },
    "10": {
      "título": "",
      "texto": "As mãos das mulheres"
    },
    "11": {
      "título": "",
      "texto": "O Senhor deu cumprimento"
    },
    "12": {
      "título": "",
      "texto": "Nem os reis da terra,"
    },
    "13": {
      "título": "",
      "texto": "Tudo isso aconteceu"
    },
    "14": {
      "título": "",
      "texto": "Vagueiam como cegos"
    },
    "15": {
      "título": "",
      "texto": "E o povo grita:"
    },
    "16": {
      "título": "",
      "texto": "A ira do Senhor os espalhou;"
    },
    "17": {
      "título": "",
      "texto": "Os nossos olhos"
    },
    "18": {
      "título": "",
      "texto": "Espreitavam"
    },
    "19": {
      "título": "",
      "texto": "Os nossos perseguidores"
    },
    "20": {
      "título": "",
      "texto": "O ungido do Senhor ,"
    },
    "21": {
      "título": "",
      "texto": "Exulte e alegre-se agora,"
    },
    "22": {
      "título": "",
      "texto": "O castigo por causa"
    }
  }
}