{
  "livro": "Lamentações de Jeremias",
  "capítulo": 2,
  "versículos": {
    "1": {
      "título": "As tristezas de Sião provêm de Deus",
      "texto": "Como o Senhor, na sua ira,"
    },
    "2": {
      "título": "",
      "texto": "O Senhor devorou"
    },
    "3": {
      "título": "",
      "texto": "No furor da sua ira,"
    },
    "4": {
      "título": "",
      "texto": "Entesou o seu arco,"
    },
    "5": {
      "título": "",
      "texto": "O Senhor se tornou"
    },
    "6": {
      "título": "",
      "texto": "Demoliu com violência"
    },
    "7": {
      "título": "",
      "texto": "O Senhor rejeitou"
    },
    "8": {
      "título": "",
      "texto": "O Senhor resolveu"
    },
    "9": {
      "título": "",
      "texto": "Os seus portões"
    },
    "10": {
      "título": "",
      "texto": "Os anciãos da filha de Sião"
    },
    "11": {
      "título": "",
      "texto": "Com lágrimas"
    },
    "12": {
      "título": "",
      "texto": "Perguntam"
    },
    "13": {
      "título": "",
      "texto": "O que posso lhe dizer?"
    },
    "14": {
      "título": "",
      "texto": "As visões que"
    },
    "15": {
      "título": "",
      "texto": "Todos os que passam"
    },
    "16": {
      "título": "",
      "texto": "Todos os seus inimigos"
    },
    "17": {
      "título": "",
      "texto": "O Senhor fez"
    },
    "18": {
      "título": "",
      "texto": "O coração do povo"
    },
    "19": {
      "título": "",
      "texto": "Levante-se"
    },
    "20": {
      "título": "",
      "texto": "Vê, ó Senhor , e considera"
    },
    "21": {
      "título": "",
      "texto": "Os jovens e os velhos"
    },
    "22": {
      "título": "",
      "texto": "Convocaste de toda parte"
    }
  }
}