{
  "livro": "Jeremias",
  "capítulo": 10,
  "versículos": {
    "1": {
      "título": "A idolatria e a verdadeira adoração",
      "texto": "Ouçam a palavra que o Senhor dirige a vocês, ó casa de Israel."
    },
    "2": {
      "título": "",
      "texto": "Assim diz o Senhor :"
    },
    "3": {
      "título": "",
      "texto": "Porque os costumes dos povos"
    },
    "4": {
      "título": "",
      "texto": "Com prata e ouro a enfeitam,"
    },
    "5": {
      "título": "",
      "texto": "Os ídolos são como"
    },
    "6": {
      "título": "",
      "texto": "Não há ninguém semelhante a ti,"
    },
    "7": {
      "título": "",
      "texto": "Quem não temeria a ti,"
    },
    "8": {
      "título": "",
      "texto": "Mas todos eles se tornaram"
    },
    "9": {
      "título": "",
      "texto": "Traz-se prata batida de Társis"
    },
    "10": {
      "título": "",
      "texto": "Mas o Senhor"
    },
    "11": {
      "título": "",
      "texto": "Digam-lhes o seguinte:"
    },
    "12": {
      "título": "",
      "texto": "O Senhor fez a terra"
    },
    "13": {
      "título": "",
      "texto": "Quando ele faz soar a sua voz,"
    },
    "14": {
      "título": "",
      "texto": "Todas as pessoas são tolas"
    },
    "15": {
      "título": "",
      "texto": "São vaidade, obras ridículas."
    },
    "16": {
      "título": "",
      "texto": "Aquele que é a Porção de Jacó"
    },
    "17": {
      "título": "Lamento sobre a destruição de Judá",
      "texto": "Ajunte do chão"
    },
    "18": {
      "título": "",
      "texto": "Porque assim diz o Senhor :"
    },
    "19": {
      "título": "",
      "texto": "Ai de mim, por causa"
    },
    "20": {
      "título": "",
      "texto": "A minha tenda foi destruída,"
    },
    "21": {
      "título": "",
      "texto": "Porque os pastores"
    },
    "22": {
      "título": "",
      "texto": "Eis aí um rumor!"
    },
    "23": {
      "título": "Jeremias ora em favor do seu povo",
      "texto": "Eu sei, ó Senhor ,"
    },
    "24": {
      "título": "",
      "texto": "Castiga-me, ó Senhor ,"
    },
    "25": {
      "título": "",
      "texto": "Derrama o teu furor"
    }
  }
}