{
  "livro": "Jeremias",
  "capítulo": 6,
  "versículos": {
    "1": {
      "título": "Jerusalém será sitiada",
      "texto": "Filhos de Benjamim,"
    },
    "2": {
      "título": "",
      "texto": "Deixarei em ruínas a formosa"
    },
    "3": {
      "título": "",
      "texto": "Contra ela virão pastores"
    },
    "4": {
      "título": "",
      "texto": "“Preparem a guerra contra ela!"
    },
    "5": {
      "título": "",
      "texto": "Levantem-se,"
    },
    "6": {
      "título": "",
      "texto": "Porque assim diz"
    },
    "7": {
      "título": "",
      "texto": "Como o poço conserva frescas"
    },
    "8": {
      "título": "",
      "texto": "Aceite a disciplina, ó Jerusalém,"
    },
    "9": {
      "título": "O castigo por causa do pecado",
      "texto": "Assim diz o Senhor dos Exércitos:"
    },
    "10": {
      "título": "",
      "texto": "A quem falarei e testemunharei,"
    },
    "11": {
      "título": "",
      "texto": "Por isso, estou repleto"
    },
    "12": {
      "título": "",
      "texto": "As suas casas"
    },
    "13": {
      "título": "",
      "texto": "“Porque desde o menor deles"
    },
    "14": {
      "título": "",
      "texto": "Curam superficialmente"
    },
    "15": {
      "título": "",
      "texto": "Será que ficaram envergonhados,"
    },
    "16": {
      "título": "",
      "texto": "Assim diz o Senhor :"
    },
    "17": {
      "título": "",
      "texto": "Também pus atalaias"
    },
    "18": {
      "título": "",
      "texto": "Portanto, escutem, ó nações,"
    },
    "19": {
      "título": "",
      "texto": "Ouça, ó terra!"
    },
    "20": {
      "título": "",
      "texto": "Para que, pois, me vem"
    },
    "21": {
      "título": "",
      "texto": "Portanto, assim diz o Senhor :"
    },
    "22": {
      "título": "O inimigo do Norte",
      "texto": "Assim diz o Senhor :"
    },
    "23": {
      "título": "",
      "texto": "Armam-se de arco e de lança;"
    },
    "24": {
      "título": "",
      "texto": "Ao ouvirmos a fama deles,"
    },
    "25": {
      "título": "",
      "texto": "Não saiam ao campo,"
    },
    "26": {
      "título": "",
      "texto": "Ó filha do meu povo,"
    },
    "27": {
      "título": "O trabalho inútil de Jeremias",
      "texto": "“Jeremias, fiz de você"
    },
    "28": {
      "título": "",
      "texto": "Todos são mais do que rebeldes"
    },
    "29": {
      "título": "",
      "texto": "O fole sopra com força,"
    },
    "30": {
      "título": "",
      "texto": "Serão chamados"
    }
  }
}