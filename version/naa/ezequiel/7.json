{
    "livro": "Ezequiel",
    "capitulo": 7,
    "titulos": {
        "1": "O fim chegou!"
    },
    "versiculos": {
        "1": "A palavra do Senhor veio a mim, dizendo:",
        "2": "— Filho do homem, assim diz o Senhor Deus a respeito da terra de Israel:",
        "3": "Agora o fim chegou",
        "4": "Os meus olhos não terão piedade,",
        "5": "— Assim diz o Senhor Deus :",
        "6": "Chegou o fim!",
        "7": "A sua sentença chegou,",
        "8": "Agora, em breve,",
        "9": "Os meus olhos não terão piedade,",
        "10": "“Eis o dia! Eis que chegou!",
        "11": "A violência se ergueu",
        "12": "Chegou a hora,",
        "13": "Porque o vendedor",
        "14": "“Tocaram a trombeta",
        "15": "Fora está a espada;",
        "16": "Se alguns deles, fugindo,",
        "17": "Todas as mãos desfalecerão,",
        "18": "Vestirão roupa",
        "19": "Jogarão a sua prata nas ruas,",
        "20": "Dessas preciosas joias fizeram",
        "21": "“Entregarei tudo isso",
        "22": "Desviarei deles o rosto,",
        "23": "“Prepare uma corrente,",
        "24": "Trarei aqui as piores nações,",
        "25": "A destruição está a caminho!",
        "26": "Virá miséria sobre miséria,",
        "27": "O rei estará de luto,"
    }
}