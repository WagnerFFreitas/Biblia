{
  "livro": "Miquéias",
  "capítulo": 7,
  "versículos": {
    "1": {
      "título": "A corrupção moral de Israel",
      "texto": "Ai de mim!"
    },
    "2": {
      "título": "",
      "texto": "Desapareceram da terra"
    },
    "3": {
      "título": "",
      "texto": "As suas mãos são hábeis"
    },
    "4": {
      "título": "",
      "texto": "O melhor deles"
    },
    "5": {
      "título": "",
      "texto": "Não acredite em seu amigo,"
    },
    "6": {
      "título": "",
      "texto": "Porque o filho despreza o pai,"
    },
    "7": {
      "título": "",
      "texto": "Eu, porém, olharei para o Senhor"
    },
    "8": {
      "título": "Deus se compadece de Israel",
      "texto": "Minha inimiga, não se alegre"
    },
    "9": {
      "título": "",
      "texto": "Sofrerei a ira do Senhor ,"
    },
    "10": {
      "título": "",
      "texto": "A minha inimiga verá isso;"
    },
    "11": {
      "título": "",
      "texto": "No dia da reedificação"
    },
    "12": {
      "título": "",
      "texto": "Nesse dia, virão a você"
    },
    "13": {
      "título": "",
      "texto": "Mas a terra se tornará em desolação,"
    },
    "14": {
      "título": "",
      "texto": "Ó Senhor , apascenta o teu povo"
    },
    "15": {
      "título": "",
      "texto": "Eu lhe mostrarei maravilhas,"
    },
    "16": {
      "título": "",
      "texto": "As nações verão isso"
    },
    "17": {
      "título": "",
      "texto": "Lamberão o pó como serpentes;"
    },
    "18": {
      "título": "",
      "texto": "Quem é semelhante a ti, ó Deus,"
    },
    "19": {
      "título": "",
      "texto": "Ele voltará a ter"
    },
    "20": {
      "título": "",
      "texto": "Mostrarás a Jacó a fidelidade"
    }
  }
}