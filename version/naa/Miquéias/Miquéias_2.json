{
  "livro": "Miquéias",
  "capítulo": 2,
  "versículos": {
    "1": {
      "título": "Ai dos opressores gananciosos",
      "texto": "“Ai daqueles que,"
    },
    "2": {
      "título": "",
      "texto": "Cobiçam campos"
    },
    "3": {
      "título": "",
      "texto": "Portanto, assim diz o Senhor :"
    },
    "4": {
      "título": "",
      "texto": "Naquele dia, se criará"
    },
    "5": {
      "título": "",
      "texto": "Portanto, na congregação"
    },
    "6": {
      "título": "Contra os falsos profetas",
      "texto": "“Eles dizem:"
    },
    "7": {
      "título": "",
      "texto": "Tais coisas anunciadas"
    },
    "8": {
      "título": "",
      "texto": "Mas, há pouco, o meu povo"
    },
    "9": {
      "título": "",
      "texto": "Vocês expulsam"
    },
    "10": {
      "título": "",
      "texto": "Levantem-se e vão embora!"
    },
    "11": {
      "título": "",
      "texto": "Se alguém, seguindo o vento"
    },
    "12": {
      "título": "O Senhor congrega o remanescente de Israel",
      "texto": "“Certamente eu ajuntarei"
    },
    "13": {
      "título": "",
      "texto": "Diante deles virá"
    }
  }
}