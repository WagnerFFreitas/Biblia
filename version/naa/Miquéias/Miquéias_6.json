{
  "livro": "Miquéias",
  "capítulo": 6,
  "versículos": {
    "1": {
      "título": "Deus entrará em juízo com o seu povo",
      "texto": "Escutem agora"
    },
    "2": {
      "título": "",
      "texto": "Ó montes, escutem"
    },
    "3": {
      "título": "",
      "texto": "“Meu povo,"
    },
    "4": {
      "título": "",
      "texto": "Pois eu o tirei da terra do Egito"
    },
    "5": {
      "título": "",
      "texto": "Meu povo,"
    },
    "6": {
      "título": "O que Deus exige",
      "texto": "Com que me apresentarei"
    },
    "7": {
      "título": "",
      "texto": "Será que o Senhor se agrada"
    },
    "8": {
      "título": "",
      "texto": "Ele já mostrou a você o que é bom;"
    },
    "9": {
      "título": "A injustiça terá seu castigo",
      "texto": "A voz do Senhor"
    },
    "10": {
      "título": "",
      "texto": "Ainda se encontram,"
    },
    "11": {
      "título": "",
      "texto": "Poderei eu inocentar"
    },
    "12": {
      "título": "",
      "texto": "Porque os ricos da cidade"
    },
    "13": {
      "título": "",
      "texto": "Assim, também"
    },
    "14": {
      "título": "",
      "texto": "Vocês comerão,"
    },
    "15": {
      "título": "",
      "texto": "Vocês irão semear,"
    },
    "16": {
      "título": "",
      "texto": "Vocês observaram"
    }
  }
}