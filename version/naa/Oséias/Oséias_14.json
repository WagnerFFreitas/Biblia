    "1": {
      "título": "Chamado para voltar ao Senhor",
      "texto": "“Israel, volte para o Senhor ,"
    },
    "2": {
      "título": "",
      "texto": "Tragam palavras"
    },
    "3": {
      "título": "",
      "texto": "A Assíria não nos salvará."
    },
    "4": {
      "título": "",
      "texto": "“Vou curar a rebeldia deles."
    },
    "5": {
      "título": "",
      "texto": "Serei para Israel como orvalho;"
    },
    "6": {
      "título": "",
      "texto": "Os seus ramos se estenderão,"
    },
    "7": {
      "título": "",
      "texto": "Os que se assentavam"
    },
    "8": {
      "título": "",
      "texto": "“Ó Efraim, que tenho eu"
    },
    "9": {
      "título": "Apelo final",
      "texto": "Quem é sábio,"
    }
  }
}