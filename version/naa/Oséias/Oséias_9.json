{
  "livro": "Oséias",
  "capítulo": 9,
  "versículos": {
    "1": {
      "título": "O castigo de Israel",
      "texto": "“Não se alegre, Israel,"
    },
    "2": {
      "título": "",
      "texto": "A eira e o lagar"
    },
    "3": {
      "título": "",
      "texto": "Não permanecerão"
    },
    "4": {
      "título": "",
      "texto": "“Não oferecerão"
    },
    "5": {
      "título": "",
      "texto": "“O que vocês farão"
    },
    "6": {
      "título": "",
      "texto": "Porque eis que eles fugiram"
    },
    "7": {
      "título": "",
      "texto": "“Chegaram os dias do castigo,"
    },
    "8": {
      "título": "",
      "texto": "O profeta é sentinela"
    },
    "9": {
      "título": "",
      "texto": "Eles se afundaram na corrupção,"
    },
    "10": {
      "título": "",
      "texto": "“Encontrei Israel"
    },
    "11": {
      "título": "",
      "texto": "Quanto a Efraim,"
    },
    "12": {
      "título": "",
      "texto": "Ainda que venham"
    },
    "13": {
      "título": "",
      "texto": "Aos meus olhos,"
    },
    "14": {
      "título": "",
      "texto": "Dá-lhes, ó Senhor !"
    },
    "15": {
      "título": "",
      "texto": "“Toda a maldade deles"
    },
    "16": {
      "título": "",
      "texto": "“Efraim está ferido."
    },
    "17": {
      "título": "",
      "texto": "O meu Deus os rejeitará,"
    }
  }
}