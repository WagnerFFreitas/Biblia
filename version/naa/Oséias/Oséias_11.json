{
  "livro": "Oséias",
  "capítulo": 11,
  "versículos": {
    "1": {
      "título": "O amor de Deus e a ingratidão de Israel",
      "texto": "“Quando Israel era menino,"
    },
    "2": {
      "título": "",
      "texto": "Quanto mais eu os chamava,"
    },
    "3": {
      "título": "",
      "texto": "Mas fui eu que ensinei"
    },
    "4": {
      "título": "",
      "texto": "Atraí-os com cordas humanas,"
    },
    "5": {
      "título": "",
      "texto": "“Não voltarão"
    },
    "6": {
      "título": "",
      "texto": "A espada cairá"
    },
    "7": {
      "título": "",
      "texto": "Porque o meu povo é inclinado"
    },
    "8": {
      "título": "",
      "texto": "Como poderia eu abandoná-lo,"
    },
    "9": {
      "título": "",
      "texto": "Não executarei o furor"
    },
    "10": {
      "título": "",
      "texto": "“Seguirão o Senhor ,"
    },
    "11": {
      "título": "",
      "texto": "tremendo, como passarinhos,"
    },
    "12": {
      "título": "A condenação de Israel e de Judá",
      "texto": "“Efraim me cercou com mentiras,"
    }
  }
}