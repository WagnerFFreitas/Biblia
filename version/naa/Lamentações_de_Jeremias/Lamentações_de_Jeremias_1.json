{
  "livro": "Lamentações de Jeremias",
  "capítulo": 1,
  "versículos": {
    "1": {
      "título": "Jerusalém destruída",
      "texto": "Como jaz solitária"
    },
    "2": {
      "título": "",
      "texto": "Chora amargamente"
    },
    "3": {
      "título": "",
      "texto": "Judá foi levado ao exílio,"
    },
    "4": {
      "título": "",
      "texto": "Os caminhos de Sião"
    },
    "5": {
      "título": "",
      "texto": "Os seus adversários"
    },
    "6": {
      "título": "",
      "texto": "Da filha de Sião já se passou"
    },
    "7": {
      "título": "",
      "texto": "Agora que está aflita"
    },
    "8": {
      "título": "",
      "texto": "Jerusalém pecou"
    },
    "9": {
      "título": "",
      "texto": "A sua impureza está"
    },
    "10": {
      "título": "",
      "texto": "O adversário pôs a mão"
    },
    "11": {
      "título": "",
      "texto": "Todo o seu povo"
    },
    "12": {
      "título": "",
      "texto": "“Todos vocês que passam"
    },
    "13": {
      "título": "",
      "texto": "“Lá do alto ele enviou fogo"
    },
    "14": {
      "título": "",
      "texto": "“Ele, com a sua mão,"
    },
    "15": {
      "título": "",
      "texto": "“O Senhor dispersou"
    },
    "16": {
      "título": "",
      "texto": "“Por estas coisas, eu choro;"
    },
    "17": {
      "título": "",
      "texto": "Sião estende as mãos,"
    },
    "18": {
      "título": "",
      "texto": "“Justo é o Senhor ,"
    },
    "19": {
      "título": "",
      "texto": "“Chamei os meus amantes,"
    },
    "20": {
      "título": "",
      "texto": "“Olha, Senhor ,"
    },
    "21": {
      "título": "",
      "texto": "“Ouvem-se os meus"
    },
    "22": {
      "título": "",
      "texto": "“Venha toda a sua iniquidade"
    }
  }
}