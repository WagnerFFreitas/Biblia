{
  "livro": "Deuteronômio",
  "capítulo": 33,
  "versículos": {
    "1": {
      "título": "A bênção de Moisés",
      "texto": "Esta é a bênção que Moisés, homem de Deus, deu aos filhos de Israel, antes da sua morte."
    },
    "2": {
      "título": "",
      "texto": "Ele disse:"
    },
    "3": {
      "título": "",
      "texto": "Na verdade, amas os povos;"
    },
    "4": {
      "título": "",
      "texto": "Moisés nos deu a Lei,"
    },
    "5": {
      "título": "",
      "texto": "O Senhor se tornou rei em Jesurum,"
    },
    "6": {
      "título": "",
      "texto": "“Que Rúben viva e não morra;"
    },
    "7": {
      "título": "",
      "texto": "Isto é o que disse de Judá:"
    },
    "8": {
      "título": "",
      "texto": "De Levi disse:"
    },
    "9": {
      "título": "",
      "texto": "aquele que disse a seu pai e a sua mãe:"
    },
    "10": {
      "título": "",
      "texto": "Ensina os teus juízos a Jacó"
    },
    "11": {
      "título": "",
      "texto": "Abençoa o seu poder, ó Senhor ,"
    },
    "12": {
      "título": "",
      "texto": "De Benjamim disse:"
    },
    "13": {
      "título": "",
      "texto": "De José disse:"
    },
    "14": {
      "título": "",
      "texto": "com o que é mais excelente daquilo que o sol amadurece"
    },
    "15": {
      "título": "",
      "texto": "com o que é mais excelente dos montes antigos"
    },
    "16": {
      "título": "",
      "texto": "com o que é mais excelente da terra e da sua plenitude"
    },
    "17": {
      "título": "",
      "texto": "Ele tem a imponência do primogênito de um touro,"
    },
    "18": {
      "título": "",
      "texto": "De Zebulom disse:"
    },
    "19": {
      "título": "",
      "texto": "Os dois chamarão os povos ao monte;"
    },
    "20": {
      "título": "",
      "texto": "De Gade disse:"
    },
    "21": {
      "título": "",
      "texto": "Ficou com a melhor parte,"
    },
    "22": {
      "título": "",
      "texto": "De Dã disse:"
    },
    "23": {
      "título": "",
      "texto": "De Naftali disse:"
    },
    "24": {
      "título": "",
      "texto": "De Aser disse:"
    },
    "25": {
      "título": "",
      "texto": "Sejam de ferro e de bronze os seus ferrolhos,"
    },
    "26": {
      "título": "",
      "texto": "“Não há ninguém como Deus, ó Jesurum!"
    },
    "27": {
      "título": "",
      "texto": "O Deus eterno é a sua habitação"
    },
    "28": {
      "título": "",
      "texto": "Israel, pois, habitará seguro,"
    },
    "29": {
      "título": "",
      "texto": "“Feliz é você, ó Israel!"
    }
  }
}