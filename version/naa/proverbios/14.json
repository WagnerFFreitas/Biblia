{
  "livro": "Provérbios",
  "capítulo": 14,
  "versículos": {
    "1": {
      "título": "",
      "texto": "A mulher sábia edifica a sua casa,"
    },
    "2": {
      "título": "",
      "texto": "Quem anda na retidão"
    },
    "3": {
      "título": "",
      "texto": "Na boca do tolo"
    },
    "4": {
      "título": "",
      "texto": "Quando não há bois,"
    },
    "5": {
      "título": "",
      "texto": "A testemunha verdadeira"
    },
    "6": {
      "título": "",
      "texto": "O zombador procura a sabedoria"
    },
    "7": {
      "título": "",
      "texto": "Fuja da presença do insensato,"
    },
    "8": {
      "título": "",
      "texto": "A sabedoria do prudente"
    },
    "9": {
      "título": "",
      "texto": "Os insensatos"
    },
    "10": {
      "título": "",
      "texto": "O coração conhece"
    },
    "11": {
      "título": "",
      "texto": "A casa dos ímpios será destruída,"
    },
    "12": {
      "título": "",
      "texto": "Há caminho que ao ser humano"
    },
    "13": {
      "título": "",
      "texto": "Até no riso"
    },
    "14": {
      "título": "",
      "texto": "O infiel de coração"
    },
    "15": {
      "título": "",
      "texto": "O ingênuo dá crédito"
    },
    "16": {
      "título": "",
      "texto": "O sábio é cauteloso"
    },
    "17": {
      "título": "",
      "texto": "Quem logo se irrita"
    },
    "18": {
      "título": "",
      "texto": "Os ingênuos herdam a tolice,"
    },
    "19": {
      "título": "",
      "texto": "Os maus se inclinarão"
    },
    "20": {
      "título": "",
      "texto": "O pobre é odiado"
    },
    "21": {
      "título": "",
      "texto": "Quem despreza o seu vizinho"
    },
    "22": {
      "título": "",
      "texto": "Por acaso não se afastam"
    },
    "23": {
      "título": "",
      "texto": "Em todo trabalho há proveito;"
    },
    "24": {
      "título": "",
      "texto": "Para os sábios a riqueza é coroa,"
    },
    "25": {
      "título": "",
      "texto": "A testemunha verdadeira"
    },
    "26": {
      "título": "",
      "texto": "Quem teme o Senhor"
    },
    "27": {
      "título": "",
      "texto": "O temor do Senhor"
    },
    "28": {
      "título": "",
      "texto": "Na multidão do povo"
    },
    "29": {
      "título": "",
      "texto": "Quem tarda em irar-se"
    },
    "30": {
      "título": "",
      "texto": "O ânimo sereno"
    },
    "31": {
      "título": "",
      "texto": "Quem oprime o pobre"
    },
    "32": {
      "título": "",
      "texto": "O ímpio é derrubado"
    },
    "33": {
      "título": "",
      "texto": "No coração do prudente"
    },
    "34": {
      "título": "",
      "texto": "A justiça é a glória da nação,"
    },
    "35": {
      "título": "",
      "texto": "O servo prudente"
    }
  }
}