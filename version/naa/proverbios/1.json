{
  "livro": "Provérbios",
  "capítulo": 1,
  "versículos": {
    "1": {
      "título": "Uso dos provérbios",
      "texto": "Provérbios de Salomão,"
    },
    "2": {
      "título": "",
      "texto": "para aprender a sabedoria"
    },
    "3": {
      "título": "",
      "texto": "para obter o ensino"
    },
    "4": {
      "título": "",
      "texto": "para dar prudência aos simples"
    },
    "5": {
      "título": "",
      "texto": "Que o sábio ouça"
    },
    "6": {
      "título": "",
      "texto": "para entender provérbios"
    },
    "7": {
      "título": "",
      "texto": "O temor do Senhor"
    },
    "8": {
      "título": "Contra as seduções dos pecadores",
      "texto": "Meu filho, ouça o ensino"
    },
    "9": {
      "título": "",
      "texto": "Porque serão um diadema"
    },
    "10": {
      "título": "",
      "texto": "Meu filho, se os pecadores"
    },
    "11": {
      "título": "",
      "texto": "Talvez eles digam:"
    },
    "12": {
      "título": "",
      "texto": "Vamos engoli-los vivos,"
    },
    "13": {
      "título": "",
      "texto": "Acharemos todo tipo"
    },
    "14": {
      "título": "",
      "texto": "Junte-se a nós!"
    },
    "15": {
      "título": "",
      "texto": "Meu filho, não se ponha"
    },
    "16": {
      "título": "",
      "texto": "Porque os pés deles"
    },
    "17": {
      "título": "",
      "texto": "Pois em vão se estende a rede"
    },
    "18": {
      "título": "",
      "texto": "mas estes armam emboscadas"
    },
    "19": {
      "título": "",
      "texto": "Este é o fim de todo ganancioso;"
    },
    "20": {
      "título": "O convite da Sabedoria",
      "texto": "A Sabedoria grita nas ruas;"
    },
    "21": {
      "título": "",
      "texto": "Do alto das muralhas clama,"
    },
    "22": {
      "título": "",
      "texto": "“Até quando vocês, ingênuos,"
    },
    "23": {
      "título": "",
      "texto": "Deem ouvidos"
    },
    "24": {
      "título": "",
      "texto": "Mas porque clamei,"
    },
    "25": {
      "título": "",
      "texto": "— pelo contrário, rejeitaram"
    },
    "26": {
      "título": "",
      "texto": "também eu darei risada"
    },
    "27": {
      "título": "",
      "texto": "quando o terror chegar"
    },
    "28": {
      "título": "",
      "texto": "Então eles me invocarão,"
    },
    "29": {
      "título": "",
      "texto": "Porque odiaram o conhecimento"
    },
    "30": {
      "título": "",
      "texto": "não quiseram o meu conselho"
    },
    "31": {
      "título": "",
      "texto": "Portanto, comerão do fruto"
    },
    "32": {
      "título": "",
      "texto": "Os ingênuos são mortos"
    },
    "33": {
      "título": "",
      "texto": "Mas o que me der ouvidos"
    }
  }
}