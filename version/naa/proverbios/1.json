    "titulos": {
        "1": "Uso dos provérbios",
        "8": "Contra as seduções dos pecadores",
        "20": "O convite da Sabedoria"
    },
    "versiculos": {
        "1": "Provérbios de Salomão,",
        "2": "para aprender a sabedoria",
        "3": "para obter o ensino",
        "4": "para dar prudência aos simples",
        "5": "Que o sábio ouça",
        "6": "para entender provérbios",
        "7": "O temor do Senhor",
        "8": "Meu filho, ouça o ensino",
        "9": "Porque serão um diadema",
        "10": "Meu filho, se os pecadores",
        "11": "Talvez eles digam:",
        "12": "Vamos engoli-los vivos,",
        "13": "Acharemos todo tipo",
        "14": "Junte-se a nós!",
        "15": "Meu filho, não se ponha",
        "16": "Porque os pés deles",
        "17": "Pois em vão se estende a rede",
        "18": "mas estes armam emboscadas",
        "19": "Este é o fim de todo ganancioso;",
        "20": "A Sabedoria grita nas ruas;",
        "21": "Do alto das muralhas clama,",
        "22": "“Até quando vocês, ingênuos,",
        "23": "Deem ouvidos",
        "24": "Mas porque clamei,",
        "25": "— pelo contrário, rejeitaram",
        "26": "também eu darei risada",
        "27": "quando o terror chegar",
        "28": "Então eles me invocarão,",
        "29": "Porque odiaram o conhecimento",
        "30": "não quiseram o meu conselho",
        "31": "Portanto, comerão do fruto",
        "32": "Os ingênuos são mortos",
        "33": "Mas o que me der ouvidos"
    }
}