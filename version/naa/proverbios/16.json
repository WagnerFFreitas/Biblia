{
  "livro": "Provérbios",
  "capítulo": 16,
  "versículos": {
    "1": {
      "título": "",
      "texto": "O coração do ser humano"
    },
    "2": {
      "título": "",
      "texto": "Todos os caminhos"
    },
    "3": {
      "título": "",
      "texto": "Entregue as suas obras"
    },
    "4": {
      "título": "",
      "texto": "O Senhor fez todas as coisas"
    },
    "5": {
      "título": "",
      "texto": "O Senhor detesta todo aquele"
    },
    "6": {
      "título": "",
      "texto": "Pela misericórdia e pela verdade"
    },
    "7": {
      "título": "",
      "texto": "Se os caminhos de alguém"
    },
    "8": {
      "título": "",
      "texto": "Melhor é o pouco, havendo justiça,"
    },
    "9": {
      "título": "",
      "texto": "O coração do ser humano"
    },
    "10": {
      "título": "",
      "texto": "Nos lábios do rei se acham"
    },
    "11": {
      "título": "",
      "texto": "Peso e balança justos"
    },
    "12": {
      "título": "",
      "texto": "Os reis detestam"
    },
    "13": {
      "título": "",
      "texto": "Os lábios justos"
    },
    "14": {
      "título": "",
      "texto": "O furor do rei é como"
    },
    "15": {
      "título": "",
      "texto": "O semblante alegre do rei"
    },
    "16": {
      "título": "",
      "texto": "Quanto melhor é adquirir"
    },
    "17": {
      "título": "",
      "texto": "O caminho dos retos"
    },
    "18": {
      "título": "",
      "texto": "Antes da ruína vem a soberba,"
    },
    "19": {
      "título": "",
      "texto": "Melhor é ser humilde de espírito"
    },
    "20": {
      "título": "",
      "texto": "Quem atenta para o ensino"
    },
    "21": {
      "título": "",
      "texto": "O sábio de coração"
    },
    "22": {
      "título": "",
      "texto": "O bom senso, para aqueles"
    },
    "23": {
      "título": "",
      "texto": "O coração do sábio"
    },
    "24": {
      "título": "",
      "texto": "Palavras agradáveis"
    },
    "25": {
      "título": "",
      "texto": "Há caminho que parece"
    },
    "26": {
      "título": "",
      "texto": "A fome do trabalhador"
    },
    "27": {
      "título": "",
      "texto": "O desprezível cava o mal,"
    },
    "28": {
      "título": "",
      "texto": "O perverso semeia discórdias,"
    },
    "29": {
      "título": "",
      "texto": "O violento alicia"
    },
    "30": {
      "título": "",
      "texto": "Quem pisca os olhos"
    },
    "31": {
      "título": "",
      "texto": "Os cabelos brancos"
    },
    "32": {
      "título": "",
      "texto": "É melhor ter paciência"
    },
    "33": {
      "título": "",
      "texto": "Para fazer um sorteio"
    }
  }
}