{
  "livro": "Provérbios",
  "capítulo": 5,
  "versículos": {
    "1": {
      "título": "Advertência contra a imoralidade",
      "texto": "Meu filho, dê atenção"
    },
    "2": {
      "título": "",
      "texto": "para que você conserve"
    },
    "3": {
      "título": "",
      "texto": "Porque os lábios"
    },
    "4": {
      "título": "",
      "texto": "mas o seu fim é amargo como fel,"
    },
    "5": {
      "título": "",
      "texto": "Os seus pés descem"
    },
    "6": {
      "título": "",
      "texto": "Ela não faz plana"
    },
    "7": {
      "título": "",
      "texto": "E agora, meu filho,"
    },
    "8": {
      "título": "",
      "texto": "Afaste o seu caminho"
    },
    "9": {
      "título": "",
      "texto": "para que você não dê a outros"
    },
    "10": {
      "título": "",
      "texto": "para que os estranhos"
    },
    "11": {
      "título": "",
      "texto": "No fim de sua vida"
    },
    "12": {
      "título": "",
      "texto": "Então você dirá: “Como foi"
    },
    "13": {
      "título": "",
      "texto": "Não escutei a voz"
    },
    "14": {
      "título": "",
      "texto": "Quase caí em ruína completa"
    },
    "15": {
      "título": "",
      "texto": "Beba a água"
    },
    "16": {
      "título": "",
      "texto": "Por que você derramaria"
    },
    "17": {
      "título": "",
      "texto": "Que sejam para você somente"
    },
    "18": {
      "título": "",
      "texto": "Seja bendito o seu manancial,"
    },
    "19": {
      "título": "",
      "texto": "corça amorosa e gazela graciosa."
    },
    "20": {
      "título": "",
      "texto": "Meu filho, por que você andaria"
    },
    "21": {
      "título": "",
      "texto": "Porque os caminhos do homem"
    },
    "22": {
      "título": "",
      "texto": "Quanto ao ímpio,"
    },
    "23": {
      "título": "",
      "texto": "Ele morrerá"
    }
  }
}