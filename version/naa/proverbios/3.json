{
  "livro": "Provérbios",
  "capítulo": 3,
  "versículos": {
    "1": {
      "título": "Conselhos da Sabedoria aos moços",
      "texto": "Meu filho, não se esqueça"
    },
    "2": {
      "título": "",
      "texto": "porque eles aumentarão"
    },
    "3": {
      "título": "",
      "texto": "Não deixe que a bondade"
    },
    "4": {
      "título": "",
      "texto": "e você encontrará favor"
    },
    "5": {
      "título": "",
      "texto": "Confie no Senhor"
    },
    "6": {
      "título": "",
      "texto": "Reconheça o Senhor em todos"
    },
    "7": {
      "título": "",
      "texto": "Não seja sábio"
    },
    "8": {
      "título": "",
      "texto": "Isto será como um remédio"
    },
    "9": {
      "título": "",
      "texto": "Honre o Senhor com os seus bens"
    },
    "10": {
      "título": "",
      "texto": "e os seus celeiros ficarão"
    },
    "11": {
      "título": "",
      "texto": "Meu filho, não rejeite"
    },
    "12": {
      "título": "",
      "texto": "Porque o Senhor repreende"
    },
    "13": {
      "título": "",
      "texto": "Feliz é quem acha"
    },
    "14": {
      "título": "",
      "texto": "Porque o lucro"
    },
    "15": {
      "título": "",
      "texto": "A sabedoria é mais preciosa"
    },
    "16": {
      "título": "",
      "texto": "Em sua mão direita"
    },
    "17": {
      "título": "",
      "texto": "Os seus caminhos"
    },
    "18": {
      "título": "",
      "texto": "Ela é árvore de vida"
    },
    "19": {
      "título": "",
      "texto": "O Senhor com sabedoria lançou"
    },
    "20": {
      "título": "",
      "texto": "Pelo seu conhecimento"
    },
    "21": {
      "título": "",
      "texto": "Meu filho, que estas coisas"
    },
    "22": {
      "título": "",
      "texto": "porque serão vida para a sua alma"
    },
    "23": {
      "título": "",
      "texto": "Então você andará seguro"
    },
    "24": {
      "título": "",
      "texto": "Quando se deitar,"
    },
    "25": {
      "título": "",
      "texto": "Não temerá o pavor repentino,"
    },
    "26": {
      "título": "",
      "texto": "Porque o Senhor"
    },
    "27": {
      "título": "",
      "texto": "Não deixe de fazer o bem"
    },
    "28": {
      "título": "",
      "texto": "Não diga ao seu próximo:"
    },
    "29": {
      "título": "",
      "texto": "Não planeje nenhum mal"
    },
    "30": {
      "título": "",
      "texto": "Não entre em litígio"
    },
    "31": {
      "título": "",
      "texto": "Não tenha inveja"
    },
    "32": {
      "título": "",
      "texto": "porque o Senhor"
    },
    "33": {
      "título": "",
      "texto": "A maldição do Senhor está"
    },
    "34": {
      "título": "",
      "texto": "Certamente ele zomba"
    },
    "35": {
      "título": "",
      "texto": "Os sábios herdarão honra,"
    }
  }
}