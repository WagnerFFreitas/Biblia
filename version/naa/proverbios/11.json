{
  "livro": "Provérbios",
  "capítulo": 11,
  "versículos": {
    "1": {
      "título": "",
      "texto": "O Senhor detesta"
    },
    "2": {
      "título": "",
      "texto": "Quando vem a soberba,"
    },
    "3": {
      "título": "",
      "texto": "A integridade dos retos os guia,"
    },
    "4": {
      "título": "",
      "texto": "As riquezas não servem para nada"
    },
    "5": {
      "título": "",
      "texto": "A justiça do íntegro"
    },
    "6": {
      "título": "",
      "texto": "A justiça dos retos os livrará,"
    },
    "7": {
      "título": "",
      "texto": "Quando morre o ímpio,"
    },
    "8": {
      "título": "",
      "texto": "O justo é libertado da angústia,"
    },
    "9": {
      "título": "",
      "texto": "O ímpio destrói o próximo"
    },
    "10": {
      "título": "",
      "texto": "A cidade se alegra"
    },
    "11": {
      "título": "",
      "texto": "Pela bênção dos retos"
    },
    "12": {
      "título": "",
      "texto": "Quem fala mal do seu próximo"
    },
    "13": {
      "título": "",
      "texto": "O mexeriqueiro"
    },
    "14": {
      "título": "",
      "texto": "Não havendo direção sábia,"
    },
    "15": {
      "título": "",
      "texto": "Quem fica por fiador"
    },
    "16": {
      "título": "",
      "texto": "A mulher bondosa"
    },
    "17": {
      "título": "",
      "texto": "O homem bondoso"
    },
    "18": {
      "título": "",
      "texto": "O ímpio recebe"
    },
    "19": {
      "título": "",
      "texto": "Tão certo como a justiça"
    },
    "20": {
      "título": "",
      "texto": "O Senhor detesta"
    },
    "21": {
      "título": "",
      "texto": "É evidente que os maus"
    },
    "22": {
      "título": "",
      "texto": "Como joia de ouro"
    },
    "23": {
      "título": "",
      "texto": "O desejo dos justos tende"
    },
    "24": {
      "título": "",
      "texto": "Uns dão com generosidade"
    },
    "25": {
      "título": "",
      "texto": "A pessoa generosa prosperará,"
    },
    "26": {
      "título": "",
      "texto": "O povo amaldiçoa"
    },
    "27": {
      "título": "",
      "texto": "Quem procura o bem"
    },
    "28": {
      "título": "",
      "texto": "Quem confia"
    },
    "29": {
      "título": "",
      "texto": "Aquele que perturba a sua casa"
    },
    "30": {
      "título": "",
      "texto": "O fruto do justo é árvore de vida,"
    },
    "31": {
      "título": "",
      "texto": "Se o justo é punido na terra,"
    }
  }
}