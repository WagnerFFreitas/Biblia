{
  "livro": "Provérbios",
  "capítulo": 4,
  "versículos": {
    "1": {
      "título": "Conselho paternal",
      "texto": "Filhos, escutem"
    },
    "2": {
      "título": "",
      "texto": "Porque eu lhes dou"
    },
    "3": {
      "título": "",
      "texto": "Quando eu era menino"
    },
    "4": {
      "título": "",
      "texto": "ele me ensinava e me dizia:"
    },
    "5": {
      "título": "",
      "texto": "Adquira a sabedoria,"
    },
    "6": {
      "título": "",
      "texto": "Não abandone a sabedoria,"
    },
    "7": {
      "título": "",
      "texto": "O princípio da sabedoria é:"
    },
    "8": {
      "título": "",
      "texto": "Valorize a sabedoria,"
    },
    "9": {
      "título": "",
      "texto": "Ela porá um diadema de graça"
    },
    "10": {
      "título": "",
      "texto": "Meu filho, escute e aceite"
    },
    "11": {
      "título": "",
      "texto": "Eu ensinei a você"
    },
    "12": {
      "título": "",
      "texto": "Se você andar por elas,"
    },
    "13": {
      "título": "",
      "texto": "Retenha a instrução"
    },
    "14": {
      "título": "",
      "texto": "Não entre na vereda dos ímpios,"
    },
    "15": {
      "título": "",
      "texto": "Evite esse caminho;"
    },
    "16": {
      "título": "",
      "texto": "Os maus não dormem,"
    },
    "17": {
      "título": "",
      "texto": "Porque comem"
    },
    "18": {
      "título": "",
      "texto": "Mas a vereda dos justos"
    },
    "19": {
      "título": "",
      "texto": "O caminho dos ímpios"
    },
    "20": {
      "título": "",
      "texto": "Meu filho, escute"
    },
    "21": {
      "título": "",
      "texto": "Não deixe que eles se afastem"
    },
    "22": {
      "título": "",
      "texto": "Porque são vida"
    },
    "23": {
      "título": "",
      "texto": "De tudo o que se deve guardar,"
    },
    "24": {
      "título": "",
      "texto": "Afaste de você"
    },
    "25": {
      "título": "",
      "texto": "Que os seus olhos olhem direito,"
    },
    "26": {
      "título": "",
      "texto": "Faça plana a vereda de seus pés,"
    },
    "27": {
      "título": "",
      "texto": "Não se incline nem para a direita"
    }
  }
}