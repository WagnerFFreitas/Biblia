{
  "livro": "Cantares de Salomâo",
  "capítulo": 2,
  "versículos": {
    "1": {
      "título": "",
      "texto": "Eu sou a rosa de Sarom,"
    },
    "2": {
      "título": "Esposo",
      "texto": "Como um lírio entre os espinhos,"
    },
    "3": {
      "título": "Esposa",
      "texto": "Como a macieira"
    },
    "4": {
      "título": "",
      "texto": "Ele me levou à sala do banquete,"
    },
    "5": {
      "título": "",
      "texto": "Sustentem-me com passas,"
    },
    "6": {
      "título": "",
      "texto": "A sua mão esquerda"
    },
    "7": {
      "título": "",
      "texto": "Filhas de Jerusalém,"
    },
    "8": {
      "título": "Esposa",
      "texto": "Ouço a voz do meu amado."
    },
    "9": {
      "título": "",
      "texto": "O meu amado é semelhante"
    },
    "10": {
      "título": "",
      "texto": "O meu amado fala e me diz:"
    },
    "11": {
      "título": "Esposo",
      "texto": "Porque eis que passou o inverno,"
    },
    "12": {
      "título": "",
      "texto": "aparecem as flores na terra,"
    },
    "13": {
      "título": "",
      "texto": "A figueira começou"
    },
    "14": {
      "título": "",
      "texto": "Minha pombinha,"
    },
    "15": {
      "título": "Esposa",
      "texto": "Peguem as raposas,"
    },
    "16": {
      "título": "",
      "texto": "O meu amado é meu,"
    },
    "17": {
      "título": "",
      "texto": "Antes que rompa o dia"
    }
  }
}