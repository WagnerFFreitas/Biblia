{
  "livro": "Cantares de Salomâo",
  "capítulo": 6,
  "versículos": {
    "1": {
      "título": "Coro",
      "texto": "Para onde foi o seu amado,"
    },
    "2": {
      "título": "Esposa",
      "texto": "O meu amado desceu"
    },
    "3": {
      "título": "",
      "texto": "Eu sou do meu amado,"
    },
    "4": {
      "título": "Esposo",
      "texto": "Minha querida,"
    },
    "5": {
      "título": "",
      "texto": "Desvie de mim os seus olhos,"
    },
    "6": {
      "título": "",
      "texto": "Os seus dentes são como"
    },
    "7": {
      "título": "",
      "texto": "As suas faces, como romã partida,"
    },
    "8": {
      "título": "",
      "texto": "Sessenta são as rainhas,"
    },
    "9": {
      "título": "",
      "texto": "mas uma só é a minha pombinha"
    },
    "10": {
      "título": "Coro",
      "texto": "Quem é esta que aparece"
    },
    "11": {
      "título": "Esposa",
      "texto": "Desci ao jardim das nogueiras,"
    },
    "12": {
      "título": "",
      "texto": "Não sei como,"
    },
    "13": {
      "título": "Coro",
      "texto": "Volte, volte, sulamita!"
    }
  }
}