{
  "livro": "Isaías",
  "capítulo": 34,
  "versículos": {
    "1": {
      "título": "A indignação de Deus contra as nações",
      "texto": "Aproximem-se, ó nações,"
    },
    "2": {
      "título": "",
      "texto": "Porque a indignação do Senhor"
    },
    "3": {
      "título": "",
      "texto": "Os mortos deles"
    },
    "4": {
      "título": "",
      "texto": "Todo o exército dos céus"
    },
    "5": {
      "título": "",
      "texto": "Porque a minha espada"
    },
    "6": {
      "título": "",
      "texto": "A espada do Senhor"
    },
    "7": {
      "título": "",
      "texto": "Os bois selvagens"
    },
    "8": {
      "título": "",
      "texto": "Porque esse será"
    },
    "9": {
      "título": "",
      "texto": "Os ribeiros de Edom"
    },
    "10": {
      "título": "",
      "texto": "O fogo não se apagará"
    },
    "11": {
      "título": "",
      "texto": "O pelicano e o ouriço"
    },
    "12": {
      "título": "",
      "texto": "Já não haverá nobres"
    },
    "13": {
      "título": "",
      "texto": "Nos seus palácios,"
    },
    "14": {
      "título": "",
      "texto": "Os animais do deserto"
    },
    "15": {
      "título": "",
      "texto": "Ali a coruja fará o seu ninho,"
    },
    "16": {
      "título": "",
      "texto": "Procurem no livro do Senhor"
    },
    "17": {
      "título": "",
      "texto": "Porque ele lançou as sortes"
    }
  }
}