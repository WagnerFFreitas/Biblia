{
  "livro": "Isaías",
  "capítulo": 35,
  "versículos": {
    "1": {
      "título": "A felicidade na Sião futura",
      "texto": "O deserto e a terra seca"
    },
    "2": {
      "título": "",
      "texto": "Ele se cobrirá de flores,"
    },
    "3": {
      "título": "",
      "texto": "Fortaleçam as mãos frouxas"
    },
    "4": {
      "título": "",
      "texto": "Digam aos desalentados"
    },
    "5": {
      "título": "",
      "texto": "Então se abrirão os olhos"
    },
    "6": {
      "título": "",
      "texto": "os coxos saltarão como as corças,"
    },
    "7": {
      "título": "",
      "texto": "A areia escaldante"
    },
    "8": {
      "título": "",
      "texto": "E ali haverá uma estrada,"
    },
    "9": {
      "título": "",
      "texto": "Ali não haverá leão;"
    },
    "10": {
      "título": "",
      "texto": "Os resgatados do Senhor"
    }
  }
}