{
  "livro": "Isaías",
  "capítulo": 24,
  "versículos": {
    "1": {
      "título": "O Senhor vai castigar o mundo",
      "texto": "Eis que o Senhor vai devastar"
    },
    "2": {
      "título": "",
      "texto": "O mesmo vai acontecer com todos:"
    },
    "3": {
      "título": "",
      "texto": "A terra será completamente"
    },
    "4": {
      "título": "",
      "texto": "A terra pranteia e murcha;"
    },
    "5": {
      "título": "",
      "texto": "A terra está contaminada"
    },
    "6": {
      "título": "",
      "texto": "Por isso, a maldição"
    },
    "7": {
      "título": "",
      "texto": "O vinho pranteia,"
    },
    "8": {
      "título": "",
      "texto": "Cessou o som alegre"
    },
    "9": {
      "título": "",
      "texto": "Já não se bebe vinho"
    },
    "10": {
      "título": "",
      "texto": "A cidade caótica está demolida;"
    },
    "11": {
      "título": "",
      "texto": "Gritam por vinho nas ruas;"
    },
    "12": {
      "título": "",
      "texto": "Na cidade, só restou a desolação,"
    },
    "13": {
      "título": "",
      "texto": "O que acontecerá na terra,"
    },
    "14": {
      "título": "A alegria dos justos e a ruína dos transgressores",
      "texto": "Eles levantam a voz"
    },
    "15": {
      "título": "",
      "texto": "Por isso, no Oriente"
    },
    "16": {
      "título": "",
      "texto": "Dos confins da terra"
    },
    "17": {
      "título": "",
      "texto": "Terror, buracos e armadilhas"
    },
    "18": {
      "título": "",
      "texto": "Aquele que fugir da voz do terror"
    },
    "19": {
      "título": "",
      "texto": "A terra será totalmente quebrada,"
    },
    "20": {
      "título": "",
      "texto": "A terra vai cambalear"
    },
    "21": {
      "título": "",
      "texto": "Naquele dia, o Senhor castigará,"
    },
    "22": {
      "título": "",
      "texto": "Serão ajuntados"
    },
    "23": {
      "título": "",
      "texto": "A lua ficará corada de vergonha"
    }
  }
}