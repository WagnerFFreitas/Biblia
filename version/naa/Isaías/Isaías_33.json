{
  "livro": "Isaías",
  "capítulo": 33,
  "versículos": {
    "1": {
      "título": "A aflição e o livramento de Jerusalém",
      "texto": "Ai de você, destruidor"
    },
    "2": {
      "título": "",
      "texto": "Senhor , tem misericórdia de nós!"
    },
    "3": {
      "título": "",
      "texto": "Ao ruído do tumulto,"
    },
    "4": {
      "título": "",
      "texto": "Então o despojo"
    },
    "5": {
      "título": "",
      "texto": "O Senhor é sublime,"
    },
    "6": {
      "título": "",
      "texto": "Ó Sião, no seu tempo"
    },
    "7": {
      "título": "",
      "texto": "Eis que os heróis"
    },
    "8": {
      "título": "",
      "texto": "As estradas estão desoladas,"
    },
    "9": {
      "título": "",
      "texto": "A terra geme e desfalece;"
    },
    "10": {
      "título": "",
      "texto": "“Agora me levantarei”,"
    },
    "11": {
      "título": "",
      "texto": "Vocês conceberam palha"
    },
    "12": {
      "título": "",
      "texto": "Os povos serão queimados"
    },
    "13": {
      "título": "",
      "texto": "Vocês que estão longe,"
    },
    "14": {
      "título": "",
      "texto": "Em Sião, os pecadores"
    },
    "15": {
      "título": "",
      "texto": "Aquele que anda em justiça"
    },
    "16": {
      "título": "",
      "texto": "Este habitará nas alturas;"
    },
    "17": {
      "título": "",
      "texto": "Os olhos de vocês verão"
    },
    "18": {
      "título": "",
      "texto": "O seu coração se lembrará"
    },
    "19": {
      "título": "",
      "texto": "Você já não verá"
    },
    "20": {
      "título": "",
      "texto": "Olhe para Sião,"
    },
    "21": {
      "título": "",
      "texto": "Mas o Senhor ali"
    },
    "22": {
      "título": "",
      "texto": "Porque o Senhor é o nosso juiz,"
    },
    "23": {
      "título": "",
      "texto": "Agora as suas cordas"
    },
    "24": {
      "título": "",
      "texto": "Nenhum morador de Jerusalém"
    }
  }
}