{
  "livro": "Isaías",
  "capítulo": 26,
  "versículos": {
    "1": {
      "título": "Cântico de confiança na proteção divina",
      "texto": "Naquele dia, se entoará este cântico na terra de Judá:"
    },
    "2": {
      "título": "",
      "texto": "Abram os portões,"
    },
    "3": {
      "título": "",
      "texto": "Tu, Senhor , conservarás"
    },
    "4": {
      "título": "",
      "texto": "Confiem sempre no Senhor ,"
    },
    "5": {
      "título": "",
      "texto": "Ele derruba os que habitam"
    },
    "6": {
      "título": "",
      "texto": "O pé a pisará; os pés dos aflitos,"
    },
    "7": {
      "título": "",
      "texto": "A vereda do justo é plana;"
    },
    "8": {
      "título": "",
      "texto": "Também através dos teus juízos,"
    },
    "9": {
      "título": "",
      "texto": "Com minha alma suspiro"
    },
    "10": {
      "título": "",
      "texto": "Ainda que se mostre favor"
    },
    "11": {
      "título": "",
      "texto": "Senhor , a tua mão está levantada,"
    },
    "12": {
      "título": "",
      "texto": "Senhor , concede-nos a paz,"
    },
    "13": {
      "título": "",
      "texto": "Ó Senhor , nosso Deus,"
    },
    "14": {
      "título": "",
      "texto": "Eles estão mortos,"
    },
    "15": {
      "título": "",
      "texto": "Tu, Senhor , aumentaste o povo,"
    },
    "16": {
      "título": "",
      "texto": "Senhor , na angústia te buscaram;"
    },
    "17": {
      "título": "",
      "texto": "Como a mulher grávida"
    },
    "18": {
      "título": "",
      "texto": "Concebemos e nos contorcemos"
    },
    "19": {
      "título": "",
      "texto": "Os teus mortos"
    },
    "20": {
      "título": "Castigo e salvação",
      "texto": "Meu povo,"
    },
    "21": {
      "título": "",
      "texto": "Pois eis que o Senhor"
    }
  }
}