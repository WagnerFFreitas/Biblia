{
  "livro": "Isaías",
  "capítulo": 60,
  "versículos": {
    "1": {
      "título": "A glória da nova Jerusalém",
      "texto": "Levante-se, resplandeça,"
    },
    "2": {
      "título": "",
      "texto": "Porque eis que as trevas"
    },
    "3": {
      "título": "",
      "texto": "As nações se encaminham"
    },
    "4": {
      "título": "",
      "texto": "Levante os olhos ao redor e veja:"
    },
    "5": {
      "título": "",
      "texto": "Ao ver isso, você ficará"
    },
    "6": {
      "título": "",
      "texto": "O seu território ficará coberto"
    },
    "7": {
      "título": "",
      "texto": "Todas as ovelhas de Quedar"
    },
    "8": {
      "título": "",
      "texto": "Quem são estes que vêm voando"
    },
    "9": {
      "título": "",
      "texto": "Certamente as terras do mar"
    },
    "10": {
      "título": "",
      "texto": "“Estrangeiros edificarão"
    },
    "11": {
      "título": "",
      "texto": "Os seus portões"
    },
    "12": {
      "título": "",
      "texto": "Porque a nação e o reino"
    },
    "13": {
      "título": "",
      "texto": "“A glória do Líbano virá a você:"
    },
    "14": {
      "título": "",
      "texto": "Também virão e se inclinarão"
    },
    "15": {
      "título": "",
      "texto": "“Você era uma cidade"
    },
    "16": {
      "título": "",
      "texto": "Você mamará o leite das nações"
    },
    "17": {
      "título": "",
      "texto": "“Em vez de bronze,"
    },
    "18": {
      "título": "",
      "texto": "Nunca mais se ouvirá falar"
    },
    "19": {
      "título": "",
      "texto": "“Nunca mais o sol será a sua luz"
    },
    "20": {
      "título": "",
      "texto": "O seu sol nunca se porá,"
    },
    "21": {
      "título": "",
      "texto": "Todos os do seu povo"
    },
    "22": {
      "título": "",
      "texto": "O pequeno virá a ser mil,"
    }
  }
}