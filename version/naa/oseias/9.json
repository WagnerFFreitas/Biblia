{
    "livro": "Oséias",
    "capitulo": 9,
    "titulos": {
        "1": "O castigo de Israel"
    },
    "versiculos": {
        "1": "“Não se alegre, Israel,",
        "2": "A eira e o lagar",
        "3": "Não permanecerão",
        "4": "“Não oferecerão",
        "5": "“O que vocês farão",
        "6": "Porque eis que eles fugiram",
        "7": "“Chegaram os dias do castigo,",
        "8": "O profeta é sentinela",
        "9": "Eles se afundaram na corrupção,",
        "10": "“Encontrei Israel",
        "11": "Quanto a Efraim,",
        "12": "Ainda que venham",
        "13": "Aos meus olhos,",
        "14": "Dá-lhes, ó Senhor !",
        "15": "“Toda a maldade deles",
        "16": "“Efraim está ferido.",
        "17": "O meu Deus os rejeitará,"
    }
}