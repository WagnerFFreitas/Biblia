{
  "livro": "Oséias",
  "capítulo": 10,
  "versículos": {
    "1": {
      "título": "Israel semeou maldade e colherá destruição",
      "texto": "“Israel era uma videira"
    },
    "2": {
      "título": "",
      "texto": "O coração deles está dividido"
    },
    "3": {
      "título": "",
      "texto": "“Agora eles vão dizer:"
    },
    "4": {
      "título": "",
      "texto": "Falam palavras vãs,"
    },
    "5": {
      "título": "",
      "texto": "Os moradores de Samaria"
    },
    "6": {
      "título": "",
      "texto": "Também o bezerro"
    },
    "7": {
      "título": "",
      "texto": "“O rei de Samaria será"
    },
    "8": {
      "título": "",
      "texto": "Os lugares altos de Áven,"
    },
    "9": {
      "título": "Deus dá a sentença contra Israel",
      "texto": "“Desde os dias de Gibeá,"
    },
    "10": {
      "título": "",
      "texto": "Eu os castigarei"
    },
    "11": {
      "título": "",
      "texto": "Porque Efraim"
    },
    "12": {
      "título": "",
      "texto": "Então eu disse:"
    },
    "13": {
      "título": "",
      "texto": "“Vocês lavraram para a maldade,"
    },
    "14": {
      "título": "",
      "texto": "e por isso entre o seu povo"
    },
    "15": {
      "título": "",
      "texto": "Assim se fará com você, ó Betel,"
    }
  }
}