{
    "livro": "Salmos",
    "capitulo": 135,
    "titulos": {
        "1": "Louvores a Deus"
    },
    "versiculos": {
        "1": "Aleluia!",
        "2": "que estão na Casa do Senhor ,",
        "3": "Louvem o Senhor ,",
        "4": "Pois o Senhor escolheu Jacó",
        "5": "De fato, eu sei",
        "6": "Tudo o que agrada ao Senhor ,",
        "7": "Faz subir as nuvens",
        "8": "Foi ele quem matou",
        "9": "Foi ele quem fez sinais",
        "10": "Ele destruiu muitas nações",
        "11": "Seom, rei dos amorreus,",
        "12": "E a terra deles deu em herança,",
        "13": "O teu nome, Senhor ,",
        "14": "Pois o Senhor julga o seu povo",
        "15": "Os ídolos das nações",
        "16": "Têm boca e não falam;",
        "17": "têm ouvidos e não ouvem;",
        "18": "Tornam-se semelhantes a eles",
        "19": "Casa de Israel,",
        "20": "Casa de Levi, bendigam o Senhor !",
        "21": "Desde Sião"
    }
}