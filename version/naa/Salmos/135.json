{
  "livro": "Salmos",
  "capítulo": 135,
  "versículos": {
    "1": {
      "título": "Louvores a Deus",
      "texto": "Aleluia!"
    },
    "2": {
      "título": "",
      "texto": "que estão na Casa do Senhor ,"
    },
    "3": {
      "título": "",
      "texto": "Louvem o Senhor ,"
    },
    "4": {
      "título": "",
      "texto": "Pois o Senhor escolheu Jacó"
    },
    "5": {
      "título": "",
      "texto": "De fato, eu sei"
    },
    "6": {
      "título": "",
      "texto": "Tudo o que agrada ao Senhor ,"
    },
    "7": {
      "título": "",
      "texto": "Faz subir as nuvens"
    },
    "8": {
      "título": "",
      "texto": "Foi ele quem matou"
    },
    "9": {
      "título": "",
      "texto": "Foi ele quem fez sinais"
    },
    "10": {
      "título": "",
      "texto": "Ele destruiu muitas nações"
    },
    "11": {
      "título": "",
      "texto": "Seom, rei dos amorreus,"
    },
    "12": {
      "título": "",
      "texto": "E a terra deles deu em herança,"
    },
    "13": {
      "título": "",
      "texto": "O teu nome, Senhor ,"
    },
    "14": {
      "título": "",
      "texto": "Pois o Senhor julga o seu povo"
    },
    "15": {
      "título": "",
      "texto": "Os ídolos das nações"
    },
    "16": {
      "título": "",
      "texto": "Têm boca e não falam;"
    },
    "17": {
      "título": "",
      "texto": "têm ouvidos e não ouvem;"
    },
    "18": {
      "título": "",
      "texto": "Tornam-se semelhantes a eles"
    },
    "19": {
      "título": "",
      "texto": "Casa de Israel,"
    },
    "20": {
      "título": "",
      "texto": "Casa de Levi, bendigam o Senhor !"
    },
    "21": {
      "título": "",
      "texto": "Desde Sião"
    }
  }
}