{
  "livro": "Salmos",
  "capítulo": 55,
  "versículos": {
    "1": {
      "título": "Confiança em meio à perseguição",
      "texto": "Dá ouvidos, ó Deus,"
    },
    "2": {
      "título": "",
      "texto": "Atende-me e responde-me;"
    },
    "3": {
      "título": "",
      "texto": "por causa do clamor do inimigo"
    },
    "4": {
      "título": "",
      "texto": "O meu coração"
    },
    "5": {
      "título": "",
      "texto": "Temor e tremor me sobrevêm,"
    },
    "6": {
      "título": "",
      "texto": "Então eu disse: “Quem me dera"
    },
    "7": {
      "título": "",
      "texto": "Eis que fugiria para longe"
    },
    "8": {
      "título": "",
      "texto": "Depressa eu me abrigaria"
    },
    "9": {
      "título": "",
      "texto": "Destrói, Senhor, e confunde"
    },
    "10": {
      "título": "",
      "texto": "Dia e noite andam em volta dela,"
    },
    "11": {
      "título": "",
      "texto": "Há destruição"
    },
    "12": {
      "título": "",
      "texto": "Porque não é um inimigo"
    },
    "13": {
      "título": "",
      "texto": "mas é você, homem meu igual,"
    },
    "14": {
      "título": "",
      "texto": "Juntos nos entretínhamos"
    },
    "15": {
      "título": "",
      "texto": "Que a morte os assalte,"
    },
    "16": {
      "título": "",
      "texto": "Eu, porém, invocarei a Deus,"
    },
    "17": {
      "título": "",
      "texto": "À tarde, pela manhã"
    },
    "18": {
      "título": "",
      "texto": "Em paz ele livra a minha alma"
    },
    "19": {
      "título": "",
      "texto": "Deus ouvirá e lhes responderá,"
    },
    "20": {
      "título": "",
      "texto": "Tal homem estendeu as mãos"
    },
    "21": {
      "título": "",
      "texto": "A sua fala era mais macia"
    },
    "22": {
      "título": "",
      "texto": "Lance os seus cuidados"
    },
    "23": {
      "título": "",
      "texto": "Tu, porém, ó Deus, os lançarás"
    }
  }
}