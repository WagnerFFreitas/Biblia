{
  "livro": "Salmos",
  "capítulo": 115,
  "versículos": {
    "1": {
      "título": "Louvor ao verdadeiro Deus",
      "texto": "Não a nós, Senhor , não a nós,"
    },
    "2": {
      "título": "",
      "texto": "Por que diriam as nações:"
    },
    "3": {
      "título": "",
      "texto": "O nosso Deus está no céu"
    },
    "4": {
      "título": "",
      "texto": "Os ídolos das nações"
    },
    "5": {
      "título": "",
      "texto": "Têm boca e não falam;"
    },
    "6": {
      "título": "",
      "texto": "têm ouvidos e não ouvem;"
    },
    "7": {
      "título": "",
      "texto": "têm mãos e não apalpam;"
    },
    "8": {
      "título": "",
      "texto": "Tornem-se semelhantes a eles"
    },
    "9": {
      "título": "",
      "texto": "Ó Israel, confie no Senhor !"
    },
    "10": {
      "título": "",
      "texto": "Casa de Arão, confie no Senhor !"
    },
    "11": {
      "título": "",
      "texto": "Vocês que temem o Senhor ,"
    },
    "12": {
      "título": "",
      "texto": "O Senhor lembrou-se de nós;"
    },
    "13": {
      "título": "",
      "texto": "Ele abençoa"
    },
    "14": {
      "título": "",
      "texto": "O Senhor os abençoe"
    },
    "15": {
      "título": "",
      "texto": "Que vocês sejam abençoados"
    },
    "16": {
      "título": "",
      "texto": "Os céus são os céus do Senhor ,"
    },
    "17": {
      "título": "",
      "texto": "Os mortos não louvam o Senhor ,"
    },
    "18": {
      "título": "",
      "texto": "Nós, porém,"
    }
  }
}