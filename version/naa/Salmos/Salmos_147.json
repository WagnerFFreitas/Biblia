{
  "livro": "Salmos",
  "capítulo": 147,
  "versículos": {
    "1": {
      "título": "Louvor ao Deus poderoso",
      "texto": "Aleluia!"
    },
    "2": {
      "título": "",
      "texto": "O Senhor edifica Jerusalém"
    },
    "3": {
      "título": "",
      "texto": "Ele sara os que têm"
    },
    "4": {
      "título": "",
      "texto": "Conta o número das estrelas,"
    },
    "5": {
      "título": "",
      "texto": "Grande é o Senhor nosso"
    },
    "6": {
      "título": "",
      "texto": "O Senhor ampara os humildes,"
    },
    "7": {
      "título": "",
      "texto": "Cantem ao Senhor"
    },
    "8": {
      "título": "",
      "texto": "que cobre de nuvens o céu,"
    },
    "9": {
      "título": "",
      "texto": "e dá o alimento aos animais"
    },
    "10": {
      "título": "",
      "texto": "Não faz caso da força do cavalo,"
    },
    "11": {
      "título": "",
      "texto": "O Senhor se agrada"
    },
    "12": {
      "título": "",
      "texto": "Louve o Senhor , ó Jerusalém!"
    },
    "13": {
      "título": "",
      "texto": "Pois ele reforçou"
    },
    "14": {
      "título": "",
      "texto": "Estabeleceu a paz"
    },
    "15": {
      "título": "",
      "texto": "Ele envia as suas ordens à terra,"
    },
    "16": {
      "título": "",
      "texto": "Faz cair a neve como lã"
    },
    "17": {
      "título": "",
      "texto": "Faz cair o seu gelo"
    },
    "18": {
      "título": "",
      "texto": "Manda a sua palavra"
    },
    "19": {
      "título": "",
      "texto": "Anuncia a sua palavra a Jacó,"
    },
    "20": {
      "título": "",
      "texto": "Não fez assim"
    }
  }
}