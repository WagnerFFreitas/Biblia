{
  "livro": "Salmos",
  "capítulo": 94,
  "versículos": {
    "1": {
      "título": "Apelo para a justiça de Deus",
      "texto": "Ó Senhor , Deus das vinganças,"
    },
    "2": {
      "título": "",
      "texto": "Levanta-te, ó juiz da terra,"
    },
    "3": {
      "título": "",
      "texto": "Até quando, Senhor , os ímpios,"
    },
    "4": {
      "título": "",
      "texto": "Fazem alarde"
    },
    "5": {
      "título": "",
      "texto": "Esmagam o teu povo, Senhor ,"
    },
    "6": {
      "título": "",
      "texto": "Matam as viúvas e os estrangeiros"
    },
    "7": {
      "título": "",
      "texto": "E dizem:"
    },
    "8": {
      "título": "",
      "texto": "Prestem atenção,"
    },
    "9": {
      "título": "",
      "texto": "Aquele que fez o ouvido"
    },
    "10": {
      "título": "",
      "texto": "Aquele que repreende as nações"
    },
    "11": {
      "título": "",
      "texto": "O Senhor conhece"
    },
    "12": {
      "título": "",
      "texto": "Bem-aventurado, Senhor ,"
    },
    "13": {
      "título": "",
      "texto": "para lhe dares descanso"
    },
    "14": {
      "título": "",
      "texto": "Pois o Senhor não abandonará"
    },
    "15": {
      "título": "",
      "texto": "Nos tribunais voltará a imperar"
    },
    "16": {
      "título": "",
      "texto": "Quem se levantará a meu favor"
    },
    "17": {
      "título": "",
      "texto": "Se não fosse o auxílio do Senhor ,"
    },
    "18": {
      "título": "",
      "texto": "Quando eu digo:"
    },
    "19": {
      "título": "",
      "texto": "Multiplicando-se em mim"
    },
    "20": {
      "título": "",
      "texto": "Será que pode associar-se"
    },
    "21": {
      "título": "",
      "texto": "Ajuntam-se"
    },
    "22": {
      "título": "",
      "texto": "Mas o Senhor"
    },
    "23": {
      "título": "",
      "texto": "Sobre eles faz recair"
    }
  }
}