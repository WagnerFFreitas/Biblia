{
  "livro": "Salmos",
  "capítulo": 105,
  "versículos": {
    "1": {
      "título": "Deus e o seu povo",
      "texto": "Deem graças ao Senhor ,"
    },
    "2": {
      "título": "",
      "texto": "Cantem a Deus,"
    },
    "3": {
      "título": "",
      "texto": "Gloriem-se no seu santo nome;"
    },
    "4": {
      "título": "",
      "texto": "Busquem o Senhor e o seu poder;"
    },
    "5": {
      "título": "",
      "texto": "Lembrem-se das maravilhas"
    },
    "6": {
      "título": "",
      "texto": "vocês, descendentes de Abraão,"
    },
    "7": {
      "título": "",
      "texto": "Ele é o Senhor , nosso Deus;"
    },
    "8": {
      "título": "",
      "texto": "Lembra-se perpetuamente"
    },
    "9": {
      "título": "",
      "texto": "da aliança que fez com Abraão"
    },
    "10": {
      "título": "",
      "texto": "o qual confirmou a Jacó"
    },
    "11": {
      "título": "",
      "texto": "dizendo: “Eu lhe darei"
    },
    "12": {
      "título": "",
      "texto": "Quando eles eram"
    },
    "13": {
      "título": "",
      "texto": "quando andavam"
    },
    "14": {
      "título": "",
      "texto": "Deus não permitiu"
    },
    "15": {
      "título": "",
      "texto": "dizendo: “Não toquem"
    },
    "16": {
      "título": "",
      "texto": "Deus fez vir fome sobre a terra"
    },
    "17": {
      "título": "",
      "texto": "Adiante deles enviou um homem,"
    },
    "18": {
      "título": "",
      "texto": "Apertaram os seus pés"
    },
    "19": {
      "título": "",
      "texto": "até cumprir-se a profecia"
    },
    "20": {
      "título": "",
      "texto": "O rei mandou soltá-lo;"
    },
    "21": {
      "título": "",
      "texto": "Constituiu-o senhor de sua casa"
    },
    "22": {
      "título": "",
      "texto": "para, como bem quisesse,"
    },
    "23": {
      "título": "",
      "texto": "Então Israel entrou no Egito,"
    },
    "24": {
      "título": "",
      "texto": "Deus fez sobremodo fecundo"
    },
    "25": {
      "título": "",
      "texto": "Mudou o coração dos egípcios"
    },
    "26": {
      "título": "",
      "texto": "Deus lhes enviou Moisés,"
    },
    "27": {
      "título": "",
      "texto": "por meio dos quais fez,"
    },
    "28": {
      "título": "",
      "texto": "Enviou trevas, e tudo escureceu;"
    },
    "29": {
      "título": "",
      "texto": "Transformou-lhes"
    },
    "30": {
      "título": "",
      "texto": "A terra deles produziu rãs"
    },
    "31": {
      "título": "",
      "texto": "Deus falou, e vieram"
    },
    "32": {
      "título": "",
      "texto": "Por chuva deu-lhes granizo"
    },
    "33": {
      "título": "",
      "texto": "Devastou-lhes os vinhedos"
    },
    "34": {
      "título": "",
      "texto": "Ele falou, e vieram gafanhotos"
    },
    "35": {
      "título": "",
      "texto": "que devoraram"
    },
    "36": {
      "título": "",
      "texto": "Também feriu de morte"
    },
    "37": {
      "título": "",
      "texto": "Então Deus fez sair o seu povo,"
    },
    "38": {
      "título": "",
      "texto": "O Egito se alegrou"
    },
    "39": {
      "título": "",
      "texto": "Deus estendeu uma nuvem"
    },
    "40": {
      "título": "",
      "texto": "Pediram,"
    },
    "41": {
      "título": "",
      "texto": "Fendeu a rocha,"
    },
    "42": {
      "título": "",
      "texto": "Porque estava lembrado"
    },
    "43": {
      "título": "",
      "texto": "Ele conduziu o seu povo"
    },
    "44": {
      "título": "",
      "texto": "Deu-lhes as terras das nações,"
    },
    "45": {
      "título": "",
      "texto": "para que lhe guardassem"
    }
  }
}