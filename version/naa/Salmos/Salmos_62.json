{
  "livro": "Salmos",
  "capítulo": 62,
  "versículos": {
    "1": {
      "título": "Esperança somente em Deus",
      "texto": "Somente em Deus a minha alma"
    },
    "2": {
      "título": "",
      "texto": "Só ele é a minha rocha,"
    },
    "3": {
      "título": "",
      "texto": "Até quando vocês atacarão"
    },
    "4": {
      "título": "",
      "texto": "Só pensam em derrubá-lo"
    },
    "5": {
      "título": "",
      "texto": "Somente em Deus, ó minha alma,"
    },
    "6": {
      "título": "",
      "texto": "Só ele é a minha rocha,"
    },
    "7": {
      "título": "",
      "texto": "De Deus dependem"
    },
    "8": {
      "título": "",
      "texto": "Confie nele em todo tempo,"
    },
    "9": {
      "título": "",
      "texto": "Pura vaidade"
    },
    "10": {
      "título": "",
      "texto": "Não confiem na opressão,"
    },
    "11": {
      "título": "",
      "texto": "Uma vez Deus falou,"
    },
    "12": {
      "título": "",
      "texto": "e a ti, Senhor, pertence a graça,"
    }
  }
}