{
  "livro": "Salmos",
  "capítulo": 71,
  "versículos": {
    "1": {
      "título": "Súplicas de um ancião",
      "texto": "Em ti, Senhor , me refugio;"
    },
    "2": {
      "título": "",
      "texto": "Livra-me por tua justiça"
    },
    "3": {
      "título": "",
      "texto": "Sê tu para mim"
    },
    "4": {
      "título": "",
      "texto": "Livra-me, Deus meu,"
    },
    "5": {
      "título": "",
      "texto": "Pois tu és a minha esperança,"
    },
    "6": {
      "título": "",
      "texto": "Em ti eu tenho me apoiado"
    },
    "7": {
      "título": "",
      "texto": "Para muitos sou motivo"
    },
    "8": {
      "título": "",
      "texto": "Os meus lábios estão repletos"
    },
    "9": {
      "título": "",
      "texto": "Não me rejeites"
    },
    "10": {
      "título": "",
      "texto": "Pois os meus inimigos"
    },
    "11": {
      "título": "",
      "texto": "dizendo: “Deus o abandonou."
    },
    "12": {
      "título": "",
      "texto": "Ó Deus, não te ausentes de mim;"
    },
    "13": {
      "título": "",
      "texto": "Que sejam envergonhados"
    },
    "14": {
      "título": "",
      "texto": "Quanto a mim, esperarei sempre"
    },
    "15": {
      "título": "",
      "texto": "A minha boca proclamará"
    },
    "16": {
      "título": "",
      "texto": "Irei na força do Senhor Deus ;"
    },
    "17": {
      "título": "",
      "texto": "Tu me tens ensinado, ó Deus,"
    },
    "18": {
      "título": "",
      "texto": "Não me desampares, ó Deus,"
    },
    "19": {
      "título": "",
      "texto": "A tua justiça, ó Deus,"
    },
    "20": {
      "título": "",
      "texto": "Tu, que me tens feito ver"
    },
    "21": {
      "título": "",
      "texto": "Aumenta a minha grandeza"
    },
    "22": {
      "título": "",
      "texto": "Eu também te louvo com a lira"
    },
    "23": {
      "título": "",
      "texto": "Os meus lábios exultarão"
    },
    "24": {
      "título": "",
      "texto": "Igualmente a minha língua"
    }
  }
}