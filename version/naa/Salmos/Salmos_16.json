{
  "livro": "Salmos",
  "capítulo": 16,
  "versículos": {
    "1": {
      "título": "O Santo de Deus",
      "texto": "Guarda-me, ó Deus,"
    },
    "2": {
      "título": "",
      "texto": "Digo ao Senhor :"
    },
    "3": {
      "título": "",
      "texto": "Quanto aos santos"
    },
    "4": {
      "título": "",
      "texto": "Muitas serão as dores"
    },
    "5": {
      "título": "",
      "texto": "O Senhor é a porção"
    },
    "6": {
      "título": "",
      "texto": "As minhas divisas caíram"
    },
    "7": {
      "título": "",
      "texto": "Bendigo o Senhor ,"
    },
    "8": {
      "título": "",
      "texto": "Tenho o Senhor sempre"
    },
    "9": {
      "título": "",
      "texto": "Por isso o meu coração se alegra"
    },
    "10": {
      "título": "",
      "texto": "Pois não deixarás"
    },
    "11": {
      "título": "",
      "texto": "Tu me farás ver"
    }
  }
}