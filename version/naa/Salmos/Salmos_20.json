{
  "livro": "Salmos",
  "capítulo": 20,
  "versículos": {
    "1": {
      "título": "Oração pela vitória",
      "texto": "Que o Senhor lhe responda"
    },
    "2": {
      "título": "",
      "texto": "Que do seu santuário"
    },
    "3": {
      "título": "",
      "texto": "Que ele se lembre de todas"
    },
    "4": {
      "título": "",
      "texto": "Que Deus lhe conceda"
    },
    "5": {
      "título": "",
      "texto": "Celebraremos com júbilo"
    },
    "6": {
      "título": "",
      "texto": "Agora sei que o Senhor"
    },
    "7": {
      "título": "",
      "texto": "Uns confiam em carros de guerra,"
    },
    "8": {
      "título": "",
      "texto": "Eles se prostram e caem;"
    },
    "9": {
      "título": "",
      "texto": "Ó Senhor , dá vitória ao rei;"
    }
  }
}