{
    "livro": "Salmos",
    "capitulo": 139,
    "titulos": {
        "1": "Deus onisciente e onipotente"
    },
    "versiculos": {
        "1": "Senhor , tu me sondas",
        "2": "Sabes quando me sento",
        "3": "Observas o meu andar",
        "4": "A palavra ainda nem chegou",
        "5": "Tu me cercas por todos os lados",
        "6": "Tal conhecimento é maravilhoso",
        "7": "Para onde me ausentarei",
        "8": "Se subo aos céus, lá estás;",
        "9": "se tomo as asas da alvorada",
        "10": "ainda ali a tua mão",
        "11": "Se eu digo: “As trevas,",
        "12": "até as próprias trevas",
        "13": "Pois tu formaste o meu interior,",
        "14": "Graças te dou, visto que de modo",
        "15": "Os meus ossos",
        "16": "Os teus olhos viram",
        "17": "Que preciosos para mim, ó Deus,",
        "18": "Se os contasse, seriam mais",
        "19": "Como eu gostaria, ó Deus,",
        "20": "Eles se rebelam contra ti",
        "21": "Acaso não odeio",
        "22": "Eu os detesto",
        "23": "Sonda-me, ó Deus,",
        "24": "vê se há em mim"
    }
}