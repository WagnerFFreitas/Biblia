{
  "livro": "Salmos",
  "capítulo": 141,
  "versículos": {
    "1": {
      "título": "Oração vespertina",
      "texto": "Senhor , eu clamo a ti;"
    },
    "2": {
      "título": "",
      "texto": "Suba à tua presença"
    },
    "3": {
      "título": "",
      "texto": "Põe guarda à minha boca, Senhor ;"
    },
    "4": {
      "título": "",
      "texto": "Não permitas que o meu coração"
    },
    "5": {
      "título": "",
      "texto": "Fira-me o justo,"
    },
    "6": {
      "título": "",
      "texto": "Quando os seus juízes"
    },
    "7": {
      "título": "",
      "texto": "Como quando se lavra"
    },
    "8": {
      "título": "",
      "texto": "Pois em ti, ó Deus , meu Senhor,"
    },
    "9": {
      "título": "",
      "texto": "Guarda-me dos laços"
    },
    "10": {
      "título": "",
      "texto": "Que os ímpios caiam"
    }
  }
}