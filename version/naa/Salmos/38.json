{
    "livro": "Salmos",
    "capitulo": 38,
    "titulos": {
        "1": "Oração de um pecador aflito"
    },
    "versiculos": {
        "1": "Não me repreendas, Senhor ,",
        "2": "Cravam-se em mim as tuas setas,",
        "3": "Não há parte sã na minha carne,",
        "4": "Pois já se elevam acima",
        "5": "Tenho feridas que cheiram mal",
        "6": "Sinto-me encurvado",
        "7": "Os meus lombos ardem,",
        "8": "Estou aflito e mui quebrantado;",
        "9": "Na tua presença, Senhor,",
        "10": "O meu coração bate acelerado,",
        "11": "Os meus amigos e companheiros",
        "12": "Armam ciladas contra mim",
        "13": "Mas eu, como surdo, não ouço",
        "14": "Sou como quem não ouve",
        "15": "Pois em ti, Senhor , espero;",
        "16": "Porque eu dizia: “Não deixes",
        "17": "Pois estou prestes a tropeçar;",
        "18": "Confesso a minha iniquidade;",
        "19": "Mas os meus inimigos",
        "20": "Aqueles que pagam",
        "21": "Não me desampares, Senhor ;",
        "22": "Apressa-te em socorrer-me,"
    }
}