{
  "livro": "Salmos",
  "capítulo": 59,
  "versículos": {
    "1": {
      "título": "Súplica por libertação",
      "texto": "Livra-me, Deus meu,"
    },
    "2": {
      "título": "",
      "texto": "Livra-me dos que praticam"
    },
    "3": {
      "título": "",
      "texto": "Pois eis que armam ciladas"
    },
    "4": {
      "título": "",
      "texto": "Sem culpa minha,"
    },
    "5": {
      "título": "",
      "texto": "Tu, Senhor , Deus dos Exércitos,"
    },
    "6": {
      "título": "",
      "texto": "Ao anoitecer, uivam como cães,"
    },
    "7": {
      "título": "",
      "texto": "Proferem ameaças;"
    },
    "8": {
      "título": "",
      "texto": "Mas tu, Senhor , vais rir deles;"
    },
    "9": {
      "título": "",
      "texto": "Em ti, força minha, esperarei;"
    },
    "10": {
      "título": "",
      "texto": "Meu Deus virá ao meu encontro"
    },
    "11": {
      "título": "",
      "texto": "Não os mates, para que"
    },
    "12": {
      "título": "",
      "texto": "Pelo pecado de sua boca,"
    },
    "13": {
      "título": "",
      "texto": "Consome-os com indignação,"
    },
    "14": {
      "título": "",
      "texto": "Ao anoitecer, uivam como cães,"
    },
    "15": {
      "título": "",
      "texto": "Vagueiam à procura de comida"
    },
    "16": {
      "título": "",
      "texto": "Eu, porém, cantarei a tua força;"
    },
    "17": {
      "título": "",
      "texto": "A ti, força minha,"
    }
  }
}