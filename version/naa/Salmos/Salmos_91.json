{
  "livro": "Salmos",
  "capítulo": 91,
  "versículos": {
    "1": {
      "título": "Sob a sombra do Altíssimo",
      "texto": "Aquele que habita"
    },
    "2": {
      "título": "",
      "texto": "diz ao Senhor :"
    },
    "3": {
      "título": "",
      "texto": "Pois ele livrará você"
    },
    "4": {
      "título": "",
      "texto": "Ele o cobrirá com as suas penas,"
    },
    "5": {
      "título": "",
      "texto": "Você não terá medo"
    },
    "6": {
      "título": "",
      "texto": "nem da peste"
    },
    "7": {
      "título": "",
      "texto": "Caiam mil ao seu lado,"
    },
    "8": {
      "título": "",
      "texto": "Somente com os seus olhos"
    },
    "9": {
      "título": "",
      "texto": "Você disse:"
    },
    "10": {
      "título": "",
      "texto": "Nenhum mal lhe sucederá,"
    },
    "11": {
      "título": "",
      "texto": "Porque aos seus anjos"
    },
    "12": {
      "título": "",
      "texto": "Eles o sustentarão"
    },
    "13": {
      "título": "",
      "texto": "Você pisará o leão e a cobra;"
    },
    "14": {
      "título": "",
      "texto": "Deus diz: “Porque a mim"
    },
    "15": {
      "título": "",
      "texto": "Ele me invocará,"
    },
    "16": {
      "título": "",
      "texto": "Vou saciá-lo com longevidade"
    }
  }
}