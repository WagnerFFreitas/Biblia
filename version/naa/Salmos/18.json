{
  "livro": "Salmos",
  "capítulo": 18,
  "versículos": {
    "1": {
      "título": "Cântico de vitória",
      "texto": "Eu te amo, ó Senhor ,"
    },
    "2": {
      "título": "",
      "texto": "O Senhor é a minha rocha,"
    },
    "3": {
      "título": "",
      "texto": "Invoco o Senhor ,"
    },
    "4": {
      "título": "",
      "texto": "Laços de morte me cercaram;"
    },
    "5": {
      "título": "",
      "texto": "Cadeias infernais me envolveram,"
    },
    "6": {
      "título": "",
      "texto": "Na minha angústia,"
    },
    "7": {
      "título": "",
      "texto": "Então a terra se abalou e tremeu;"
    },
    "8": {
      "título": "",
      "texto": "Das suas narinas subiu fumaça,"
    },
    "9": {
      "título": "",
      "texto": "Ele baixou os céus e desceu,"
    },
    "10": {
      "título": "",
      "texto": "Cavalgava um querubim e voou;"
    },
    "11": {
      "título": "",
      "texto": "Das trevas fez um manto"
    },
    "12": {
      "título": "",
      "texto": "Do resplendor"
    },
    "13": {
      "título": "",
      "texto": "O Senhor trovejou nos céus;"
    },
    "14": {
      "título": "",
      "texto": "Atirou as suas flechas"
    },
    "15": {
      "título": "",
      "texto": "Então se viu o leito das águas,"
    },
    "16": {
      "título": "",
      "texto": "Do alto o Senhor me estendeu"
    },
    "17": {
      "título": "",
      "texto": "Livrou-me de forte inimigo"
    },
    "18": {
      "título": "",
      "texto": "Eles me atacaram no dia"
    },
    "19": {
      "título": "",
      "texto": "Trouxe-me"
    },
    "20": {
      "título": "",
      "texto": "O Senhor me retribuiu"
    },
    "21": {
      "título": "",
      "texto": "Pois tenho guardado"
    },
    "22": {
      "título": "",
      "texto": "Porque todos os seus juízos"
    },
    "23": {
      "título": "",
      "texto": "Também fui íntegro para com ele"
    },
    "24": {
      "título": "",
      "texto": "Por isso, o Senhor me retribuiu"
    },
    "25": {
      "título": "",
      "texto": "Para com quem é fiel,"
    },
    "26": {
      "título": "",
      "texto": "Com o puro, puro te mostras;"
    },
    "27": {
      "título": "",
      "texto": "Porque tu salvas o povo humilde,"
    },
    "28": {
      "título": "",
      "texto": "Porque fazes resplandecer"
    },
    "29": {
      "título": "",
      "texto": "Pois contigo posso"
    },
    "30": {
      "título": "",
      "texto": "O caminho de Deus é perfeito;"
    },
    "31": {
      "título": "",
      "texto": "Pois quem é Deus"
    },
    "32": {
      "título": "",
      "texto": "O Deus que me revestiu de força"
    },
    "33": {
      "título": "",
      "texto": "ele deu aos meus pés"
    },
    "34": {
      "título": "",
      "texto": "Ele treinou as minhas mãos"
    },
    "35": {
      "título": "",
      "texto": "Também me deste"
    },
    "36": {
      "título": "",
      "texto": "Alargaste o caminho"
    },
    "37": {
      "título": "",
      "texto": "Persegui os meus inimigos"
    },
    "38": {
      "título": "",
      "texto": "Esmaguei-os a tal ponto,"
    },
    "39": {
      "título": "",
      "texto": "Pois me cingiste de força"
    },
    "40": {
      "título": "",
      "texto": "Também puseste em fuga"
    },
    "41": {
      "título": "",
      "texto": "Gritaram por socorro,"
    },
    "42": {
      "título": "",
      "texto": "Então os reduzi a pó,"
    },
    "43": {
      "título": "",
      "texto": "Dos conflitos do povo"
    },
    "44": {
      "título": "",
      "texto": "Bastou-lhe ouvir a minha voz,"
    },
    "45": {
      "título": "",
      "texto": "Os estrangeiros fraquejaram"
    },
    "46": {
      "título": "",
      "texto": "O Senhor vive!"
    },
    "47": {
      "título": "",
      "texto": "o Deus que por mim"
    },
    "48": {
      "título": "",
      "texto": "o Deus que me livrou"
    },
    "49": {
      "título": "",
      "texto": "Por isso, eu te glorificarei"
    },
    "50": {
      "título": "",
      "texto": "É ele quem dá grandes vitórias"
    }
  }
}