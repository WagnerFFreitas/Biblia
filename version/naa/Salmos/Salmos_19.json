{
  "livro": "Salmos",
  "capítulo": 19,
  "versículos": {
    "1": {
      "título": "A excelência da criação e da palavra de Deus",
      "texto": "Os céus proclamam"
    },
    "2": {
      "título": "",
      "texto": "Um dia discursa a outro dia,"
    },
    "3": {
      "título": "",
      "texto": "Não há linguagem,"
    },
    "4": {
      "título": "",
      "texto": "No entanto, por toda a terra"
    },
    "5": {
      "título": "",
      "texto": "que é como um noivo"
    },
    "6": {
      "título": "",
      "texto": "Principia numa extremidade"
    },
    "7": {
      "título": "",
      "texto": "A lei do Senhor é perfeita"
    },
    "8": {
      "título": "",
      "texto": "Os preceitos do Senhor são retos"
    },
    "9": {
      "título": "",
      "texto": "O temor do Senhor é límpido"
    },
    "10": {
      "título": "",
      "texto": "São mais desejáveis do que ouro,"
    },
    "11": {
      "título": "",
      "texto": "Além disso, por eles"
    },
    "12": {
      "título": "",
      "texto": "Quem há que possa discernir"
    },
    "13": {
      "título": "",
      "texto": "Também da soberba"
    },
    "14": {
      "título": "",
      "texto": "As palavras dos meus lábios"
    }
  }
}