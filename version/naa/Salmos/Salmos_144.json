{
  "livro": "Salmos",
  "capítulo": 144,
  "versículos": {
    "1": {
      "título": "Gratidão pela proteção de Deus",
      "texto": "Bendito seja o Senhor ,"
    },
    "2": {
      "título": "",
      "texto": "Ele é a minha misericórdia"
    },
    "3": {
      "título": "",
      "texto": "Senhor , que é o homem"
    },
    "4": {
      "título": "",
      "texto": "O ser humano é como um sopro;"
    },
    "5": {
      "título": "",
      "texto": "Abaixa, Senhor ,"
    },
    "6": {
      "título": "",
      "texto": "Manda relâmpagos"
    },
    "7": {
      "título": "",
      "texto": "Estende a mão lá do alto;"
    },
    "8": {
      "título": "",
      "texto": "cuja boca profere mentiras,"
    },
    "9": {
      "título": "",
      "texto": "A ti, ó Deus, entoarei"
    },
    "10": {
      "título": "",
      "texto": "É ele quem dá aos reis a vitória;"
    },
    "11": {
      "título": "",
      "texto": "Livra-me e salva-me"
    },
    "12": {
      "título": "",
      "texto": "Que os nossos filhos"
    },
    "13": {
      "título": "",
      "texto": "Que os nossos celeiros"
    },
    "14": {
      "título": "",
      "texto": "Que o nosso gado seja fértil,"
    },
    "15": {
      "título": "",
      "texto": "Bem-aventurado o povo"
    }
  }
}