{
  "livro": "Salmos",
  "capítulo": 64,
  "versículos": {
    "1": {
      "título": "Pedido de proteção contra inimigos",
      "texto": "Ouve, ó Deus, a minha voz"
    },
    "2": {
      "título": "",
      "texto": "Esconde-me da conspiração"
    },
    "3": {
      "título": "",
      "texto": "Eles afiam a língua como espada"
    },
    "4": {
      "título": "",
      "texto": "para, às escondidas,"
    },
    "5": {
      "título": "",
      "texto": "Teimam no mau propósito;"
    },
    "6": {
      "título": "",
      "texto": "Planejam iniquidades e dizem:"
    },
    "7": {
      "título": "",
      "texto": "Mas Deus atira contra eles"
    },
    "8": {
      "título": "",
      "texto": "Assim, serão levados a tropeçar;"
    },
    "9": {
      "título": "",
      "texto": "Todas as pessoas temerão,"
    },
    "10": {
      "título": "",
      "texto": "O justo se alegra no Senhor"
    }
  }
}