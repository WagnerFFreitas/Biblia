{
    "livro": "Salmos",
    "capitulo": 114,
    "titulos": {
        "1": "As maravilhas do êxodo"
    },
    "versiculos": {
        "1": "Quando Israel saiu do Egito,",
        "2": "Judá se tornou",
        "3": "O mar viu isso e fugiu;",
        "4": "Os montes saltaram",
        "5": "O que lhe aconteceu, ó mar,",
        "6": "Montes, por que estão saltando",
        "7": "Trema, ó terra,",
        "8": "Ele transformou a rocha"
    }
}