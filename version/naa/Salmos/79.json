{
  "livro": "Salmos",
  "capítulo": 79,
  "versículos": {
    "1": {
      "título": "Apelo à misericórdia divina",
      "texto": "Ó Deus, as nações invadiram"
    },
    "2": {
      "título": "",
      "texto": "Deram os cadáveres"
    },
    "3": {
      "título": "",
      "texto": "Derramaram como água"
    },
    "4": {
      "título": "",
      "texto": "Tornamo-nos objeto de deboche"
    },
    "5": {
      "título": "",
      "texto": "Até quando, Senhor ?"
    },
    "6": {
      "título": "",
      "texto": "Derrama o teu furor"
    },
    "7": {
      "título": "",
      "texto": "Porque eles devoraram Jacó"
    },
    "8": {
      "título": "",
      "texto": "Não nos faças pagar"
    },
    "9": {
      "título": "",
      "texto": "Ajuda-nos, ó Deus"
    },
    "10": {
      "título": "",
      "texto": "Por que diriam as nações:"
    },
    "11": {
      "título": "",
      "texto": "Chegue à tua presença"
    },
    "12": {
      "título": "",
      "texto": "Retribui, Senhor,"
    },
    "13": {
      "título": "",
      "texto": "Quanto a nós, teu povo"
    }
  }
}