{
  "livro": "Salmos",
  "capítulo": 102,
  "versículos": {
    "1": {
      "título": "Oração de um aflito",
      "texto": "Ouve, Senhor , a minha súplica,"
    },
    "2": {
      "título": "",
      "texto": "Não escondas de mim o teu rosto"
    },
    "3": {
      "título": "",
      "texto": "Porque os meus dias"
    },
    "4": {
      "título": "",
      "texto": "Cortado como a erva,"
    },
    "5": {
      "título": "",
      "texto": "Os meus ossos"
    },
    "6": {
      "título": "",
      "texto": "Sou como o pelicano no deserto,"
    },
    "7": {
      "título": "",
      "texto": "Não durmo"
    },
    "8": {
      "título": "",
      "texto": "Os meus inimigos"
    },
    "9": {
      "título": "",
      "texto": "Por pão tenho comido cinza"
    },
    "10": {
      "título": "",
      "texto": "por causa da tua indignação"
    },
    "11": {
      "título": "",
      "texto": "Como a sombra que declina,"
    },
    "12": {
      "título": "",
      "texto": "Tu, porém, Senhor ,"
    },
    "13": {
      "título": "",
      "texto": "Tu te levantarás"
    },
    "14": {
      "título": "",
      "texto": "Porque os teus servos"
    },
    "15": {
      "título": "",
      "texto": "Todas as nações"
    },
    "16": {
      "título": "",
      "texto": "quando o Senhor reconstruir Sião"
    },
    "17": {
      "título": "",
      "texto": "quando atender"
    },
    "18": {
      "título": "",
      "texto": "Isto ficará registrado"
    },
    "19": {
      "título": "",
      "texto": "“O Senhor , do alto"
    },
    "20": {
      "título": "",
      "texto": "a fim de ouvir"
    },
    "21": {
      "título": "",
      "texto": "Em Sião será anunciado"
    },
    "22": {
      "título": "",
      "texto": "quando se reunirem"
    },
    "23": {
      "título": "",
      "texto": "Ele me abateu"
    },
    "24": {
      "título": "",
      "texto": "Eu disse: “Deus meu, não me leves"
    },
    "25": {
      "título": "",
      "texto": "Em tempos remotos, lançaste"
    },
    "26": {
      "título": "",
      "texto": "Eles perecerão,"
    },
    "27": {
      "título": "",
      "texto": "Tu, porém, és sempre o mesmo,"
    },
    "28": {
      "título": "",
      "texto": "Os filhos dos teus servos"
    }
  }
}