{
  "livro": "Salmos",
  "capítulo": 61,
  "versículos": {
    "1": {
      "título": "Confiança na proteção de Deus",
      "texto": "Ouve, ó Deus, a minha súplica;"
    },
    "2": {
      "título": "",
      "texto": "Desde os confins da terra"
    },
    "3": {
      "título": "",
      "texto": "pois tu tens sido o meu refúgio"
    },
    "4": {
      "título": "",
      "texto": "Que eu possa habitar"
    },
    "5": {
      "título": "",
      "texto": "Pois ouviste, ó Deus,"
    },
    "6": {
      "título": "",
      "texto": "Dias sobre dias acrescentas ao rei;"
    },
    "7": {
      "título": "",
      "texto": "Que ele permaneça para sempre"
    },
    "8": {
      "título": "",
      "texto": "Assim, cantarei louvores"
    }
  }
}