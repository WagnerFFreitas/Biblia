{
    "livro": "Salmos",
    "capitulo": 16,
    "titulos": {
        "1": "O Santo de Deus"
    },
    "versiculos": {
        "1": "Guarda-me, ó Deus,",
        "2": "Digo ao Senhor :",
        "3": "Quanto aos santos",
        "4": "Muitas serão as dores",
        "5": "O Senhor é a porção",
        "6": "As minhas divisas caíram",
        "7": "Bendigo o Senhor ,",
        "8": "Tenho o Senhor sempre",
        "9": "Por isso o meu coração se alegra",
        "10": "Pois não deixarás",
        "11": "Tu me farás ver"
    }
}