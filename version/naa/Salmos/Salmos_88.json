{
  "livro": "Salmos",
  "capítulo": 88,
  "versículos": {
    "1": {
      "título": "Oração de um sofredor",
      "texto": "Ó Senhor , Deus da minha"
    },
    "2": {
      "título": "",
      "texto": "Chegue à tua presença"
    },
    "3": {
      "título": "",
      "texto": "Pois a minha alma"
    },
    "4": {
      "título": "",
      "texto": "Sou contado"
    },
    "5": {
      "título": "",
      "texto": "atirado entre os mortos;"
    },
    "6": {
      "título": "",
      "texto": "Puseste-me"
    },
    "7": {
      "título": "",
      "texto": "Sobre mim pesa a tua ira;"
    },
    "8": {
      "título": "",
      "texto": "Afastaste de mim"
    },
    "9": {
      "título": "",
      "texto": "Os meus olhos desfalecem"
    },
    "10": {
      "título": "",
      "texto": "Será que farás maravilhas"
    },
    "11": {
      "título": "",
      "texto": "A tua bondade será anunciada"
    },
    "12": {
      "título": "",
      "texto": "Acaso nas trevas se manifestam"
    },
    "13": {
      "título": "",
      "texto": "Mas eu, Senhor ,"
    },
    "14": {
      "título": "",
      "texto": "Por que rejeitas, Senhor ,"
    },
    "15": {
      "título": "",
      "texto": "Ando aflito e prestes a morrer"
    },
    "16": {
      "título": "",
      "texto": "Sobre mim passou a tua ira;"
    },
    "17": {
      "título": "",
      "texto": "O dia todo eles me rodeiam"
    },
    "18": {
      "título": "",
      "texto": "Para longe de mim afastaste"
    }
  }
}