{
  "livro": "Salmos",
  "capítulo": 63,
  "versículos": {
    "1": {
      "título": "Sede de Deus",
      "texto": "Ó Deus, tu és o meu Deus;"
    },
    "2": {
      "título": "",
      "texto": "Assim, quero ver-te no santuário,"
    },
    "3": {
      "título": "",
      "texto": "Porque a tua graça"
    },
    "4": {
      "título": "",
      "texto": "Assim, eu te bendirei"
    },
    "5": {
      "título": "",
      "texto": "Como de saborosa comida,"
    },
    "6": {
      "título": "",
      "texto": "no meu leito,"
    },
    "7": {
      "título": "",
      "texto": "Porque tu tens sido o meu auxílio;"
    },
    "8": {
      "título": "",
      "texto": "A minha alma apega-se a ti;"
    },
    "9": {
      "título": "",
      "texto": "Porém os que procuram"
    },
    "10": {
      "título": "",
      "texto": "Serão entregues"
    },
    "11": {
      "título": "",
      "texto": "Mas o rei se alegrará em Deus;"
    }
  }
}