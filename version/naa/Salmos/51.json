{
  "livro": "Salmos",
  "capítulo": 51,
  "versículos": {
    "1": {
      "título": "Confissão e arrependimento",
      "texto": "Compadece-te de mim, ó Deus,"
    },
    "2": {
      "título": "",
      "texto": "Lava-me completamente"
    },
    "3": {
      "título": "",
      "texto": "Pois eu conheço"
    },
    "4": {
      "título": "",
      "texto": "Pequei contra ti,"
    },
    "5": {
      "título": "",
      "texto": "Eu nasci na iniquidade,"
    },
    "6": {
      "título": "",
      "texto": "Eis que te agradas"
    },
    "7": {
      "título": "",
      "texto": "Purifica-me com hissopo,"
    },
    "8": {
      "título": "",
      "texto": "Faze-me ouvir júbilo e alegria,"
    },
    "9": {
      "título": "",
      "texto": "Esconde o teu rosto"
    },
    "10": {
      "título": "",
      "texto": "Cria em mim, ó Deus,"
    },
    "11": {
      "título": "",
      "texto": "Não me lances fora"
    },
    "12": {
      "título": "",
      "texto": "Restitui-me a alegria"
    },
    "13": {
      "título": "",
      "texto": "Então ensinarei"
    },
    "14": {
      "título": "",
      "texto": "Livra-me dos crimes"
    },
    "15": {
      "título": "",
      "texto": "Abre, Senhor, os meus lábios,"
    },
    "16": {
      "título": "",
      "texto": "Pois não te agradas"
    },
    "17": {
      "título": "",
      "texto": "Sacrifício agradável a Deus"
    },
    "18": {
      "título": "",
      "texto": "Faze bem a Sião,"
    },
    "19": {
      "título": "",
      "texto": "Então te agradarás"
    }
  }
}