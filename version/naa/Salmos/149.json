{
    "livro": "Salmos",
    "capitulo": 149,
    "titulos": {
        "1": "Hino de louvor"
    },
    "versiculos": {
        "1": "Aleluia!",
        "2": "Alegre-se Israel no seu Criador;",
        "3": "Louvem o nome do Senhor",
        "4": "Porque o Senhor",
        "5": "Que os santos exultem de glória,",
        "6": "Nos seus lábios estejam",
        "7": "para exercer vingança",
        "8": "para prender os seus reis",
        "9": "para executar contra eles"
    }
}