{
  "livro": "Salmos",
  "capítulo": 68,
  "versículos": {
    "1": {
      "título": "A vitória de Deus sobre os inimigos",
      "texto": "Deus se levanta; os seus inimigos"
    },
    "2": {
      "título": "",
      "texto": "Como se dissipa a fumaça,"
    },
    "3": {
      "título": "",
      "texto": "Os justos, porém, se alegram;"
    },
    "4": {
      "título": "",
      "texto": "Cantem a Deus, cantem louvores"
    },
    "5": {
      "título": "",
      "texto": "Pai dos órfãos e juiz das viúvas"
    },
    "6": {
      "título": "",
      "texto": "Deus faz com que o solitário"
    },
    "7": {
      "título": "",
      "texto": "Ao saíres, ó Deus,"
    },
    "8": {
      "título": "",
      "texto": "a terra tremeu;"
    },
    "9": {
      "título": "",
      "texto": "Chuva abundante derramaste,"
    },
    "10": {
      "título": "",
      "texto": "Aí habitou o teu povo;"
    },
    "11": {
      "título": "",
      "texto": "O Senhor deu a palavra,"
    },
    "12": {
      "título": "",
      "texto": "“Reis de exércitos fogem!"
    },
    "13": {
      "título": "",
      "texto": "Por que estão repousando"
    },
    "14": {
      "título": "",
      "texto": "Quando o Todo-Poderoso ali"
    },
    "15": {
      "título": "",
      "texto": "Monte altíssimo"
    },
    "16": {
      "título": "",
      "texto": "Por que olham com inveja,"
    },
    "17": {
      "título": "",
      "texto": "Os carros de Deus são vinte mil,"
    },
    "18": {
      "título": "",
      "texto": "Subiste às alturas,"
    },
    "19": {
      "título": "",
      "texto": "Bendito seja o Senhor que,"
    },
    "20": {
      "título": "",
      "texto": "O nosso Deus é"
    },
    "21": {
      "título": "",
      "texto": "Sim, Deus parte a cabeça"
    },
    "22": {
      "título": "",
      "texto": "O Senhor disse:"
    },
    "23": {
      "título": "",
      "texto": "para que você banhe"
    },
    "24": {
      "título": "",
      "texto": "Viu-se, ó Deus, o teu cortejo,"
    },
    "25": {
      "título": "",
      "texto": "Os cantores iam na frente,"
    },
    "26": {
      "título": "",
      "texto": "Bendigam a Deus"
    },
    "27": {
      "título": "",
      "texto": "Ali está o mais novo,"
    },
    "28": {
      "título": "",
      "texto": "Reúne, ó Deus, a tua força,"
    },
    "29": {
      "título": "",
      "texto": "oriunda do teu templo"
    },
    "30": {
      "título": "",
      "texto": "Reprime a fera dos canaviais,"
    },
    "31": {
      "título": "",
      "texto": "Príncipes vêm do Egito;"
    },
    "32": {
      "título": "",
      "texto": "Reinos da terra, cantem a Deus,"
    },
    "33": {
      "título": "",
      "texto": "àquele que vai montado"
    },
    "34": {
      "título": "",
      "texto": "Deem glória a Deus!"
    },
    "35": {
      "título": "",
      "texto": "Ó Deus, tu és tremendo"
    }
  }
}