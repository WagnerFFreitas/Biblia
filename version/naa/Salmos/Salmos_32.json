{
  "livro": "Salmos",
  "capítulo": 32,
  "versículos": {
    "1": {
      "título": "A bem-aventurança de quem recebe o perdão",
      "texto": "Bem-aventurado aquele"
    },
    "2": {
      "título": "",
      "texto": "Bem-aventurado é aquele"
    },
    "3": {
      "título": "",
      "texto": "Enquanto calei os meus pecados,"
    },
    "4": {
      "título": "",
      "texto": "Porque a tua mão pesava"
    },
    "5": {
      "título": "",
      "texto": "Confessei-te o meu pecado"
    },
    "6": {
      "título": "",
      "texto": "Sendo assim,"
    },
    "7": {
      "título": "",
      "texto": "Tu és o meu esconderijo;"
    },
    "8": {
      "título": "",
      "texto": "Eu o instruirei"
    },
    "9": {
      "título": "",
      "texto": "Não sejam"
    },
    "10": {
      "título": "",
      "texto": "Muitos são os sofrimentos"
    },
    "11": {
      "título": "",
      "texto": "Alegrem-se no Senhor"
    }
  }
}