{
    "livro": "Salmos",
    "capitulo": 39,
    "titulos": {
        "1": "A vaidade da vida"
    },
    "versiculos": {
        "1": "Eu disse comigo mesmo:",
        "2": "Emudeci em silêncio,",
        "3": "O coração me ardia no peito;",
        "4": "“ Senhor , dá-me a conhecer",
        "5": "Deste aos meus dias",
        "6": "De fato, o ser humano passa",
        "7": "“E eu, Senhor, que espero?",
        "8": "Livra-me de todas",
        "9": "Emudeço, não abro os lábios",
        "10": "Tira de sobre mim o teu flagelo;",
        "11": "Quando castigas alguém",
        "12": "“Ouve, Senhor , a minha oração,",
        "13": "Desvia de mim o olhar,"
    }
}