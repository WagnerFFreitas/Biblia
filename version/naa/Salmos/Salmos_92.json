{
  "livro": "Salmos",
  "capítulo": 92,
  "versículos": {
    "1": {
      "título": "Hino de gratidão a Deus",
      "texto": "Bom é render graças ao Senhor"
    },
    "2": {
      "título": "",
      "texto": "anunciar de manhã"
    },
    "3": {
      "título": "",
      "texto": "com instrumentos de dez cordas,"
    },
    "4": {
      "título": "",
      "texto": "Pois me alegraste, Senhor ,"
    },
    "5": {
      "título": "",
      "texto": "Como são grandes, Senhor ,"
    },
    "6": {
      "título": "",
      "texto": "O tolo não compreende,"
    },
    "7": {
      "título": "",
      "texto": "ainda que os ímpios"
    },
    "8": {
      "título": "",
      "texto": "Mas tu, Senhor ,"
    },
    "9": {
      "título": "",
      "texto": "Eis que os teus inimigos, Senhor ,"
    },
    "10": {
      "título": "",
      "texto": "Porém tu exaltas o meu poder"
    },
    "11": {
      "título": "",
      "texto": "Os meus olhos veem a derrota"
    },
    "12": {
      "título": "",
      "texto": "O justo florescerá"
    },
    "13": {
      "título": "",
      "texto": "Plantados na Casa do Senhor ,"
    },
    "14": {
      "título": "",
      "texto": "Na velhice ainda darão frutos,"
    },
    "15": {
      "título": "",
      "texto": "para anunciar"
    }
  }
}