{
  "livro": "Salmos",
  "capítulo": 34,
  "versículos": {
    "1": {
      "título": "O Senhor é bom!",
      "texto": "Bendirei o Senhor"
    },
    "2": {
      "título": "",
      "texto": "A minha alma se gloriará"
    },
    "3": {
      "título": "",
      "texto": "Louvem comigo"
    },
    "4": {
      "título": "",
      "texto": "Busquei o Senhor ,"
    },
    "5": {
      "título": "",
      "texto": "Os que olham para ele"
    },
    "6": {
      "título": "",
      "texto": "Clamou este aflito,"
    },
    "7": {
      "título": "",
      "texto": "O anjo do Senhor acampa-se"
    },
    "8": {
      "título": "",
      "texto": "Provem e vejam"
    },
    "9": {
      "título": "",
      "texto": "Temam o Senhor ,"
    },
    "10": {
      "título": "",
      "texto": "Os leõezinhos"
    },
    "11": {
      "título": "",
      "texto": "Venham, meus filhos, e escutem;"
    },
    "12": {
      "título": "",
      "texto": "Quem de vocês"
    },
    "13": {
      "título": "",
      "texto": "Refreie a língua do mal"
    },
    "14": {
      "título": "",
      "texto": "Afaste-se do mal"
    },
    "15": {
      "título": "",
      "texto": "Os olhos do Senhor repousam"
    },
    "16": {
      "título": "",
      "texto": "O rosto do Senhor está contra"
    },
    "17": {
      "título": "",
      "texto": "Clamam os justos,"
    },
    "18": {
      "título": "",
      "texto": "Perto está o Senhor dos que têm"
    },
    "19": {
      "título": "",
      "texto": "Muitas são as aflições do justo,"
    },
    "20": {
      "título": "",
      "texto": "Preserva-lhe todos os ossos,"
    },
    "21": {
      "título": "",
      "texto": "A desgraça matará o ímpio,"
    },
    "22": {
      "título": "",
      "texto": "O Senhor resgata a alma"
    }
  }
