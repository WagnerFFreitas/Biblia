{
  "livro": "Salmos",
  "capítulo": 56,
  "versículos": {
    "1": {
      "título": "Oração de confiança em Deus",
      "texto": "Tem misericórdia de mim, ó Deus,"
    },
    "2": {
      "título": "",
      "texto": "Os meus inimigos sempre"
    },
    "3": {
      "título": "",
      "texto": "Quando eu ficar com medo,"
    },
    "4": {
      "título": "",
      "texto": "Em Deus, cuja palavra eu exalto,"
    },
    "5": {
      "título": "",
      "texto": "Todo o dia"
    },
    "6": {
      "título": "",
      "texto": "Ajuntam-se, escondem-se,"
    },
    "7": {
      "título": "",
      "texto": "Dá-lhes a retribuição"
    },
    "8": {
      "título": "",
      "texto": "Contaste os meus passos"
    },
    "9": {
      "título": "",
      "texto": "No dia em que eu te invocar,"
    },
    "10": {
      "título": "",
      "texto": "Em Deus, cuja palavra eu louvo,"
    },
    "11": {
      "título": "",
      "texto": "neste Deus ponho"
    },
    "12": {
      "título": "",
      "texto": "Os votos que fiz,"
    },
    "13": {
      "título": "",
      "texto": "Pois da morte livraste"
    }
  }
}