{
  "livro": "Salmos",
  "capítulo": 25,
  "versículos": {
    "1": {
      "título": "Oração por auxílio divino",
      "texto": "A ti, Senhor , elevo a minha alma."
    },
    "2": {
      "título": "",
      "texto": "Deus meu, em ti confio."
    },
    "3": {
      "título": "",
      "texto": "Na verdade,"
    },
    "4": {
      "título": "",
      "texto": "Faze-me conhecer"
    },
    "5": {
      "título": "",
      "texto": "Guia-me na tua verdade"
    },
    "6": {
      "título": "",
      "texto": "Lembra-te, Senhor ,"
    },
    "7": {
      "título": "",
      "texto": "Não te lembres dos meus"
    },
    "8": {
      "título": "",
      "texto": "Bom e reto é o Senhor ,"
    },
    "9": {
      "título": "",
      "texto": "Guia os humildes na justiça"
    },
    "10": {
      "título": "",
      "texto": "Todas as veredas do Senhor"
    },
    "11": {
      "título": "",
      "texto": "Por causa do teu nome, Senhor ,"
    },
    "12": {
      "título": "",
      "texto": "Àquele que teme o Senhor ,"
    },
    "13": {
      "título": "",
      "texto": "Na prosperidade"
    },
    "14": {
      "título": "",
      "texto": "O Senhor confia o seu segredo"
    },
    "15": {
      "título": "",
      "texto": "Os meus olhos se elevam"
    },
    "16": {
      "título": "",
      "texto": "Volta-te para mim"
    },
    "17": {
      "título": "",
      "texto": "Alivia-me as tribulações"
    },
    "18": {
      "título": "",
      "texto": "Considera as minhas aflições"
    },
    "19": {
      "título": "",
      "texto": "Considera os meus inimigos,"
    },
    "20": {
      "título": "",
      "texto": "Guarda a minha alma e livra-me;"
    },
    "21": {
      "título": "",
      "texto": "Que a sinceridade"
    },
    "22": {
      "título": "",
      "texto": "Ó Deus, redime Israel"
    }
  }
}