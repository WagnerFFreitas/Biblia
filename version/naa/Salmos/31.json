{
  "livro": "Salmos",
  "capítulo": 31,
  "versículos": {
    "1": {
      "título": "Lamento e louvor",
      "texto": "Em ti, Senhor , me refugio;"
    },
    "2": {
      "título": "",
      "texto": "Inclina-me os ouvidos,"
    },
    "3": {
      "título": "",
      "texto": "Porque tu és a minha rocha"
    },
    "4": {
      "título": "",
      "texto": "Tira-me do laço que,"
    },
    "5": {
      "título": "",
      "texto": "Nas tuas mãos entrego"
    },
    "6": {
      "título": "",
      "texto": "Tu detestas"
    },
    "7": {
      "título": "",
      "texto": "Eu me alegrarei"
    },
    "8": {
      "título": "",
      "texto": "e não me entregaste"
    },
    "9": {
      "título": "",
      "texto": "Compadece-te de mim, Senhor ,"
    },
    "10": {
      "título": "",
      "texto": "Gasta-se a minha vida"
    },
    "11": {
      "título": "",
      "texto": "Tornei-me objeto de deboche"
    },
    "12": {
      "título": "",
      "texto": "Estou esquecido"
    },
    "13": {
      "título": "",
      "texto": "Pois tenho ouvido"
    },
    "14": {
      "título": "",
      "texto": "Quanto a mim,"
    },
    "15": {
      "título": "",
      "texto": "Nas tuas mãos"
    },
    "16": {
      "título": "",
      "texto": "Faze resplandecer o teu rosto"
    },
    "17": {
      "título": "",
      "texto": "Não seja eu envergonhado,"
    },
    "18": {
      "título": "",
      "texto": "Emudeçam os lábios mentirosos,"
    },
    "19": {
      "título": "",
      "texto": "Como é grande a tua bondade,"
    },
    "20": {
      "título": "",
      "texto": "No recôndito da tua presença,"
    },
    "21": {
      "título": "",
      "texto": "Bendito seja o Senhor ,"
    },
    "22": {
      "título": "",
      "texto": "Eu disse na minha pressa:"
    },
    "23": {
      "título": "",
      "texto": "Amem o Senhor , todos vocês"
    },
    "24": {
      "título": "",
      "texto": "Sejam fortes,"
    }
  }
