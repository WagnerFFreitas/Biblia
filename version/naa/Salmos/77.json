{
  "livro": "Salmos",
  "capítulo": 77,
  "versículos": {
    "1": {
      "título": "Consolo em tempo de angústia",
      "texto": "Elevo a Deus a minha voz"
    },
    "2": {
      "título": "",
      "texto": "No dia da minha angústia,"
    },
    "3": {
      "título": "",
      "texto": "Lembro-me de Deus"
    },
    "4": {
      "título": "",
      "texto": "Não me deixas pregar os olhos;"
    },
    "5": {
      "título": "",
      "texto": "Penso nos dias de outrora,"
    },
    "6": {
      "título": "",
      "texto": "De noite indago o meu íntimo,"
    },
    "7": {
      "título": "",
      "texto": "“Será que o Senhor nos rejeitará"
    },
    "8": {
      "título": "",
      "texto": "Cessou perpetuamente"
    },
    "9": {
      "título": "",
      "texto": "Será que Deus se esqueceu"
    },
    "10": {
      "título": "",
      "texto": "Então eu disse:"
    },
    "11": {
      "título": "",
      "texto": "Recordarei os feitos do Senhor ;"
    },
    "12": {
      "título": "",
      "texto": "Meditarei em todas as tuas obras"
    },
    "13": {
      "título": "",
      "texto": "O teu caminho, ó Deus,"
    },
    "14": {
      "título": "",
      "texto": "Tu és o Deus"
    },
    "15": {
      "título": "",
      "texto": "Com o teu braço"
    },
    "16": {
      "título": "",
      "texto": "As águas te viram, ó Deus,"
    },
    "17": {
      "título": "",
      "texto": "Grossas nuvens"
    },
    "18": {
      "título": "",
      "texto": "O estrondo do teu trovão"
    },
    "19": {
      "título": "",
      "texto": "O teu caminho foi pelo mar;"
    },
    "20": {
      "título": "",
      "texto": "O teu povo, tu o conduziste,"
    }
  }
}