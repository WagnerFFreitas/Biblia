{
  "livro": "Salmos",
  "capítulo": 9,
  "versículos": {
    "1": {
      "título": "Louvor a Deus pela sua justiça",
      "texto": "Eu te louvarei, Senhor ,"
    },
    "2": {
      "título": "",
      "texto": "Em ti me alegrarei e exultarei;"
    },
    "3": {
      "título": "",
      "texto": "Pois, ao retrocederem"
    },
    "4": {
      "título": "",
      "texto": "Porque defendes o meu direito"
    },
    "5": {
      "título": "",
      "texto": "Tu repreendes as nações,"
    },
    "6": {
      "título": "",
      "texto": "Quanto aos inimigos,"
    },
    "7": {
      "título": "",
      "texto": "Mas o Senhor permanece"
    },
    "8": {
      "título": "",
      "texto": "Ele mesmo julga o mundo"
    },
    "9": {
      "título": "",
      "texto": "O Senhor é também"
    },
    "10": {
      "título": "",
      "texto": "Em ti, pois, confiam"
    },
    "11": {
      "título": "",
      "texto": "Cantem louvores ao Senhor ,"
    },
    "12": {
      "título": "",
      "texto": "Pois aquele que requer o sangue"
    },
    "13": {
      "título": "",
      "texto": "Compadece-te de mim, Senhor ;"
    },
    "14": {
      "título": "",
      "texto": "para que, às portas"
    },
    "15": {
      "título": "",
      "texto": "As nações se afundaram"
    },
    "16": {
      "título": "",
      "texto": "O Senhor se dá a conhecer"
    },
    "17": {
      "título": "",
      "texto": "No inferno serão lançados"
    },
    "18": {
      "título": "",
      "texto": "Pois o necessitado não será"
    },
    "19": {
      "título": "",
      "texto": "Levanta-te, Senhor ;"
    },
    "20": {
      "título": "",
      "texto": "Infunde-lhes o medo, Senhor ;"
    }
  }
}