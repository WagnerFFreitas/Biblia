{
    "livro": "Salmos",
    "capitulo": 143,
    "titulos": {
        "1": "Súplica por libertação"
    },
    "versiculos": {
        "1": "Ouve, Senhor , a minha oração,",
        "2": "Não entres em juízo",
        "3": "Pois o inimigo tem perseguido",
        "4": "Por isso, dentro de mim",
        "5": "Lembro-me dos dias de outrora,",
        "6": "A ti levanto as mãos;",
        "7": "Senhor , responde-me depressa!",
        "8": "Faze-me ouvir, pela manhã,",
        "9": "Livra-me, Senhor ,",
        "10": "Ensina-me a fazer a tua vontade,",
        "11": "Vivifica-me, Senhor ,",
        "12": "E, por tua misericórdia,"
    }
}