{
  "livro": "Salmos",
  "capítulo": 28,
  "versículos": {
    "1": {
      "título": "Súplica e ações de graças",
      "texto": "A ti clamo, ó Senhor ;"
    },
    "2": {
      "título": "",
      "texto": "Ouve a voz das minhas súplicas,"
    },
    "3": {
      "título": "",
      "texto": "Não me arrastes com os ímpios,"
    },
    "4": {
      "título": "",
      "texto": "Paga-lhes segundo as suas obras,"
    },
    "5": {
      "título": "",
      "texto": "E, visto que não compreendem"
    },
    "6": {
      "título": "",
      "texto": "Bendito seja o Senhor ,"
    },
    "7": {
      "título": "",
      "texto": "O Senhor é a minha força"
    },
    "8": {
      "título": "",
      "texto": "O Senhor é a força do seu povo,"
    },
    "9": {
      "título": "",
      "texto": "Salva o teu povo"
    }
  }
}