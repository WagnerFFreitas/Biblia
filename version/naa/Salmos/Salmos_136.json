{
  "livro": "Salmos",
  "capítulo": 136,
  "versículos": {
    "1": {
      "título": "A misericórdia de Deus",
      "texto": "Deem graças ao Senhor ,"
    },
    "2": {
      "título": "",
      "texto": "Deem graças"
    },
    "3": {
      "título": "",
      "texto": "Deem graças"
    },
    "4": {
      "título": "",
      "texto": "Ao único que opera"
    },
    "5": {
      "título": "",
      "texto": "Àquele que com entendimento"
    },
    "6": {
      "título": "",
      "texto": "Àquele que estendeu"
    },
    "7": {
      "título": "",
      "texto": "Àquele que fez"
    },
    "8": {
      "título": "",
      "texto": "Fez o sol para presidir o dia,"
    },
    "9": {
      "título": "",
      "texto": "Fez a lua e as estrelas"
    },
    "10": {
      "título": "",
      "texto": "Àquele que matou"
    },
    "11": {
      "título": "",
      "texto": "E tirou Israel do meio deles,"
    },
    "12": {
      "título": "",
      "texto": "Ele os tirou com mão poderosa"
    },
    "13": {
      "título": "",
      "texto": "Àquele que dividiu"
    },
    "14": {
      "título": "",
      "texto": "E fez Israel passar pelo meio dele,"
    },
    "15": {
      "título": "",
      "texto": "Mas lançou Faraó"
    },
    "16": {
      "título": "",
      "texto": "Àquele que conduziu"
    },
    "17": {
      "título": "",
      "texto": "Àquele que matou"
    },
    "18": {
      "título": "",
      "texto": "E tirou a vida de reis famosos,"
    },
    "19": {
      "título": "",
      "texto": "Matou Seom, rei dos amorreus,"
    },
    "20": {
      "título": "",
      "texto": "E matou Ogue, rei de Basã,"
    },
    "21": {
      "título": "",
      "texto": "E deu a terra deles em herança,"
    },
    "22": {
      "título": "",
      "texto": "Em herança a Israel, seu servo,"
    },
    "23": {
      "título": "",
      "texto": "Àquele que se lembrou de nós"
    },
    "24": {
      "título": "",
      "texto": "E nos libertou"
    },
    "25": {
      "título": "",
      "texto": "Ele dá alimento"
    },
    "26": {
      "título": "",
      "texto": "Deem louvores"
    }
  }
}