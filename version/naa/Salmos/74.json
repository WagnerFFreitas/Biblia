{
  "livro": "Salmos",
  "capítulo": 74,
  "versículos": {
    "1": {
      "título": "Oração pela libertação do povo",
      "texto": "Ó Deus, por que nos rejeitas"
    },
    "2": {
      "título": "",
      "texto": "Lembra-te da tua congregação,"
    },
    "3": {
      "título": "",
      "texto": "Dirige os teus passos"
    },
    "4": {
      "título": "",
      "texto": "Os teus adversários bramam"
    },
    "5": {
      "título": "",
      "texto": "Parecem-se com os que"
    },
    "6": {
      "título": "",
      "texto": "e agora, com os seus machados"
    },
    "7": {
      "título": "",
      "texto": "Incendeiam o teu santuário;"
    },
    "8": {
      "título": "",
      "texto": "Disseram no seu coração:"
    },
    "9": {
      "título": "",
      "texto": "Já não vemos os nossos sinais;"
    },
    "10": {
      "título": "",
      "texto": "Até quando, ó Deus, o adversário"
    },
    "11": {
      "título": "",
      "texto": "Por que retiras a tua mão,"
    },
    "12": {
      "título": "",
      "texto": "Mas Deus é meu Rei"
    },
    "13": {
      "título": "",
      "texto": "Tu, com o teu poder,"
    },
    "14": {
      "título": "",
      "texto": "Despedaçaste as cabeças"
    },
    "15": {
      "título": "",
      "texto": "Tu abriste fontes e ribeiros;"
    },
    "16": {
      "título": "",
      "texto": "Teu é o dia;"
    },
    "17": {
      "título": "",
      "texto": "Fixaste os confins da terra;"
    },
    "18": {
      "título": "",
      "texto": "Lembra-te disto: o inimigo"
    },
    "19": {
      "título": "",
      "texto": "Não entregues à rapina"
    },
    "20": {
      "título": "",
      "texto": "Lembra-te da tua aliança,"
    },
    "21": {
      "título": "",
      "texto": "Não fique envergonhado"
    },
    "22": {
      "título": "",
      "texto": "Levanta-te, ó Deus,"
    },
    "23": {
      "título": "",
      "texto": "Não te esqueças da gritaria"
    }
  }
}