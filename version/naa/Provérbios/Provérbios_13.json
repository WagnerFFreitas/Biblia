{
  "livro": "Provérbios",
  "capítulo": 13,
  "versículos": {
    "1": {
      "título": "",
      "texto": "O filho sábio ouve"
    },
    "2": {
      "título": "",
      "texto": "Do fruto da boca"
    },
    "3": {
      "título": "",
      "texto": "Quem vigia as suas palavras"
    },
    "4": {
      "título": "",
      "texto": "O preguiçoso deseja e nada tem,"
    },
    "5": {
      "título": "",
      "texto": "O justo odeia a mentira,"
    },
    "6": {
      "título": "",
      "texto": "A justiça guarda"
    },
    "7": {
      "título": "",
      "texto": "Uns se dizem ricos"
    },
    "8": {
      "título": "",
      "texto": "O resgate pela vida de alguém"
    },
    "9": {
      "título": "",
      "texto": "A luz dos justos"
    },
    "10": {
      "título": "",
      "texto": "Da soberba só resulta"
    },
    "11": {
      "título": "",
      "texto": "A riqueza obtida com facilidade,"
    },
    "12": {
      "título": "",
      "texto": "Esperança adiada"
    },
    "13": {
      "título": "",
      "texto": "Quem despreza a palavra"
    },
    "14": {
      "título": "",
      "texto": "O ensino do sábio"
    },
    "15": {
      "título": "",
      "texto": "O bom senso conquista favor,"
    },
    "16": {
      "título": "",
      "texto": "Quem é prudente"
    },
    "17": {
      "título": "",
      "texto": "O mensageiro perverso"
    },
    "18": {
      "título": "",
      "texto": "Pobreza e vergonha sobrevêm"
    },
    "19": {
      "título": "",
      "texto": "O desejo que se cumpre"
    },
    "20": {
      "título": "",
      "texto": "Quem anda com os sábios"
    },
    "21": {
      "título": "",
      "texto": "A desgraça persegue"
    },
    "22": {
      "título": "",
      "texto": "O homem bom deixa herança"
    },
    "23": {
      "título": "",
      "texto": "As terras dos pobres"
    },
    "24": {
      "título": "",
      "texto": "O que retém a vara"
    },
    "25": {
      "título": "",
      "texto": "O justo tem o bastante"
    }
  }
}