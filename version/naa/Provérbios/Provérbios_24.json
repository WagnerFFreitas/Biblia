{
  "livro": "Provérbios",
  "capítulo": 24,
  "versículos": {
    "1": {
      "título": "— 19 —",
      "texto": "Não tenha inveja dos maus"
    },
    "2": {
      "título": "",
      "texto": "porque o coração deles"
    },
    "3": {
      "título": "— 20 —",
      "texto": "Com a sabedoria"
    },
    "4": {
      "título": "",
      "texto": "pelo conhecimento"
    },
    "5": {
      "título": "— 21 —",
      "texto": "Quem é sábio é forte,"
    },
    "6": {
      "título": "",
      "texto": "Porque com prudência"
    },
    "7": {
      "título": "— 22 —",
      "texto": "A sabedoria é elevada demais"
    },
    "8": {
      "título": "— 23 —",
      "texto": "Quem só pensa em fazer o mal"
    },
    "9": {
      "título": "",
      "texto": "Os planos do tolo são pecado,"
    },
    "10": {
      "título": "— 24 —",
      "texto": "Se você se mostra fraco"
    },
    "11": {
      "título": "— 25 —",
      "texto": "Liberte os que estão sendo"
    },
    "12": {
      "título": "",
      "texto": "Você poderá dizer:"
    },
    "13": {
      "título": "— 26 —",
      "texto": "Meu filho, coma mel,"
    },
    "14": {
      "título": "",
      "texto": "Saiba que assim é a sabedoria"
    },
    "15": {
      "título": "— 27 —",
      "texto": "Não espie a habitação do justo,"
    },
    "16": {
      "título": "",
      "texto": "porque sete vezes cairá o justo"
    },
    "17": {
      "título": "— 28 —",
      "texto": "Quando o seu inimigo cair,"
    },
    "18": {
      "título": "",
      "texto": "para que o Senhor não veja isso"
    },
    "19": {
      "título": "— 29 —",
      "texto": "Não se irrite"
    },
    "20": {
      "título": "",
      "texto": "porque os maus"
    },
    "21": {
      "título": "— 30 —",
      "texto": "Meu filho,"
    },
    "22": {
      "título": "",
      "texto": "porque de repente"
    },
    "23": {
      "título": "Mais alguns provérbios dos sábios",
      "texto": "Estes também são provérbios dos sábios."
    },
    "24": {
      "título": "",
      "texto": "Quem disser ao ímpio:"
    },
    "25": {
      "título": "",
      "texto": "Mas haverá bem-estar"
    },
    "26": {
      "título": "",
      "texto": "Como beijo nos lábios,"
    },
    "27": {
      "título": "",
      "texto": "Cuide dos seus negócios lá fora,"
    },
    "28": {
      "título": "",
      "texto": "Não testemunhe sem motivo"
    },
    "29": {
      "título": "",
      "texto": "Não diga: “Vou fazer com ele"
    },
    "30": {
      "título": "",
      "texto": "Passei pelo campo do preguiçoso"
    },
    "31": {
      "título": "",
      "texto": "Eis que tudo estava"
    },
    "32": {
      "título": "",
      "texto": "Ao contemplar aquilo,"
    },
    "33": {
      "título": "",
      "texto": "Um pouco de sono,"
    },
    "34": {
      "título": "",
      "texto": "e a sua pobreza"
    }
  }
}