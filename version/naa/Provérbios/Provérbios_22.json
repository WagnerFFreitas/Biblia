{
  "livro": "Provérbios",
  "capítulo": 22,
  "versículos": {
    "1": {
      "título": "",
      "texto": "Mais vale o bom nome"
    },
    "2": {
      "título": "",
      "texto": "O rico e o pobre"
    },
    "3": {
      "título": "",
      "texto": "O prudente vê o mal"
    },
    "4": {
      "título": "",
      "texto": "A recompensa da humildade"
    },
    "5": {
      "título": "",
      "texto": "No caminho do perverso"
    },
    "6": {
      "título": "",
      "texto": "Ensine a criança no caminho"
    },
    "7": {
      "título": "",
      "texto": "O rico domina sobre o pobre,"
    },
    "8": {
      "título": "",
      "texto": "O que semeia a injustiça"
    },
    "9": {
      "título": "",
      "texto": "O generoso será abençoado,"
    },
    "10": {
      "título": "",
      "texto": "Mande embora o zombador,"
    },
    "11": {
      "título": "",
      "texto": "Quem ama a pureza do coração"
    },
    "12": {
      "título": "",
      "texto": "Os olhos do Senhor"
    },
    "13": {
      "título": "",
      "texto": "O preguiçoso diz:"
    },
    "14": {
      "título": "",
      "texto": "Cova profunda"
    },
    "15": {
      "título": "",
      "texto": "A tolice está ligada"
    },
    "16": {
      "título": "",
      "texto": "Quem oprime o pobre"
    },
    "17": {
      "título": "Trinta provérbios dos sábios",
      "texto": "Preste atenção e ouça"
    },
    "18": {
      "título": "",
      "texto": "Porque será agradável"
    },
    "19": {
      "título": "",
      "texto": "Quero que a sua confiança"
    },
    "20": {
      "título": "",
      "texto": "Por acaso, não lhe escrevi"
    },
    "21": {
      "título": "",
      "texto": "Fiz isso para que você"
    },
    "22": {
      "título": "— 1 —",
      "texto": "Não roube o pobre,"
    },
    "23": {
      "título": "",
      "texto": "porque o Senhor defenderá"
    },
    "24": {
      "título": "— 2 —",
      "texto": "Não faça amizade com quem"
    },
    "25": {
      "título": "",
      "texto": "para que você não aprenda"
    },
    "26": {
      "título": "— 3 —",
      "texto": "Não esteja entre os que"
    },
    "27": {
      "título": "",
      "texto": "pois, se você não tiver"
    },
    "28": {
      "título": "— 4 —",
      "texto": "Não remova os marcos antigos"
    },
    "29": {
      "título": "— 5 —",
      "texto": "Você está vendo alguém"
    }
  }
}