  "versículos": {
    "1": {
      "título": "",
      "texto": "Quem teima em rejeitar"
    },
    "2": {
      "título": "",
      "texto": "Quando os justos se multiplicam,"
    },
    "3": {
      "título": "",
      "texto": "Quem ama a sabedoria"
    },
    "4": {
      "título": "",
      "texto": "O rei justo"
    },
    "5": {
      "título": "",
      "texto": "Quem lisonjeia o seu próximo"
    },
    "6": {
      "título": "",
      "texto": "Na transgressão do homem mau,"
    },
    "7": {
      "título": "",
      "texto": "O justo se interessa"
    },
    "8": {
      "título": "",
      "texto": "Os zombadores"
    },
    "9": {
      "título": "",
      "texto": "Se o sábio discute"
    },
    "10": {
      "título": "",
      "texto": "Os homens sanguinários"
    },
    "11": {
      "título": "",
      "texto": "O tolo derrama toda a sua ira,"
    },
    "12": {
      "título": "",
      "texto": "Se um governador dá atenção"
    },
    "13": {
      "título": "",
      "texto": "O pobre e o seu opressor"
    },
    "14": {
      "título": "",
      "texto": "O rei que julga os pobres"
    },
    "15": {
      "título": "",
      "texto": "A vara e a disciplina"
    },
    "16": {
      "título": "",
      "texto": "Quando os ímpios"
    },
    "17": {
      "título": "",
      "texto": "Corrija o seu filho,"
    },
    "18": {
      "título": "",
      "texto": "Não havendo profecia,"
    },
    "19": {
      "título": "",
      "texto": "O servo não se emendará"
    },
    "20": {
      "título": "",
      "texto": "Você viu alguém que é"
    },
    "21": {
      "título": "",
      "texto": "Se alguém mimar o escravo"
    },
    "22": {
      "título": "",
      "texto": "A pessoa colérica"
    },
    "23": {
      "título": "",
      "texto": "O orgulho do ser humano"
    },
    "24": {
      "título": "",
      "texto": "Quem tem parte com um ladrão"
    },
    "25": {
      "título": "",
      "texto": "Quem tem medo dos outros"
    },
    "26": {
      "título": "",
      "texto": "Muitos buscam o favor"
    },
    "27": {
