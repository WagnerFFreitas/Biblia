{
  "livro": "Provérbios",
  "capítulo": 25,
  "versículos": {
    "1": {
      "título": "Símiles e lições morais",
      "texto": "Também estes são provérbios de Salomão, que foram transcritos pelos homens a serviço de Ezequias, rei de Judá."
    },
    "2": {
      "título": "",
      "texto": "A glória de Deus"
    },
    "3": {
      "título": "",
      "texto": "Como a altura dos céus"
    },
    "4": {
      "título": "",
      "texto": "Tire a escória da prata,"
    },
    "5": {
      "título": "",
      "texto": "tire o ímpio da presença do rei,"
    },
    "6": {
      "título": "",
      "texto": "Não se glorie na presença do rei,"
    },
    "7": {
      "título": "",
      "texto": "porque melhor é que lhe digam:"
    },
    "8": {
      "título": "",
      "texto": "não se apresse a levar ao tribunal,"
    },
    "9": {
      "título": "",
      "texto": "Defenda a sua causa"
    },
    "10": {
      "título": "",
      "texto": "Do contrário, quem o ouvir"
    },
    "11": {
      "título": "",
      "texto": "Como maçãs de ouro"
    },
    "12": {
      "título": "",
      "texto": "Como pendentes"
    },
    "13": {
      "título": "",
      "texto": "Como o frescor de neve"
    },
    "14": {
      "título": "",
      "texto": "Como nuvens e ventos"
    },
    "15": {
      "título": "",
      "texto": "Com paciência"
    },
    "16": {
      "título": "",
      "texto": "Você encontrou mel?"
    },
    "17": {
      "título": "",
      "texto": "Não seja frequente"
    },
    "18": {
      "título": "",
      "texto": "Martelo, espada e flecha aguda"
    },
    "19": {
      "título": "",
      "texto": "Como dente quebrado"
    },
    "20": {
      "título": "",
      "texto": "Como quem se despe"
    },
    "21": {
      "título": "",
      "texto": "Se o seu inimigo tiver fome,"
    },
    "22": {
      "título": "",
      "texto": "porque assim você amontoará"
    },
    "23": {
      "título": "",
      "texto": "O vento norte traz chuva,"
    },
    "24": {
      "título": "",
      "texto": "Melhor é morar"
    },
    "25": {
      "título": "",
      "texto": "Como água fria"
    },
    "26": {
      "título": "",
      "texto": "Como fonte que foi turvada"
    },
    "27": {
      "título": "",
      "texto": "Comer muito mel não é bom;"
    },
    "28": {
      "título": "",
      "texto": "Como cidade derrubada,"
    }
  }
}