{
  "livro": "Provérbios",
  "capítulo": 7,
  "versículos": {
    "1": {
      "título": "",
      "texto": "Meu filho, guarde"
    },
    "2": {
      "título": "",
      "texto": "Observe os meus mandamentos"
    },
    "3": {
      "título": "",
      "texto": "Amarre-os aos dedos,"
    },
    "4": {
      "título": "",
      "texto": "Diga à Sabedoria:"
    },
    "5": {
      "título": "",
      "texto": "Eles o guardarão"
    },
    "6": {
      "título": "A mulher imoral",
      "texto": "Porque da janela da minha casa,"
    },
    "7": {
      "título": "",
      "texto": "vi entre os ingênuos,"
    },
    "8": {
      "título": "",
      "texto": "Ele ia e vinha pela rua"
    },
    "9": {
      "título": "",
      "texto": "no crepúsculo, ao anoitecer,"
    },
    "10": {
      "título": "",
      "texto": "Eis que a mulher"
    },
    "11": {
      "título": "",
      "texto": "É espalhafatosa e inquieta;"
    },
    "12": {
      "título": "",
      "texto": "Ora está nas ruas,"
    },
    "13": {
      "título": "",
      "texto": "Ela agarrou o jovem e o beijou;"
    },
    "14": {
      "título": "",
      "texto": "“Eu tinha de oferecer"
    },
    "15": {
      "título": "",
      "texto": "Por isso, saí ao seu encontro;"
    },
    "16": {
      "título": "",
      "texto": "Já cobri de colchas"
    },
    "17": {
      "título": "",
      "texto": "Já perfumei o meu leito"
    },
    "18": {
      "título": "",
      "texto": "Venha, vamos nos embriagar"
    },
    "19": {
      "título": "",
      "texto": "Porque o meu marido"
    },
    "20": {
      "título": "",
      "texto": "Levou consigo uma bolsa"
    },
    "21": {
      "título": "",
      "texto": "Ela o seduziu"
    },
    "22": {
      "título": "",
      "texto": "E, num instante, ele a seguiu,"
    },
    "23": {
      "título": "",
      "texto": "até que uma flecha"
    },
    "24": {
      "título": "",
      "texto": "Agora, meu filho,"
    },
    "25": {
      "título": "",
      "texto": "Não deixe que o seu coração"
    },
    "26": {
      "título": "",
      "texto": "Porque a muitos"
    },
    "27": {
      "título": "",
      "texto": "A casa dela é caminho"
    }
  }
}