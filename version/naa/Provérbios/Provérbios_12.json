{
  "livro": "Provérbios",
  "capítulo": 12,
  "versículos": {
    "1": {
      "título": "",
      "texto": "Quem ama a disciplina"
    },
    "2": {
      "título": "",
      "texto": "Quem faz o bem"
    },
    "3": {
      "título": "",
      "texto": "Ninguém se estabelece"
    },
    "4": {
      "título": "",
      "texto": "A mulher virtuosa"
    },
    "5": {
      "título": "",
      "texto": "Os pensamentos dos justos"
    },
    "6": {
      "título": "",
      "texto": "As palavras dos ímpios"
    },
    "7": {
      "título": "",
      "texto": "Os ímpios são derrubados"
    },
    "8": {
      "título": "",
      "texto": "Cada um será louvado"
    },
    "9": {
      "título": "",
      "texto": "Melhor é ser modesto"
    },
    "10": {
      "título": "",
      "texto": "O justo cuida dos seus animais,"
    },
    "11": {
      "título": "",
      "texto": "O que lavra a sua terra"
    },
    "12": {
      "título": "",
      "texto": "O ímpio quer viver"
    },
    "13": {
      "título": "",
      "texto": "Os maus se enredam"
    },
    "14": {
      "título": "",
      "texto": "Cada um se farta de bem"
    },
    "15": {
      "título": "",
      "texto": "O caminho do insensato"
    },
    "16": {
      "título": "",
      "texto": "O insensato mostra logo"
    },
    "17": {
      "título": "",
      "texto": "Quem diz a verdade"
    },
    "18": {
      "título": "",
      "texto": "Palavras precipitadas"
    },
    "19": {
      "título": "",
      "texto": "Os lábios que falam a verdade"
    },
    "20": {
      "título": "",
      "texto": "Há fraude no coração"
    },
    "21": {
      "título": "",
      "texto": "Nenhuma desgraça"
    },
    "22": {
      "título": "",
      "texto": "O Senhor detesta"
    },
    "23": {
      "título": "",
      "texto": "Aquele que é prudente"
    },
    "24": {
      "título": "",
      "texto": "O homem esforçado dominará,"
    },
    "25": {
      "título": "",
      "texto": "A ansiedade no coração"
    },
    "26": {
      "título": "",
      "texto": "O justo serve de guia"
    },
    "27": {
      "título": "",
      "texto": "O preguiçoso não assará"
    },
    "28": {
      "título": "",
      "texto": "Na vereda da justiça está a vida,"
    }
  }
}