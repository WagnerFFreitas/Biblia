{
  "livro": "Provérbios",
  "capítulo": 8,
  "versículos": {
    "1": {
      "título": "A excelência da Sabedoria",
      "texto": "Por acaso, não clama"
    },
    "2": {
      "título": "",
      "texto": "A Sabedoria se coloca no topo"
    },
    "3": {
      "título": "",
      "texto": "Junto aos portões,"
    },
    "4": {
      "título": "",
      "texto": "“É para vocês, homens,"
    },
    "5": {
      "título": "",
      "texto": "Vocês, ingênuos,"
    },
    "6": {
      "título": "",
      "texto": "Escutem, pois falarei"
    },
    "7": {
      "título": "",
      "texto": "Porque a minha boca"
    },
    "8": {
      "título": "",
      "texto": "Todas as palavras da minha boca"
    },
    "9": {
      "título": "",
      "texto": "Todas são retas"
    },
    "10": {
      "título": "",
      "texto": "Aceitem o meu ensino,"
    },
    "11": {
      "título": "",
      "texto": "Porque a sabedoria"
    },
    "12": {
      "título": "",
      "texto": "“Eu, a Sabedoria,"
    },
    "13": {
      "título": "",
      "texto": "O temor do Senhor consiste"
    },
    "14": {
      "título": "",
      "texto": "Meu é o conselho"
    },
    "15": {
      "título": "",
      "texto": "Por meio de mim"
    },
    "16": {
      "título": "",
      "texto": "Por meio de mim"
    },
    "17": {
      "título": "",
      "texto": "“Eu amo os que me amam;"
    },
    "18": {
      "título": "",
      "texto": "Riquezas e honra estão comigo,"
    },
    "19": {
      "título": "",
      "texto": "O meu fruto é melhor"
    },
    "20": {
      "título": "",
      "texto": "Ando pelo caminho da justiça"
    },
    "21": {
      "título": "",
      "texto": "para dotar de bens os que me amam"
    },
    "22": {
      "título": "A eternidade da Sabedoria",
      "texto": "“O Senhor me possuía"
    },
    "23": {
      "título": "",
      "texto": "Fui estabelecida"
    },
    "24": {
      "título": "",
      "texto": "Nasci antes de haver abismos,"
    },
    "25": {
      "título": "",
      "texto": "Antes que os montes"
    },
    "26": {
      "título": "",
      "texto": "Deus ainda não tinha feito"
    },
    "27": {
      "título": "",
      "texto": "Eu estava lá"
    },
    "28": {
      "título": "",
      "texto": "Estava lá quando ele firmava"
    },
    "29": {
      "título": "",
      "texto": "quando fixava ao mar"
    },
    "30": {
      "título": "",
      "texto": "eu estava com ele"
    },
    "31": {
      "título": "",
      "texto": "divertindo-me"
    },
    "32": {
      "título": "",
      "texto": "“Agora, meus filhos,"
    },
    "33": {
      "título": "",
      "texto": "Ouçam o ensino, sejam sábios"
    },
    "34": {
      "título": "",
      "texto": "Feliz é aquele que me ouve,"
    },
    "35": {
      "título": "",
      "texto": "Pois quem me encontra"
    },
    "36": {
      "título": "",
      "texto": "Mas quem peca contra mim"
    }
  }
}