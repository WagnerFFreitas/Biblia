{
  "livro": "Cantares de Salomâo",
  "capítulo": 3,
  "versículos": {
    "1": {
      "título": "",
      "texto": "De noite, na minha cama,"
    },
    "2": {
      "título": "",
      "texto": "Eu me levantarei agora"
    },
    "3": {
      "título": "",
      "texto": "Os guardas, que rondavam"
    },
    "4": {
      "título": "",
      "texto": "Mal os deixei, encontrei logo"
    },
    "5": {
      "título": "",
      "texto": "Filhas de Jerusalém,"
    },
    "6": {
      "título": "Coro",
      "texto": "O que é aquilo que vem"
    },
    "7": {
      "título": "",
      "texto": "É a liteira de Salomão."
    },
    "8": {
      "título": "",
      "texto": "Todos sabem manejar a espada"
    },
    "9": {
      "título": "",
      "texto": "O rei Salomão mandou fazer"
    },
    "10": {
      "título": "",
      "texto": "As colunas eram de prata,"
    },
    "11": {
      "título": "",
      "texto": "Saiam, ó filhas de Sião,"
    }
  }
}