{
  "livro": "Cantares de Salomâo",
  "capítulo": 8,
  "versículos": {
    "1": {
      "título": "",
      "texto": "Quem dera que você"
    },
    "2": {
      "título": "",
      "texto": "Eu o levaria para a casa"
    },
    "3": {
      "título": "",
      "texto": "A sua mão esquerda estaria"
    },
    "4": {
      "título": "",
      "texto": "Filhas de Jerusalém,"
    },
    "5": {
      "título": "Coro",
      "texto": "Quem é esta que vem"
    },
    "6": {
      "título": "Esposa",
      "texto": "Ponha-me como selo"
    },
    "7": {
      "título": "",
      "texto": "As muitas águas não poderiam"
    },
    "8": {
      "título": "Os irmãos",
      "texto": "Temos uma irmãzinha"
    },
    "9": {
      "título": "",
      "texto": "Se ela fosse uma muralha,"
    },
    "10": {
      "título": "Esposa",
      "texto": "Eu sou uma muralha,"
    },
    "11": {
      "título": "Coro",
      "texto": "Salomão teve uma vinha"
    },
    "12": {
      "título": "Esposa",
      "texto": "A minha vinha,"
    },
    "13": {
      "título": "Esposo",
      "texto": "Você, que habita nos jardins,"
    },
    "14": {
      "título": "Esposa",
      "texto": "Venha depressa, meu amado,"
    }
  }
}