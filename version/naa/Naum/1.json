{
  "livro": "Naum",
  "capítulo": 1,
  "versículos": {
    "1": {
      "título": "",
      "texto": "Sentença contra Nínive. Livro da visão de Naum, da cidade de Elcos."
    },
    "2": {
      "título": "A ira e a misericórdia de Deus",
      "texto": "O Senhor é Deus zeloso"
    },
    "3": {
      "título": "",
      "texto": "O Senhor é tardio em irar-se,"
    },
    "4": {
      "título": "",
      "texto": "Repreende o mar, e ele seca;"
    },
    "5": {
      "título": "",
      "texto": "Os montes tremem diante dele,"
    },
    "6": {
      "título": "",
      "texto": "Quem pode suportar"
    },
    "7": {
      "título": "",
      "texto": "O Senhor é bom,"
    },
    "8": {
      "título": "",
      "texto": "Mas, com inundação"
    },
    "9": {
      "título": "",
      "texto": "O que é que vocês estão"
    },
    "10": {
      "título": "",
      "texto": "Porque, ainda que eles"
    },
    "11": {
      "título": "",
      "texto": "De você, Nínive, saiu um"
    },
    "12": {
      "título": "",
      "texto": "Assim diz o Senhor :"
    },
    "13": {
      "título": "",
      "texto": "Quebrarei o jugo deles"
    },
    "14": {
      "título": "",
      "texto": "Porém contra você, Assíria,"
    },
    "15": {
      "título": "",
      "texto": "Eis sobre os montes"
    }
  }
}