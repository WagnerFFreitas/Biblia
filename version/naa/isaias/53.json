{
  "livro": "Isaías",
  "capítulo": 53,
  "versículos": {
    "1": {
      "título": "",
      "texto": "Quem creu em nossa pregação?"
    },
    "2": {
      "título": "",
      "texto": "Porque foi subindo"
    },
    "3": {
      "título": "",
      "texto": "Era desprezado e o mais rejeitado"
    },
    "4": {
      "título": "",
      "texto": "Certamente ele tomou sobre si"
    },
    "5": {
      "título": "",
      "texto": "Mas ele foi traspassado"
    },
    "6": {
      "título": "",
      "texto": "Todos nós andávamos"
    },
    "7": {
      "título": "",
      "texto": "Ele foi oprimido e humilhado,"
    },
    "8": {
      "título": "",
      "texto": "Pela opressão e pelo juízo,"
    },
    "9": {
      "título": "",
      "texto": "Designaram-lhe a sepultura"
    },
    "10": {
      "título": "",
      "texto": "Todavia, ao Senhor"
    },
    "11": {
      "título": "",
      "texto": "Ele verá o fruto do trabalho"
    },
    "12": {
      "título": "",
      "texto": "Por isso, eu lhe darei a sua parte"
    }
  }
}