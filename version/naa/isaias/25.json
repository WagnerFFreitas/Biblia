{
  "livro": "Isaías",
  "capítulo": 25,
  "versículos": {
    "1": {
      "título": "Cântico de louvor pela misericórdia divina",
      "texto": "Ó Senhor , tu és o meu Deus;"
    },
    "2": {
      "título": "",
      "texto": "Porque da cidade fizeste"
    },
    "3": {
      "título": "",
      "texto": "Por isso, um povo forte"
    },
    "4": {
      "título": "",
      "texto": "Porque foste a fortaleza do pobre"
    },
    "5": {
      "título": "",
      "texto": "como o calor em lugar seco."
    },
    "6": {
      "título": "Um banquete para todos os povos",
      "texto": "O Senhor dos Exércitos"
    },
    "7": {
      "título": "",
      "texto": "Neste monte ele acabará"
    },
    "8": {
      "título": "",
      "texto": "Tragará a morte para sempre,"
    },
    "9": {
      "título": "",
      "texto": "Naquele dia, se dirá:"
    },
    "10": {
      "título": "Deus castigará Moabe",
      "texto": "Porque a mão do Senhor"
    },
    "11": {
      "título": "",
      "texto": "No meio disto"
    },
    "12": {
      "título": "",
      "texto": "Ele derrubará as altas fortalezas"
    }
  }
}