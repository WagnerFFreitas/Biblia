{
  "livro": "Isaías",
  "capítulo": 5,
  "versículos": {
    "1": {
      "título": "A parábola da vinha",
      "texto": "Agora cantarei ao meu amado"
    },
    "2": {
      "título": "",
      "texto": "Ele cavou a terra, tirou as pedras"
    },
    "3": {
      "título": "",
      "texto": "“E agora,"
    },
    "4": {
      "título": "",
      "texto": "Que mais se podia fazer"
    },
    "5": {
      "título": "",
      "texto": "“E agora lhes darei a conhecer"
    },
    "6": {
      "título": "",
      "texto": "Farei dela um lugar abandonado;"
    },
    "7": {
      "título": "",
      "texto": "Porque a vinha"
    },
    "8": {
      "título": "Ai dos maus!",
      "texto": "Ai dos que ajuntam casas"
    },
    "9": {
      "título": "",
      "texto": "Eu ouvi o Senhor dos Exércitos"
    },
    "10": {
      "título": "",
      "texto": "Uma grande vinha produzirá"
    },
    "11": {
      "título": "",
      "texto": "Ai dos que se levantam"
    },
    "12": {
      "título": "",
      "texto": "Liras e harpas, tamborins e flautas"
    },
    "13": {
      "título": "",
      "texto": "Portanto, o meu povo"
    },
    "14": {
      "título": "",
      "texto": "Por isso, a sepultura aumentou"
    },
    "15": {
      "título": "",
      "texto": "Então o povo será abatido"
    },
    "16": {
      "título": "",
      "texto": "Mas o Senhor dos Exércitos"
    },
    "17": {
      "título": "",
      "texto": "Então os cordeiros pastarão ali"
    },
    "18": {
      "título": "",
      "texto": "Ai dos que puxam para si"
    },
    "19": {
      "título": "",
      "texto": "E dizem: “Que Deus se apresse"
    },
    "20": {
      "título": "",
      "texto": "Ai dos que ao mal chamam bem"
    },
    "21": {
      "título": "",
      "texto": "Ai dos que são sábios"
    },
    "22": {
      "título": "",
      "texto": "Ai dos que são heróis"
    },
    "23": {
      "título": "",
      "texto": "que por suborno"
    },
    "24": {
      "título": "Deus castigará o seu povo",
      "texto": "Portanto, assim como as labaredas"
    },
    "25": {
      "título": "",
      "texto": "Por isso, a ira do Senhor"
    },
    "26": {
      "título": "",
      "texto": "Ele levantará um estandarte"
    },
    "27": {
      "título": "",
      "texto": "Não há entre elas"
    },
    "28": {
      "título": "",
      "texto": "As suas flechas são afiadas,"
    },
    "29": {
      "título": "",
      "texto": "O rugido deles é como o do leão;"
    },
    "30": {
      "título": "",
      "texto": "Naquele dia,"
    }
  }
}