{
  "livro": "Isaías",
  "capítulo": 42,
  "versículos": {
    "1": {
      "título": "O Servo do Senhor",
      "texto": "“Eis aqui o meu servo,"
    },
    "2": {
      "título": "",
      "texto": "Não clamará, não gritará,"
    },
    "3": {
      "título": "",
      "texto": "Não esmagará a cana quebrada,"
    },
    "4": {
      "título": "",
      "texto": "Não desanimará,"
    },
    "5": {
      "título": "",
      "texto": "Assim diz Deus, o Senhor ,"
    },
    "6": {
      "título": "",
      "texto": "“Eu, o Senhor , chamei você"
    },
    "7": {
      "título": "",
      "texto": "para abrir os olhos dos cegos,"
    },
    "8": {
      "título": "",
      "texto": "Eu sou o Senhor :"
    },
    "9": {
      "título": "",
      "texto": "“Eis que as primeiras predições"
    },
    "10": {
      "título": "Cântico de louvor pela salvação do povo",
      "texto": "Cantem ao Senhor"
    },
    "11": {
      "título": "",
      "texto": "Ergam a voz o deserto,"
    },
    "12": {
      "título": "",
      "texto": "deem honra ao Senhor"
    },
    "13": {
      "título": "",
      "texto": "O Senhor sairá como valente,"
    },
    "14": {
      "título": "",
      "texto": "“Por muito tempo me calei,"
    },
    "15": {
      "título": "",
      "texto": "Devastarei os montes"
    },
    "16": {
      "título": "",
      "texto": "Guiarei os cegos"
    },
    "17": {
      "título": "",
      "texto": "Retrocederão e ficarão"
    },
    "18": {
      "título": "Lamento sobre a cegueira de Israel",
      "texto": "“Escutem, surdos,"
    },
    "19": {
      "título": "",
      "texto": "Quem é tão cego"
    },
    "20": {
      "título": "",
      "texto": "Você vê muitas coisas,"
    },
    "21": {
      "título": "",
      "texto": "Foi do agrado do Senhor ,"
    },
    "22": {
      "título": "",
      "texto": "Mas este é um povo"
    },
    "23": {
      "título": "",
      "texto": "Quem de vocês dará ouvidos"
    },
    "24": {
      "título": "",
      "texto": "Quem entregou Jacó por despojo"
    },
    "25": {
      "título": "",
      "texto": "Por isso derramou sobre eles"
    }
  }
}