{
  "livro": "Jó",
  "capítulo": 5,
  "versículos": {
    "1": {
      "título": "O ser humano nasce para o sofrimento",
      "texto": "“Grite agora, para ver"
    },
    "2": {
      "título": "",
      "texto": "Porque a ira mata o insensato,"
    },
    "3": {
      "título": "",
      "texto": "Eu mesmo vi o insensato"
    },
    "4": {
      "título": "",
      "texto": "Os filhos dele estão longe"
    },
    "5": {
      "título": "",
      "texto": "A sua colheita,"
    },
    "6": {
      "título": "",
      "texto": "Porque a aflição não vem do pó,"
    },
    "7": {
      "título": "",
      "texto": "Mas o ser humano nasce"
    },
    "8": {
      "título": "Há esperança para os pobres",
      "texto": "“Quanto a mim,"
    },
    "9": {
      "título": "",
      "texto": "Deus faz coisas grandes"
    },
    "10": {
      "título": "",
      "texto": "Faz chover sobre a terra"
    },
    "11": {
      "título": "",
      "texto": "Põe os abatidos num lugar alto"
    },
    "12": {
      "título": "",
      "texto": "Deus frustra os planos"
    },
    "13": {
      "título": "",
      "texto": "Ele apanha os sábios"
    },
    "14": {
      "título": "",
      "texto": "De dia eles encontram as trevas,"
    },
    "15": {
      "título": "",
      "texto": "Porém Deus salva da espada"
    },
    "16": {
      "título": "",
      "texto": "Assim, há esperança"
    },
    "17": {
      "título": "As mãos de Deus curam",
      "texto": "“Bem-aventurado é aquele"
    },
    "18": {
      "título": "",
      "texto": "Porque ele faz a ferida"
    },
    "19": {
      "título": "",
      "texto": "De seis angústias ele o livrará,"
    },
    "20": {
      "título": "",
      "texto": "Na fome ele livrará você"
    },
    "21": {
      "título": "",
      "texto": "Você estará abrigado"
    },
    "22": {
      "título": "",
      "texto": "Da destruição e da fome"
    },
    "23": {
      "título": "",
      "texto": "Porque com as pedras do campo"
    },
    "24": {
      "título": "",
      "texto": "Saberá que a sua tenda"
    },
    "25": {
      "título": "",
      "texto": "Saberá que a sua descendência"
    },
    "26": {
      "título": "",
      "texto": "Em robusta velhice"
    },
    "27": {
      "título": "",
      "texto": "Veja bem!"
    }
  }
}