{
  "livro": "Jó",
  "capítulo": 40,
  "versículos": {
    "1": {
      "título": "",
      "texto": "O Senhor disse mais a Jó:"
    },
    "2": {
      "título": "",
      "texto": "“Será que alguém"
    },
    "3": {
      "título": "Primeira resposta de Jó a Deus",
      "texto": "Então Jó respondeu ao Senhor e disse:"
    },
    "4": {
      "título": "",
      "texto": "“Sou indigno."
    },
    "5": {
      "título": "",
      "texto": "Uma vez falei,"
    },
    "6": {
      "título": "Você tem um braço tão forte como o braço de Deus?",
      "texto": "Então o Senhor , do meio de um redemoinho, respondeu a Jó e disse:"
    },
    "7": {
      "título": "",
      "texto": "“Cinja os lombos como homem,"
    },
    "8": {
      "título": "",
      "texto": "Será que você está querendo"
    },
    "9": {
      "título": "",
      "texto": "Você tem um braço tão forte"
    },
    "10": {
      "título": "",
      "texto": "Adorne-se, então,"
    },
    "11": {
      "título": "",
      "texto": "Derrame as torrentes da sua ira;"
    },
    "12": {
      "título": "",
      "texto": "Sim, olhe para eles e humilhe-os;"
    },
    "13": {
      "título": "",
      "texto": "Cubra-os todos no pó;"
    },
    "14": {
      "título": "",
      "texto": "Então também eu confessarei"
    },
    "15": {
      "título": "Quem é capaz de apanhar o monstro Beemote?",
      "texto": "“Contemple agora"
    },
    "16": {
      "título": "",
      "texto": "A força dele está"
    },
    "17": {
      "título": "",
      "texto": "Ele endurece a sua cauda"
    },
    "18": {
      "título": "",
      "texto": "Os seus ossos"
    },
    "19": {
      "título": "",
      "texto": "Ele é obra-prima"
    },
    "20": {
      "título": "",
      "texto": "Na verdade,"
    },
    "21": {
      "título": "",
      "texto": "Deita-se debaixo"
    },
    "22": {
      "título": "",
      "texto": "As árvores de lótus"
    },
    "23": {
      "título": "",
      "texto": "Se um rio transborda,"
    },
    "24": {
      "título": "",
      "texto": "Será que alguém pode apanhá-lo"
    }
  }
}