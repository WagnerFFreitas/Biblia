{
  "livro": "Jó",
  "capítulo": 28,
  "versículos": {
    "1": {
      "título": "Os seus olhos veem o que há de precioso",
      "texto": "“Na verdade, a prata"
    },
    "2": {
      "título": "",
      "texto": "O ferro é tirado da terra,"
    },
    "3": {
      "título": "",
      "texto": "Os homens põem termo"
    },
    "4": {
      "título": "",
      "texto": "Abrem entrada para minas"
    },
    "5": {
      "título": "",
      "texto": "Da terra procede o alimento,"
    },
    "6": {
      "título": "",
      "texto": "Nas suas pedras"
    },
    "7": {
      "título": "",
      "texto": "Essa vereda,"
    },
    "8": {
      "título": "",
      "texto": "Feras majestosas nunca pisaram"
    },
    "9": {
      "título": "",
      "texto": "O homem estende a sua mão"
    },
    "10": {
      "título": "",
      "texto": "Abre canais nas pedras,"
    },
    "11": {
      "título": "",
      "texto": "Tapa os veios de água,"
    },
    "12": {
      "título": "O valor da sabedoria",
      "texto": "“Mas onde se achará"
    },
    "13": {
      "título": "",
      "texto": "O ser humano não conhece"
    },
    "14": {
      "título": "",
      "texto": "O abismo diz:"
    },
    "15": {
      "título": "",
      "texto": "Não se compra a sabedoria"
    },
    "16": {
      "título": "",
      "texto": "O seu valor"
    },
    "17": {
      "título": "",
      "texto": "O ouro não se iguala a ela,"
    },
    "18": {
      "título": "",
      "texto": "Ela faz esquecer o coral"
    },
    "19": {
      "título": "",
      "texto": "O topázio da Etiópia"
    },
    "20": {
      "título": "O temor do Senhor é a sabedoria",
      "texto": "“Mas de onde vem a sabedoria?"
    },
    "21": {
      "título": "",
      "texto": "Está encoberta aos olhos"
    },
    "22": {
      "título": "",
      "texto": "O abismo e a morte dizem:"
    },
    "23": {
      "título": "",
      "texto": "“Deus lhe entende o caminho,"
    },
    "24": {
      "título": "",
      "texto": "Porque o seu olhar"
    },
    "25": {
      "título": "",
      "texto": "Quando Deus regulou"
    },
    "26": {
      "título": "",
      "texto": "quando determinou leis"
    },
    "27": {
      "título": "",
      "texto": "então ele viu a sabedoria"
    },
    "28": {
      "título": "",
      "texto": "E disse ao ser humano:"
    }
  }
}