{
  "livro": "Jó",
  "capítulo": 34,
  "versículos": {
    "1": {
      "título": "Cap.",
      "texto": "Eliú disse mais:"
    },
    "2": {
      "título": "",
      "texto": "“Vocês que são sábios,"
    },
    "3": {
      "título": "",
      "texto": "Porque o ouvido"
    },
    "4": {
      "título": "",
      "texto": "Escolhamos para nós"
    },
    "5": {
      "título": "Deus não perverte o direito",
      "texto": "“Porque Jó disse: ‘Sou justo,"
    },
    "6": {
      "título": "",
      "texto": "Apesar do meu direito,"
    },
    "7": {
      "título": "",
      "texto": "“Será que existe outro homem"
    },
    "8": {
      "título": "",
      "texto": "Ele segue o caminho"
    },
    "9": {
      "título": "",
      "texto": "Pois disse: ‘De nada adianta"
    },
    "10": {
      "título": "",
      "texto": "“Por isso, vocês que têm"
    },
    "11": {
      "título": "",
      "texto": "Pois Deus retribui ao homem"
    },
    "12": {
      "título": "",
      "texto": "Na verdade,"
    },
    "13": {
      "título": "",
      "texto": "Quem lhe entregou"
    },
    "14": {
      "título": "",
      "texto": "Se Deus pensasse"
    },
    "15": {
      "título": "",
      "texto": "toda a humanidade morreria"
    },
    "16": {
      "título": "Deus é justo e poderoso",
      "texto": "“Portanto, se você"
    },
    "17": {
      "título": "",
      "texto": "Se Deus odiasse o direito,"
    },
    "18": {
      "título": "",
      "texto": "Será que alguém diria a um rei:"
    },
    "19": {
      "título": "",
      "texto": "Quanto menos dirá isso"
    },
    "20": {
      "título": "",
      "texto": "De repente, morrem;"
    },
    "21": {
      "título": "",
      "texto": "Os olhos de Deus estão"
    },
    "22": {
      "título": "",
      "texto": "Não há trevas nem sombra"
    },
    "23": {
      "título": "",
      "texto": "Pois Deus não precisa observar"
    },
    "24": {
      "título": "",
      "texto": "Deus arrasa os poderosos,"
    },
    "25": {
      "título": "",
      "texto": "Porque ele conhece"
    },
    "26": {
      "título": "",
      "texto": "Ele os castiga"
    },
    "27": {
      "título": "",
      "texto": "porque se afastaram de Deus,"
    },
    "28": {
      "título": "",
      "texto": "e assim fizeram com que"
    },
    "29": {
      "título": "",
      "texto": "“Se ele se calar,"
    },
    "30": {
      "título": "",
      "texto": "para que o ímpio não reine,"
    },
    "31": {
      "título": "Você prometeu parar de praticar injustiça?",
      "texto": "“Se alguém se dirige a Deus,"
    },
    "32": {
      "título": "",
      "texto": "ensina-me o que"
    },
    "33": {
      "título": "",
      "texto": "será que Deus deve"
    },
    "34": {
      "título": "",
      "texto": "“Os homens que têm"
    },
    "35": {
      "título": "",
      "texto": "‘Jó falou sem conhecimento,"
    },
    "36": {
      "título": "",
      "texto": "Quem dera Jó"
    },
    "37": {
      "título": "",
      "texto": "Pois ao seu pecado"
    }
  }
}