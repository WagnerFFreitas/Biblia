{
  "livro": "Jó",
  "capítulo": 30,
  "versículos": {
    "1": {
      "título": "Todos zombam de mim",
      "texto": "“Mas agora zombam de mim"
    },
    "2": {
      "título": "",
      "texto": "De que também me serviria"
    },
    "3": {
      "título": "",
      "texto": "Enfraqueceram de tanto"
    },
    "4": {
      "título": "",
      "texto": "Apanham malvas"
    },
    "5": {
      "título": "",
      "texto": "São expulsos"
    },
    "6": {
      "título": "",
      "texto": "Têm de morar"
    },
    "7": {
      "título": "",
      "texto": "Uivam entre os arbustos"
    },
    "8": {
      "título": "",
      "texto": "São filhos de doidos,"
    },
    "9": {
      "título": "",
      "texto": "“Mas agora sou"
    },
    "10": {
      "título": "",
      "texto": "Eles me detestam,"
    },
    "11": {
      "título": "",
      "texto": "Deus afrouxou a corda"
    },
    "12": {
      "título": "",
      "texto": "À minha direita se levanta"
    },
    "13": {
      "título": "",
      "texto": "Arruínam o meu caminho;"
    },
    "14": {
      "título": "",
      "texto": "Vêm contra mim"
    },
    "15": {
      "título": "",
      "texto": "Sobrevieram-me pavores;"
    },
    "16": {
      "título": "Tu foste cruel comigo",
      "texto": "“Agora a minha alma"
    },
    "17": {
      "título": "",
      "texto": "A noite perfura os meus ossos,"
    },
    "18": {
      "título": "",
      "texto": "Pela grande violência"
    },
    "19": {
      "título": "",
      "texto": "Deus me lançou na lama,"
    },
    "20": {
      "título": "",
      "texto": "“Clamo a ti, ó Deus,"
    },
    "21": {
      "título": "",
      "texto": "Tu foste cruel comigo;"
    },
    "22": {
      "título": "",
      "texto": "Tu me levantas sobre o vento"
    },
    "23": {
      "título": "",
      "texto": "Pois eu sei que me levarás"
    },
    "24": {
      "título": "",
      "texto": "“Não é fato que"
    },
    "25": {
      "título": "",
      "texto": "Por acaso, não chorei"
    },
    "26": {
      "título": "",
      "texto": "Quando eu esperava o bem,"
    },
    "27": {
      "título": "Eu clamo por socorro",
      "texto": "“O meu íntimo se agita"
    },
    "28": {
      "título": "",
      "texto": "Tenho a pele queimada,"
    },
    "29": {
      "título": "",
      "texto": "Sou irmão dos chacais"
    },
    "30": {
      "título": "",
      "texto": "A minha pele escurece e cai;"
    },
    "31": {
      "título": "",
      "texto": "Por isso, a minha harpa é usada"
    }
  }
}