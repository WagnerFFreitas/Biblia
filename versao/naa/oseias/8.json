{
    "livro": "Oséias",
    "capitulo": 8,
    "titulos": {
        "1": "O castigo está próximo"
    },
    "versiculos": {
        "1": "“Toque a trombeta!",
        "2": "Eles me invocam, dizendo:",
        "3": "Mas Israel rejeitou o que é bom;",
        "4": "“Eles estabeleceram reis,",
        "5": "O seu bezerro, ó Samaria,",
        "6": "Porque esse bezerro",
        "7": "“Porque semeiam ventos",
        "8": "Israel foi devorado.",
        "9": "Porque foram pedir ajuda",
        "10": "Todavia, ainda que contratem",
        "11": "“Visto que Efraim multiplicou",
        "12": "Embora eu lhe escreva",
        "13": "Amam o sacrifício;",
        "14": "Porque Israel se esqueceu"
    }
}