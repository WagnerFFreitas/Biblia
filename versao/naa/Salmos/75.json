{
    "livro": "Salmos",
    "capitulo": 75,
    "titulos": {
        "1": "Deus é juiz"
    },
    "versiculos": {
        "1": "Graças te rendemos, ó Deus,",
        "2": "Pois disseste: “Quando chegar",
        "3": "Ainda que tremam a terra",
        "4": "Digo aos soberbos",
        "5": "Não levantem orgulhosamente",
        "6": "Porque não é do Oriente,",
        "7": "Deus é o juiz;",
        "8": "Porque na mão do Senhor",
        "9": "Quanto a mim,",
        "10": "Ele diz: “Abaterei"
    }
}