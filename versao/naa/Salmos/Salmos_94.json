{
    "livro": "Salmos",
    "capitulo": 94,
    "titulos": {
        "1": "Apelo para a justiça de Deus"
    },
    "versiculos": {
        "1": "Ó Senhor , Deus das vinganças,",
        "2": "Levanta-te, ó juiz da terra,",
        "3": "Até quando, Senhor , os ímpios,",
        "4": "Fazem alarde",
        "5": "Esmagam o teu povo, Senhor ,",
        "6": "Matam as viúvas e os estrangeiros",
        "7": "E dizem:",
        "8": "Prestem atenção,",
        "9": "Aquele que fez o ouvido",
        "10": "Aquele que repreende as nações",
        "11": "O Senhor conhece",
        "12": "Bem-aventurado, Senhor ,",
        "13": "para lhe dares descanso",
        "14": "Pois o Senhor não abandonará",
        "15": "Nos tribunais voltará a imperar",
        "16": "Quem se levantará a meu favor",
        "17": "Se não fosse o auxílio do Senhor ,",
        "18": "Quando eu digo:",
        "19": "Multiplicando-se em mim",
        "20": "Será que pode associar-se",
        "21": "Ajuntam-se",
        "22": "Mas o Senhor",
        "23": "Sobre eles faz recair"
    }
}