{
    "livro": "Salmos",
    "capitulo": 86,
    "titulos": {
        "1": "Súplica e confiança"
    },
    "versiculos": {
        "1": "Inclina, Senhor , os teus ouvidos",
        "2": "Preserva a minha alma,",
        "3": "Compadece-te de mim, ó Senhor,",
        "4": "Alegra a alma do teu servo,",
        "5": "Pois tu, Senhor,",
        "6": "Escuta, Senhor , a minha oração",
        "7": "No dia da minha angústia",
        "8": "Não há entre os deuses",
        "9": "Todas as nações que fizeste",
        "10": "Pois tu és grande",
        "11": "Ensina-me, Senhor ,",
        "12": "Eu te darei graças,",
        "13": "Pois grande é a tua misericórdia",
        "14": "Ó Deus, os soberbos",
        "15": "Mas tu, Senhor,",
        "16": "Volta-te para mim",
        "17": "Mostra-me um sinal do teu favor,"
    }
}