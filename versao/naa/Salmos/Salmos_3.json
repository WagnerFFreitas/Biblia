{
    "livro": "Salmos",
    "capitulo": 3,
    "titulos": {
        "1": "Confiança na salvação de Deus"
    },
    "versiculos": {
        "1": "Senhor , como tem crescido",
        "2": "São muitos os que dizem de mim:",
        "3": "Porém tu, Senhor ,",
        "4": "Com a minha voz",
        "5": "Eu me deito e pego no sono;",
        "6": "Não tenho medo dos milhares",
        "7": "Levanta-te, Senhor !",
        "8": "Do Senhor é a salvação."
    }
