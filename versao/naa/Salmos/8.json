{
    "livro": "Salmos",
    "capitulo": 8,
    "titulos": {
        "1": "A glória divina e a dignidade do filho do homem"
    },
    "versiculos": {
        "1": "Ó Senhor , Senhor nosso,",
        "2": "Da boca de pequeninos",
        "3": "Quando contemplo os teus céus,",
        "4": "que é o homem,",
        "5": "Fizeste-o, no entanto,",
        "6": "Deste-lhe domínio",
        "7": "ovelhas e bois, todos,",
        "8": "as aves do céu,",
        "9": "Ó Senhor , Senhor nosso,"
    }
