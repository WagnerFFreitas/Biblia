{
    "livro": "Salmos",
    "capitulo": 47,
    "titulos": {
        "1": "Deus, o rei de toda a terra"
    },
    "versiculos": {
        "1": "Batam palmas, todos os povos;",
        "2": "Pois o Senhor Altíssimo",
        "3": "Ele nos submeteu os povos",
        "4": "Escolheu para nós",
        "5": "Deus subiu em meio a aclamações,",
        "6": "Cantem louvores a Deus,",
        "7": "Deus é o Rei de toda a terra;",
        "8": "Deus reina sobre as nações;",
        "9": "Os príncipes dos povos"
    }
}