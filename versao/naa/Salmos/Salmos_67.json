    "titulos": {
        "1": "Convite para louvar a Deus"
    },
    "versiculos": {
        "1": "Seja Deus gracioso para conosco,",
        "2": "para que se conheça na terra",
        "3": "Louvem-te os povos, ó Deus!",
        "4": "Alegrem-se e exultem as nações,",
        "5": "Louvem-te os povos, ó Deus!",
        "6": "A terra deu o seu fruto,",
        "7": "Que Deus nos abençoe,"
    }
