<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.0.1" modifiedIn="OpenLP 2.0.1" modifiedDate="2013-11-14T21:42:00">
  <properties>
    <titles>
      <title>CC 074 Graças a Jesus</title>
      <title>St. Bees</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <authors>
      <author>John Dykes</author>
      <author>Robert Kalley</author>
    </authors>
    <songbooks>
      <songbook name="Cantor Cristão" entry="074"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Graças ao bom Salvador,<br/>que me livra do furor<br/>do feroz destruidor!<br/>Graças, graças a Jesus!</lines>
    </verse>
    <verse name="v2">
      <lines>Graças ao fiel Pastor,<br/>que morreu por grande amor<br/>deste pobre pecador!<br/>Graças, graças a Jesus!</lines>
    </verse>
  </lyrics>
</song>
