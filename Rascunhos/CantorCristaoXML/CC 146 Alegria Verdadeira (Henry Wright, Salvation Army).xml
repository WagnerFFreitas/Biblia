<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.0.1" modifiedIn="OpenLP 2.0.1" modifiedDate="2013-11-14T21:42:14">
  <properties>
    <titles>
      <title>CC 146 Alegria Verdadeira</title>
      <title>Come, My Soul</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <authors>
      <author>Henry Wright</author>
      <author>Salvation Army</author>
    </authors>
    <songbooks>
      <songbook name="Cantor Cristão" entry="146"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Sei agora o que me alegra,<br/>sei agora o que me alegra,<br/>sei agora o que me alegra,<br/>confiando no Senhor!</lines>
    </verse>
    <verse name="v2">
      <lines>É Jesus o que me alegra,<br/>é Jesus, o que me alegra,<br/>é Jesus o que me alegra,<br/>confiando em seu amor!</lines>
    </verse>
  </lyrics>
</song>
