      <title>HM046 - Promessa Divina</title>
      <title>Jesus, aos céus subindo,</title>
    </titles>
    <copyright>2001-Imprensa Metodista</copyright>
    <authors>
      <author>Sarah Poulton Kalley</author>
    </authors>
