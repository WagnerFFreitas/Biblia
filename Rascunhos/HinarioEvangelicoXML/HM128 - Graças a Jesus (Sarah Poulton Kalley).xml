<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2018-06-21T18:16:21">
  <properties>
    <titles>
      <title>HM128 - Graças a Jesus</title>
      <title>Graças ao bom Salvador,</title>
    </titles>
    <copyright>2001- Imprensa Metodista</copyright>
    <authors>
      <author>Sarah Poulton Kalley</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Graças ao bom Salvador,<br/>Que me livra do furor<br/>Do cruel destriudor;<br/>Graças, graças a Jesus!</lines>
    </verse>
    <verse name="v2">
      <lines>Graças ao fiel Pastor,<br/>Que na Cruz, por grande amor,<br/>Quis salvar o pecador!<br/>Graças, graças a Jesus!</lines>
    </verse>
  </lyrics>
</song>
