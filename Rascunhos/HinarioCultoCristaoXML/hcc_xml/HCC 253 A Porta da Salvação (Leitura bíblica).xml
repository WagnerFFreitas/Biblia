    <songbooks>
      <songbook name="Hinário Para o Culto Cristão" entry="253"/>
    </songbooks>
  </properties>
  <format>
    <tags application="OpenLP">
      <tag name="y">
        <open>&lt;span style="-webkit-text-fill-color:yellow"&gt;</open>
        <close>&lt;/span&gt;</close>
      </tag>
    </tags>
